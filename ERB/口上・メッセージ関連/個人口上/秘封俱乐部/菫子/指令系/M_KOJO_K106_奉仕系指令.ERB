;==================================================
;80,手淫
;==================================================
@M_KOJO_MESSAGE_COM_K106_80
CALL TRAIN_MESSAGE

CALL M_KOJO_MESSAGE_COM_K106_80_1

RETURN RESULT

@M_KOJO_MESSAGE_COM_K106_80_1

;口上启动开关
LOCAL = 1

IF LOCAL
	
	;小分岐
	
	;初めて
	;記入チェック
	LOCAL:1 = 1
	
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		IF TALENT:恋人
			PRINTFORML 「呜……要用手做么？」
			PRINTFORML 「……知，知道了」
			PRINTFORMW 菫子娇嫩的手指慢慢的握住了%MASTERNAME:106%的肉棒
			PRINTFORML 「呜……热腾腾的呢……%UNICODE(0x2764)%」
			PRINTFORML 「%MASTERNAME:106%的……在一跳一跳的……」
			PRINTFORMW %MASTERNAME:106%一边享受着堇子的侍奉，一边温柔的抚摸着堇子的脑袋
		ENDIF
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMW 
		RETURN 1
	;恋人
	ELSEIF TALENT:恋人
		IF RAND:3 == 0
			PRINTFORML 「是不是再用力一点会更舒服呢？」
			PRINTFORML 「啊，%MASTERNAME:106%……很痛么？」
			PRINTFORMW 堇子努力的用手侍奉着肉棒，取悦着%MASTERNAME:106%
		ELSEIF RAND:2 == 0
			PRINTFORML 「用上超能力会不会更方便呢」
			PRINTFORML 「哎，不要用么？ 呜……好吧」
		ELSE
			PRINTFORML 「不知道为什么，感觉%MASTERNAME:106%的肉棒君很可爱呢……」
			PRINTFORML 「肉棒君，要加油哦……%UNICODE(0x2764)%」
		ENDIF
	;屈服3
	ELSEIF MARK:不埒刻印 == 3
		PRINTFORMW 
		RETURN 1
	;それ以外
	ELSE
		PRINTFORMW 
		RETURN 1
	ENDIF
ENDIF
RETURN 1

