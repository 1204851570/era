@M_KOJO_MESSAGE_COM_K106_0
CALL TRAIN_MESSAGE

CALL M_KOJO_MESSAGE_COM_K106_0_1

RETURN RESULT

@M_KOJO_MESSAGE_COM_K106_0_1

;口上启动开关
LOCAL = 1

IF LOCAL
	
	;小分岐
	
	;初めて
	;記入チェック
	LOCAL:1 = 1
	
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)	
		IF TALENT:恋人
			PRINTFORML 「想摸啊……可以哦」
			PRINTFORMW 把手放在堇子的身上，柔软顺滑的感觉很舒服……
			PRINTFORMW 一边品味着菫子的触感一边开始精心地爱抚
			PRINTFORML 「啊，呜啊……啊」
			PRINTFORML 「更多的……爱抚我吧……%UNICODE(0x2764)%」
		ENDIF
	ENDIF
	
	;挿入中
	;記入チェック
	LOCAL:1 = 1
	
	;V挿入
	IF LOCAL:1 && TEQUIP:50 == PLAYER
		;射精した
		IF NOWEX:PLAYER:11
			PRINTFORMW 
			RETURN 1
		ELSE
			PRINTFORMW 
			RETURN 1
		ENDIF
	;A挿入
	ELSEIF LOCAL:1 && TEQUIP:51 == PLAYER
		;射精した
		IF NOWEX:PLAYER:11
			PRINTFORMW 
			RETURN 1
		ELSE
			PRINTFORMW 
			RETURN 1
		ENDIF
	ENDIF
	
	;Futanari
	;記入チェック
	LOCAL:1 = 1
	
	IF LOCAL:1 && (TALENT:2 & 3) == 3
		PRINTFORMW 
		RETURN 1
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMW 
		RETURN 1
	;恋人
	ELSEIF TALENT:恋人
		IF RAND:3 == 0
			PRINTFORML 「嗯……呃……」
			PRINTFORML 「那么，是在挑逗我吗……？」
		ELSEIF RAND:2 == 0
			PRINTFORML 「再多摸一摸……」
			PRINTFORML 「呜啊%UNICODE(0x2764)%……会变奇怪的……%UNICODE(0x2764)%」
		ELSE
			PRINTFORML 「手法很温柔呢」
			PRINTFORML 「激烈的欺负我也没问题的……%UNICODE(0x2764)%」
		ENDIF
	;屈服3
	ELSEIF MARK:不埒刻印 == 3
		PRINTFORMW 
		RETURN 1
	;それ以外
	ELSE
		PRINTFORMW 
		RETURN 1
	ENDIF
ENDIF
RETURN 1
