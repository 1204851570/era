	ENDIF
	;二回目以降
	IF !(CFLAG:K17C_CF_喘ぎタイプ & K17C_喘ぎF_襲い)
		;ねだり系
		;汎用　性技判定
		CALL K17_MESSAGE
		CALL K17_CLL_COUNTER_REACTIONMESSAGE
		
		;パイズリフェラの時喘ぎ箇所を変える
		IF TCVAR:counter行動的派生 == 4
			CALL K17_SET_AUTO_PVOICE(-1,K17C_部位M_口交)
		ELSE
			SELECTCASE ABL:TARGET:胸
				CASE IS < 2
					;PRINTL あんまり気持ちよくない…
					PRINTFORML 「%K17_SPLIT_SHUFFLE(@"这样%K17_RS("很困难/很不熟练")%…\\%K17_AP_NUM(2)%\\%K17_AP_NUM(1)%","\\")%%K17_AP_NUM(1,1)%」
				CASE IS  > 2
					;PRINTFORML %CALLNAME:TARGET%はこなれた奉仕で%CALLNAME:PLAYER%を悦ばせた…
					PRINTFORML 「%K17_SPLIT_SHUFFLE(@"这样%K17_RS("舒服了/变得熟练了")%吗？　…\\%K17_AP_NUM(2)%\\%K17_AP_NUM(1)%","\\")%%K17_AP_NUM(1,1)%」
				CASEELSE
					;PRINTFORML %CALLNAME:TARGET%の奉仕は%CALLNAME:PLAYER%を気持よくさせている…
					PRINTFORML 「%K17_SPLIT_SHUFFLE(@"这样%K17_RS("已经习惯/越来越熟练")%了…\\%K17_AP_NUM(2)%\\%K17_AP_NUM(1)%","\\")%%K17_AP_NUM(1,1)%」
			ENDSELECT
		ENDIF
	ELSE
		;襲い系
		PRINTFORMDL %CALLNAME:MASTER%在%CALLNAME:TARGET%胸口的山谷间摩擦着%TINKO(PLAYER)%摇了摇腰部
		;カウンター追加
		IF TCVAR:counter行動的派生
			PRINTFORMDL 顺便张开%CALLNAME:TARGET%的嘴插了进去
			CALL K17_SET_AUTO_PVOICE(-1,K17C_性技_舌)
			TCVAR:counter行動的派生 = 0
		ENDIF
	ENDIF
	
	PRINTFORML 「%K17_AUTO_PVOICE()%」
	PRINTW
	;汎用　感覚判定
	PRINTFORMDL %CALLNAME:MASTER%的%TINKO(PLAYER)%在%OPPAI_DESCRIPTION(TARGET)%里%K17_RS("被刺激/摩擦着")%，服务的%CALLNAME:TARGET%看起来也很舒服
	CALL K17_SET_AUTO_PVOICE(K17_GRT_EX(),K17C_部位B)
	PRINTFORML 「%K17_AUTO_PVOICE()%」
	
ENDIF
RETURN 1

