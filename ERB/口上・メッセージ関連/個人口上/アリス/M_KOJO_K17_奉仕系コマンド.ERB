			;襲い系
			PRINTFORMDL 让%CALLNAME:TARGET%握住%TINKO(PLAYER)%
			IF TCVAR:counter行動的派生
				PRINTFORMDL %CALLNAME:MASTER%把%TINKO(PLAYER)%压到%CALLNAME:TARGET%的阴道孔的同时，摇动腰润滑着%TINKO(PLAYER)%……
				TCVAR:counter行動的派生 = 0
			ENDIF
		ELSE
			;合意系
			;汎用　性技判定
			CALL K17_MESSAGE
			CALL K17_CLL_COUNTER_REACTIONMESSAGE
		ENDIF
	ENDIF
	PRINTFORML 「%K17_AUTO_PVOICE()%」
ENDIF
RETURN 1

