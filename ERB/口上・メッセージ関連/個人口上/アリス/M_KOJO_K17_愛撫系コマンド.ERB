@M_KOJO_MESSAGE_COM_K17_9_1
#DIM iType = 0
#DIMS cText
VARSET cText
;-------------------------------------------------
;感覚部位を選定
;-------------------------------------------------
IF TEQUIP:振動棒
	CALL K17_SET_AUTO_PVOICE(K17_GRT_EX(),K17C_部位V)
ELSEIF TEQUIP:肛用振動棒
	CALL K17_SET_AUTO_PVOICE(K17_GRT_EX(),K17C_部位A)
ELSEIF TEQUIP:淋浴
	CALL K17_SET_AUTO_PVOICE(K17_GRT_EX(),K17C_部位C)
ELSE
	CALL K17_SET_AUTO_PVOICE(K17_GRT_EX(),RAND:2?K17C_部位C#K17C_部位B)
ENDIF

;-------------------------------------------------
;9,自慰
;性癖あり
;-------------------------------------------------
IF K17_RT_FG() & K17C_RTF_性癖 && !(CFLAG:K17C_CF_喘ぎタイプ & K17C_喘ぎF_襲い) && GET_TARGETNUM() == 1
	;性癖あり　ねだり系　二人きり
	;妄想発表
	;一旦タイプ切り替え
	iType = CFLAG:K17C_CF_喘ぎタイプ
	cText = %K17_H妄想()%
	CALL K17_SET_AUTO_PVOICE(,,K17_喘ぎタイプ_H妄想(RESULT:0,RESULT:1))
	
	PRINTFORMDL %CALLNAME:MASTER%对%CALLNAME:TARGET%说，希望她能像平常一样做%K17_RS("自慰/自慰/秘密の研究")%
	
	IF FIRSTTIME(@"{SELECTCOM}口上")
		;初回
		PRINTFORML 「啊！？呜……这样抓住别人的把柄……」
		PRINTFORMDL 虽然有些害羞，但%CALLNAME:TARGET%还是好好回应了
		PRINTW
		PRINTFORML 「听我说、那个……这么做倒无所谓、但……一个人……H、这样子、呐……会变得非常寂寞……」
		PRINTFORML 「所以在这之后，如果不好好疼爱我的话，可不行啊？」
		PRINTFORMDL %CALLNAME:MASTER%用温柔的手抚摸着%CALLNAME:TARGET%的头的同时，让她坐在膝盖上
	ELSE
		;二回目以降
		PRINTFORML 「又……想要做吗？	真是……%K17_RS("H/変態/色鬼")%」
		PRINTFORMDL %K17_RS("害羞地笑着/虽然嘴里这么说")%，%CALLNAME:TARGET%好好回应了
		PRINTFORML 「在这之后，如果不好好地从%K17_CNAME()%那里得到%K17_RS("疼爱/舒服")%的话，%K17_RS("不行/不会原谅你/不可以")%呐%K17_HEART()%」
		PRINTFORMDL %CALLNAME:MASTER%把%CALLNAME:TARGET%%K17_RS("的头温柔抚摸着/温柔地抱着")%，让她%K17_RS("坐上/坐在")%膝盖
	ENDIF
	
	PRINTW
	CALL K17_MESSAGE
	CALL K17_CLL_COUNTER_REACTIONMESSAGE
	PRINTFORML 「%K17_AUTO_PVOICE()%」
	PRINTFORMDL 「热心研究」的%CALLNAME:TARGET%，闭着眼睛沉浸在自慰的妄想中……
	PRINTW
	PRINTFORMDL %CALLNAME:MASTER%在耳边低声细语着问%CALLNAME:TARGET%到底是怎样的妄想
	PRINTFORML 「%cText%在%K17_RS("妄想/想像")%里、%K17_RS("这么/那样的事")%做了た%K17_BLUSH(1)%」
	PRINTW
	PRINTFORMDL %CALLNAME:TARGET%虽然脸红着，但还是有些兴奋地这样告白了
	PRINTFORMDL 似乎暴露妄想完全成了一种游戏……
	CFLAG:K17C_CF_喘ぎタイプ = iType
	RETURN 1
	
;-------------------------------------------------
;9,自慰
;汎用
;-------------------------------------------------
ELSE
	;性癖なしor襲い系
	CALL K17_MESSAGE
	CALL K17_CLL_COUNTER_REACTIONMESSAGE
	PRINTFORML 「%K17_AUTO_PVOICE()%」
	IF !(CFLAG:K17C_CF_喘ぎタイプ & K17C_喘ぎF_襲い)
		;ねだり系
		PRINTFORMDL 贪图快乐的%CALLNAME:TARGET%似乎已经很习惯了这种手法……
	ELSE
		;襲い系
		PRINTFORMDL 虽然是很讨厌，但还是遵从着吩咐，手势却感觉很熟练……
	ENDIF
	RETURN 1
ENDIF



