;==================================================
;107,拘束プレイ
;==================================================
@M_KOJO_MESSAGE_COM_K17_107
IF FLAG:時間停止 || CFLAG:TARGET:睡眠
	CALL TRAIN_MESSAGE
	CALL M_KOJO_MESSAGE_COM_K17_睡眠
ELSE
	CALL TRAIN_MESSAGE
	CALL M_KOJO_MESSAGE_COM_K17_107_1
	PRINTW
ENDIF
RETURN 1

@M_KOJO_MESSAGE_COM_K17_107_1
#DIM iType = 0
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	
	;-------------------------------------------------
	;タイプ保存
	;-------------------------------------------------
	IF TEQUIP:拘束
		;拘束のみが入った時　つまり装着した時タイプ一時保存
		iType = CFLAG:K17C_ID:K17C_CF_喘ぎタイプ
		CFLAG:K17C_CF_喘ぎタイプ = K17C_受け_强行系
	ELSE
		;拘束が入ってないでここ来た場合は外した時
		;縄ありなら先に解除しないとこっちもできないみたい
		CFLAG:K17C_ID:K17C_CF_喘ぎタイプ = iType
	ENDIF
	;-------------------------------------------------
	;泥酔先処理
	;-------------------------------------------------
	IF TCVAR:烂醉
		IF TEQUIP:拘束
			PRINTFORML 「被抓到了~啊哈哈」
		ELSE
			PRINTFORML 「嗯……卸下了啊……」
		ENDIF
	ENDIF
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初回　装着時
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(@"{SELECTCOM}口上") && TEQUIP:拘束
		PRINTFORML 「什么……拘束……总觉得这样……虽然真的动不了……」
		PRINTFORMDL %CALLNAME:TARGET%没有抵抗就被%CALLNAME:MASTER%老实地束缚住了
		PRINTFORML 「居然想做这种事……变态！」
		PRINTW
		PRINTFORMDL 果然还是不喜欢吧……
		PRINTFORML 「呃……随你便吧，够了」
		PRINTFORML （即使不愿意也要强行……被骗了……）
	ELSEIF TEQUIP:拘束
		;無理矢理中出ししたことあるか見る
		IF CFLAG:强行内射
			PRINTFORML 「已经……没办法啊……%K17_RS("変態/下流/H/笨蛋")%！
		ELSE
			PRINTFORML 「又是……拘束……就算说不愿意你也会做的吧，真是%K17_RS("変態/下流/H/笨蛋")%！」
		ENDIF
		PRINTFORML （%K17_RS("对于不能动的对手，想随心所欲/拘束起来，像人偶一样对待")%……什么的……）
	ELSE
		;外した時
		PRINTFORML 「呼……%K17_RS("我累了/开放了的感觉/变得可怕了？")%」
	ENDIF
ENDIF
RETURN 1

