				cPANTS += "突起"
			ELSE
				cPANTS += "割れ目"
			ENDIF
		ELSE
			cPANTS += SHOW_下半身内衣(TARGET,3)
			cALICE += "あそこ"
		ENDIF
	;パンツ
	ELSEIF !CFLAG:没穿内裤
		IF GETBIT(TALENT:TARGET:性別,1)
			IF BASE:勃起 >= 1000
				cPANTS += "ぷっくりと目立つ突起"
			ELSE
				cPANTS += "目立つ突起"
			ENDIF
