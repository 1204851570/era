	SIF FIRSTTIME(@"{SELECTCOM}口上",1) && (CFLAG:好感度 > 5000 && iRT > K17C_思慕接吻 && BASE:情緒 > 500)
		RESULT = 1
	;パンツ見せ既読＆ムード不足
	SIF !FIRSTTIME(@"{SELECTCOM}口上",1) && BASE:情緒 > 500
		RESULT = 1

