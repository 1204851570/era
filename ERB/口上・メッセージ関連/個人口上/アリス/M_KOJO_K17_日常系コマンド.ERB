	;失敗
	;--------------------
	IF CFLAG:TARGET:ブチギレ == 1 || CFLAG:好感度 < 100 || TALENT:心情 == -1 || (iAnger && !RESULT)
		;拡張条件満たしてない時の失敗
		PRINTFORM 「%K17_RS("咕/呃/那个")%！！　%K17C_人形名:iDoll%\@iDoll ? #们\@！！」
		TRYCALL K17_人形台詞(iDoll,1,K17_RS("チカン/オトメノ　テキ/ヘンタイ"))
