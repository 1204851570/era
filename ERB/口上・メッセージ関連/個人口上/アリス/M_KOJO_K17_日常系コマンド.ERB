		IF iOpen
			cPANTS += PANTSNAME(EQUIP:TARGET:下半身内衣２, TARGET)
		ELSE
			cPANTS += PANTS_DESCRIPTION(EQUIP:TARGET:下半身内衣２, TARGET)+K17_GET_CLOTHNAME_BOTTOM_UNDER(TARGET)
		ENDIF
