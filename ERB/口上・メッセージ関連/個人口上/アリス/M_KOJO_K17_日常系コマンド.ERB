@M_KOJO_MESSAGE_COM_K17_351_1
#DIM iFriends = 0
#DIM iMOB = 0
#DIMS cMove
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL && !FLAG:時間停止
	PRINTL
	;泥酔
	IF TCVAR:(TARGET:LOCAL):烂醉
		PRINTFORML 「哼~……你要带我去哪里……」
		PRINTW
		RETURN 1
	ENDIF
	
	;友達優先で同行している人を探す
	iFriends = 0
	iMOB = 0
	;MASTERに同行がついてる場合　誰かを探す
	IF CFLAG:MASTER:同行
		;友達を見る
		FOR i,0,K17C_友人人数
			IF CFLAG:(K17C_友人ID:i):同行
				iFriends = K17C_友人ID:i
				BREAK
			ENDIF
		NEXT
		;普通に見る
		IF !iFriends
			FOR i,1,CHARANUM
				IF (CFLAG:(TARGET:i):同行) && TARGET:i != K17C_ID
					iMOB = TARGET:i
					BREAK
				ENDIF
			NEXT
		ENDIF
	ENDIF
	
	;二人の移動方式を選択
	VARSET cMove
	IF ( !FIRSTTIME(@"311约会移動",1) && K17_LUCKY_SUKEBE(30) ) && (!iFriends && !iMOB)
		;腕抱き経験済み
		cMove = %CALLNAME:TARGET%は自然と%CALLNAME:MASTER%の腕に抱き付いて、%K17_RS("胸部/胸")%を%K17_RS("押し付け/押し当て")%ながらついてきた
	ELSEIF ( !FIRSTTIME(603,1) || !FIRSTTIME(@"身体接觸0",1) ) && (!iFriends && !iMOB)
		;手を繋ぐ経験済み
		IF K17_LUCKY_SUKEBE(40)
			cMove = %CALLNAME:TARGET%は少し%K17_RS("红着脸/照れたように")%、%CALLNAME:MASTER%の手を握ってついてきた
		ELSE
			cMove = %CALLNAME:MASTER%が%CALLNAME:TARGET%の手を握ると、少し%K17_RS("红着脸/照れたように")%握り返しついてきた
		ENDIF
	ELSEIF iFriends || iMOB
		;他に同行がいる
		cMove = %CALLNAME:TARGET%は%CALLNAME:(iFriends!=0 ? iFriends#iMOB)%に轻轻地%K17_RS("会釈/挨拶")%して%CALLNAME:MASTER%についてきた
	ELSE
		;まだやってない
		cMove = %CALLNAME:MASTER%は%CALLNAME:TARGET%を連れ出した
	ENDIF
	
	;反応と描写
	SELECTCASE K17_GRT_EX(TARGET)
		CASE IS >= K17C_恋慕
			PRINTFORML 「那么，%K17_RS("要带去/要带到/要去")%哪里？」
			PRINTFORMDL %cMove%
		CASE IS >= K17C_思慕逢引
			PRINTFORML 「那么，%K17_RS("要带去/要带到/要去")%哪里？」
			PRINTFORMDL %cMove%
			
		CASE IS >= K17C_思慕
			PRINTFORML 「那么，%K17_RS("要带去/要去")%哪里？」
			PRINTFORMDL %cMove%
		CASEELSE
			PRINTFORML 「……嗯……不要去奇怪的地方哦？」
			PRINTFORMDL %CALLNAME:TARGET%似乎有些戒备
	ENDSELECT
	
	;他同行者への反応
	IF iFriends
		PRINTFORML （……%K17_CNAME(iFriends)%也一起，%K17_RS("啊/呢")%……%K17_RS("是怎么了？/打算干什么？")%）
	ELSEIF iMOB
		PRINTFORML （……其他孩子也一起……%K17_RS("啊/是为什么？/呢")%）
	ELSE
		;同行者が他にいない時はうふふ合意基準
		SELECTCASE K17_GRT()
			CASE IS >= K17C_思慕诶嘿嘿
				PRINTFORML （……去%K17_RS("安静的地方/两人独处的地方/没什么人气的地方")%吗……？）
			CASE IS >= K17C_思慕逢引
				PRINTFORML （……%K17_RS("要被带去/要被带到")%奇怪的地方、不会吧……？）
		ENDSELECT
		;同行者がアリスだけで他の人がその場にいる場合
		SIF GET_TARGETNUM() > 1
			PRINTFORMDL 只有自己被带走，%CALLNAME:TARGET%显得有些害羞
	ENDIF
	
ENDIF
PRINTW
RETURN 1
