;302-1-選択肢	スキンシップ選択肢
;影狼口上の真似に見えますがパクリです、ごめんなさい
;あっれー思ったよりでっかくなっちゃった、イチャコラしすぎだろ
;====================================================================================================
@M_KOJO_MESSAGE_COM_K17_302_1_選択肢
#DIM iRT = 0		;関係性　GRT_EXで取る
#DIM BathFlag = 0	;お風呂フラグ
#DIM iHoldHands = 0	;手を握る選択肢フラグ ほーるどはんど？知るか日本語で書け
#DIM iExFlag,5		;拡張選択肢フラグ
#DIMS cDoll			;人形の表示テキスト
#DIMS cEx,5			;拡張選択肢文字列
#DIM iRand=0		;ランダム値の入れ物

;各種初期化
iRT=K17_GRT_EX()
VARSET BathFlag
VARSET iHoldHands
VARSET iExFlag
VARSET cDoll
VARSET cEx
VARSET iRand

;お風呂チェック
SIF BATHCHECK(CFLAG:MASTER:現在位置) && IN_HOME(CFLAG:MASTER:現在位置)
	BathFlag = 1
;成功or宴会時　普段より大胆に
SIF TFLAG:193 == 1 || CFLAG:TARGET:職種 == 49
	iRT++


PRINTL
PRINTFORMDL 那么，怎么办呢……
PRINTL

;お風呂時
IF BathFlag
	TRYCALL M_KOJO_MESSAGE_COM_K17_302_1_選択肢_お浴室(iRT)
	RETURN 1
ENDIF


;----選択肢場所とかの判定　長くなりそうなので変数に入れる
;甲斐性無しだと全部だめ
IF !FLAG:甲斐性無
	;耳、首は特になし
	iExFlag:0=1
	iExFlag:1=1
	;太もも　デート中歩きながら手を握る(コマンド出てるはずなので)、太もも触るのはやめよう
	IF !(CHK_DATENOW(CFLAG:TARGET:约会中) && CFLAG:MASTER:現在位置 != OMANEKIBEYA() && (CFLAG:TARGET:現在位置 % 100) % 10)
		iExFlag:2 = 1
		iHoldHands = 1
	ENDIF
	
	;内もも　部屋で二人きり
	SIF INROOM(CFLAG:現在位置) && GET_TARGETNUM()==1
		iExFlag:3 = 1
ENDIF

;人形は特になし　テキストの変更はあり
iExFlag:4 = 1

;----選択肢表示判定　関係性判定
SIF iRT >= K17C_思慕
	cEx:0 = 耳に息を吹きかける
	
SIF iRT >= K17C_思慕接吻
	cEx:1 = 脖子に接吻
	
SIF iRT >= K17C_思慕诶嘿嘿
	cEx:2 = 太ももすりすり
	
SIF iRT >= K17C_恋慕
	cEx:3 = 从后面将内ももに触れる

;人形表現処理　上海優先で
cDoll = %K17_GET_DOLL_CNAME(0)%
cEX:4 = 裏をかいて%cDoll%に身体接觸


;通常スキンシップ処理
;関係性で条件を満たしていればテキスト表示
;場所的に無理なら選択できない表示
IF !BathFlag && TCVAR:K17C_ID:K17C_TC_身体接觸 == 0
{
	CALL K17_ASK_M(
		"手を握る",iHoldHands,
		"身体をつんつん",1,
		"髪を撫でる",1,
		"揉肩 ",1,
		cEx:0,iExFlag:0,
		cEX:1,iExFlag:1,
		cEX:2,iExFlag:2,
		cEX:3,iExFlag:3,
		cEX:4,iExFlag:4,
		"随机",1,
		"今日一日随机",1)
}
ENDIF

;ランダム時
IF !BathFlag && (RESULT == 9 || RESULT == 10 || TCVAR:K17C_ID:K17C_TC_身体接觸)
	SIF RESULT == 10
		TCVAR:K17C_ID:K17C_TC_身体接觸 = 1
	;選択雑ぅ（恐れ
	WHILE 1
		RESULT=RAND:9
		SELECTCASE RESULT
			CASE 0
				SIF iHoldHands
					BREAK
			CASE 1 TO 3
				BREAK
			CASE 4 
				SIF iRT >= K17C_思慕 && iExFlag:0
					BREAK
			CASE 5
				SIF iRT >= K17C_思慕接吻 && iExFlag:1
					BREAK
			CASE 6
				SIF iRT >= K17C_思慕诶嘿嘿 && iExFlag:2
					BREAK
			CASE 7
				SIF iRT >= K17C_思慕诶嘿嘿 && iExFlag:3
					BREAK
			CASE 8
				SIF iExFlag:4
					BREAK
		ENDSELECT
	WEND

ENDIF
SIF TCVAR:K17C_ID:K17C_TC_身体接觸 != 1
	PRINTL

