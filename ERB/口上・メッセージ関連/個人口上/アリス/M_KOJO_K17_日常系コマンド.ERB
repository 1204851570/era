;====================================================================================================
@M_KOJO_MESSAGE_COM_K17_301
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K17_301_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K17_301_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL

	;怒り状態
	IF CFLAG:TARGET:ブチギレ == 1
		TRYCALL M_KOJO_MESSAGE_COM_K17_憤怒
		RETURN 1
	ENDIF
	
	;-------------------------------------------------
	;初めて
	;選んだ瞬間にFIRSTTIME記録されるので怒り状態で初回されるとスキップされる
	;自前で用意してもいいけど、仕様ってことでひとつ……
	;必要な場合口上つけて保存してみるテスト
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(@"{SELECTCOM}口上")
		PRINTFORML 「哎呀，你泡茶了啊」
		PRINTFORML 「对不起，让你费心了。应该由我来泡吧」
		PRINTFORMDL %CALLNAME:MASTER%告诉%CALLNAME:TARGET%，能为%CALLNAME:TARGET%做的只有这些，所以希望尽可能的去做
		PRINTFORML 「嗯。那么下次就拜托你了吧」
		PRINTFORML 「可是我会稍微严格一点哟？」
		PRINTFORMDL 今后每次都加油吧
		PRINTW
	ENDIF
	
	;仕事中
	IF CFLAG:TARGET:行動 == 5
		PRINTDATAL
			DATAFORM 「ちょうど一息入れたいところだったわ」
			DATAFORM 「いいタイミングね」
			DATAFORM 「あら嬉しい」
		ENDDATA
	ENDIF
	
	;成功判定
	IF TFLAG:193 == 1
		PRINTFORML 「今天的茶特别%K17_RS("好/美味/香")%啊～、付出锻炼有所价值了」
		PRINTFORMDL 看来拿到了及格分
		
	;失敗
	ELSEIF TFLAG:193 == -1
		SELECTCASE K17_GRT(TARGET)
			CASE IS >= K17C_恋慕
				PRINTFORML 「…………嗯……」
				PRINTFORMDL %CALLNAME:TARGET%一瞬间里皱起了眉头，但什么也没说就喝完了
			CASE IS >= K17C_思慕
				PRINTFORML 「……嗯？稍稍失败了点吧」
			CASE K17C_知り合い
				PRINTFORML 「……放了什么……这个」
				PRINTFORMDL 怀疑有什么混入了茶里
		ENDSELECT
		PRINTFORMDL 看来是失败了。下次加油吧
	ENDIF
	
	;感想
	SELECTCASE K17_GRT_EX(TARGET)
		CASE IS >= K17C_恋慕
