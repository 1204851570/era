	;各種初期化
	;-----------------
	iRT = K17_GRT_EX()
	iKiss = 0
	iRand = 0
	
	;-----------------
	;押し倒され中　タイプ一時変更
	;-----------------
	IF CFLAG:诶嘿嘿 == 2
		iType = CFLAG:K17C_ID:K17C_CF_喘ぎタイプ
		CALL K17_SET_AUTO_PVOICE(iRT, K17C_部位M_接吻,K17C_受け_おねだり系)
		PRINTFORML 「%K17_AUTO_PVOICE()%」
		CFLAG:K17C_ID:K17C_CF_喘ぎタイプ = iType
		RETURN 1
	ENDIF
	
	;-----------------
	;通常キス
	;-----------------
	;この後怒り
	IF K17_CHECK_ANGER_COM(TARGET)
		PRINTFORML 「嗯……%K17_RS("不要/不行/不可以")%啊」
		PRINTFORMDL %CALLNAME:TARGET%用双手将%CALLNAME:MASTER%推开
		RETURN 1
	ENDIF
	
	;驚きパターン
	IF ABL:欲望 < 5 && ABL:親密 < 5
		CALL K17_SET_AUTO_PVOICE(iRT, K17C_部位M_接吻,K17C_受け_强行系)
		PRINTFORML 「%K17_AP_NUM(1,1)%」
		PRINTFORML 「嗯……这样……的…不行啊……%K17_BLUSH()%」
		PRINTFORMDL %CALLNAME:TARGET%转过脸把%CALLNAME:MASTER%推回去
		RETURN 1
	ENDIF
	
	
	
	;ちゅっちゅするほう
	IF (PREVCOM == 305 || CFLAG:TARGET:陪睡中) && RESULT:1 <= 300
		iRand = K17_MAX_LIMIT_RAND(iRT,K17C_恋慕告白,K17C_恋人)
		SELECTCASE iRand
			CASE IS >= K17C_恋人
				PRINTFORML 「嗯～～%K17_RS("啾/啾嗯/嗯啾")%%K17_HEART()%」
				PRINTFORML 「%K17_RS("欸嘿嘿/欸嘿/呜")%……喂，再多做点吧？好吗？%K17_HEART()%」
				PRINTFORML 「%K17_RS("啾/啾嗯/嗯啾")%%K17_HEART()% %K17_RS("啾/啾嗯/嗯啾")%%K17_HEART()%」
			CASE IS >= K17C_恋慕告白
				PRINTFORML 「%K17_RS("嗯啾/唔呣/嗯啾")%%K17_HEART()%　……%K17_CNAME()%的嘴唇，为什么会那么%K17_RS("舒服/柔软/棒")%呢……」
				PRINTFORML 「无论几次都不会厌倦……%K17_RS("嗯啾/唔呣/嗯啾")%%K17_HEART()%」
			CASE IS >= K17C_思慕
				PRINTFORML 「嗯～～%K17_RS("嗯啾/唔呣/嗯啾")%」
				PRINTFORML 「%K17_RS("欸嘿嘿/欸嘿/唔呼")%……%K17_BLUSH()%」
			CASEELSE
				PRINTFORML 「%K17_チュパ音(K17C_受け_强行系,iKiss,2,1)%」
				PRINTFORMDL %CALLNAME:TARGET%有点害羞地脸红了，错开%CALLNAME:MASTER%的视线
				RETURN 1
		ENDSELECT
		PRINTFORMDL %CALLNAME:TARGET%很高兴的样子
		RETURN 1
	ENDIF
	
	;ディープ判定
	IF ABL:欲望 >= 5
		iKiss = K17_ABL_RT_EX(iRT,K17C_部位M_接吻)
	ELSE
		iKiss = -1
	ENDIF
	
	;関係査定EX
	;ぶちゅぶちゅするやつ
	;K17_チュパ音(タイプ,感覚値,回数,終端
	;反感固定ソース　300
	SELECTCASE BASE:情緒
		CASE IS >= 650
			SELECTCASE iRT
				CASE IS >= K17C_恋人
					PRINTFORML 「%K17_チュパ音(K17C_受け_おねだり系,iKiss,2,1)%啊%K17_HEART()%　更加%K17_チュパ音(K17C_受け_おねだり系,iKiss,3,1)%
					PRINTFORMDL %CALLNAME:TARGET%兴奋地脸颊泛红，不停地索求着
				CASE IS >= K17C_恋慕告白
					PRINTFORML 「%K17_チュパ音(K17C_受け_おねだり系,iKiss,2)%更多……しよっ―%K17_チュパ音(K17C_受け_おねだり系,iKiss,2,1)%」
					PRINTFORMDL %CALLNAME:TARGET%催促着继续
				CASE IS >= K17C_思慕接吻
					PRINTFORML 「%K17_チュパ音(K17C_受け_おねだり系,iKiss,2,1)%」
					PRINTFORMDL %CALLNAME:TARGET%的眼睛融化般地注视着
				CASEELSE
					PRINTFORML 「%K17_チュパ音(K17C_受け_强行系,iKiss,2,1)%」
					PRINTFORMDL %CALLNAME:TARGET%有点害羞地脸红了，错开%CALLNAME:MASTER%的视线
			ENDSELECT
		CASE IS >= 450
			SELECTCASE iRT
				CASE IS >= K17C_恋人
					PRINTFORML 「%K17_チュパ音(K17C_受け_おねだり系,iKiss,2)%更加……啊？　%K17_チュパ音(K17C_受け_おねだり系,iKiss,2,1)%」
					PRINTFORMDL %CALLNAME:TARGET%催促着继续
				CASE IS >= K17C_恋慕告白
					PRINTFORML 「%K17_チュパ音(K17C_受け_おねだり系,iKiss,2,1)%」
					PRINTFORMDL %CALLNAME:TARGET%的眼睛融化般地注视着
				CASE IS >= K17C_思慕接吻
					PRINTFORML 「%K17_チュパ音(K17C_受け_强行系,iKiss,2,1)%」
					PRINTFORMDL %CALLNAME:TARGET%有点害羞地脸红了，错开%CALLNAME:MASTER%的视线
				CASEELSE
					PRINTFORML 「%K17_チュパ音(K17C_受け_强行系,iKiss,1,1)%」
					PRINTFORMDL %CALLNAME:TARGET%有点害羞地脸红了，错开%CALLNAME:MASTER%的视线
			ENDSELECT
			
		;ムード足りない
		CASEELSE
				PRINTFORML 「%K17_チュパ音(K17C_受け_强行系,-1,1,1)%」
				PRINTFORML 「啊，这么突然%K17_RS("不行哦/不要啊/不行哟")%……%K17_BLUSH()%」
				PRINTFORMDL %CALLNAME:TARGET%稍微转过脸去
				RETURN 1
	ENDSELECT
	
	;屋内で二人きり追加　実値逢引以上 押し倒され中以外
	IF INROOM(CFLAG:現在位置) && GET_TARGETNUM()==1 && K17_GRT() >= K17C_思慕逢引 && CFLAG:诶嘿嘿 != 2
		;グローバル保存
		CALL K17_SET_AUTO_PVOICE(iRT, K17C_部位M_接吻,K17C_受け_おねだり系)
		PRINTW
		PRINTFORML 「……嗯……哈……%K17_AP_DEC()%还不……%K17_RS("做/要做/要/し想要")%吗？」
		PRINTFORML 「%K17_AP_NUM(1)%%K17_RS("等/等等/呀")%……%K17_AP_NUM(3)%喜欢%K17_HEART()%」
		PRINTFORMDL %CALLNAME:MASTER%把%CALLNAME:TARGET%压倒，不断地接吻
		PRINTFORML 「%K17_AUTO_PVOICE()%」
		IF RAND:2
			PRINTFORMDL 只有二人的%K17_RS("接吻声/吐息")%在房间中%K17_RS("回响/回荡")%
		ELSE
			PRINTFORMDL 两人的嘴唇一分开，就在之间连起了%K17_RS("唾液/銀色")%的桥
		ENDIF
	ENDIF
	
ENDIF
