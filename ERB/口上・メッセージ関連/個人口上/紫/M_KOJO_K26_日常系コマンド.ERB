@M_KOJO_MESSAGE_COM_K26_300_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;怒り
	IF CFLAG:318 == 1
		PRINTFORMW “你居然敢直截了当地这么说呢。”
		PRINTFORMW 紫用愤怒的声音回答了。
	;会話不能
	ELSEIF TCVAR:302
		PRINTFORMW “……”
		PRINTFORMW 紫沉默了。
	ELSEIF TFLAG:193 == -2
		IF RAND:5 == 0
			PRINTFORMW “能不能多点教养呢？”
		ELSEIF RAND:4 == 0
			PRINTFORMW “和妖精说话可能更开心。”
		ELSEIF RAND:3 == 0
			PRINTFORMW “真无聊，不不，太无聊了。”
		ELSEIF RAND:2 == 0
			PRINTFORMW “要我教教你该怎么学习吗？”
		ELSE
			PRINTFORMW “不好意思，请您变得更聪明一点。”
		ENDIF
	;ミニマップ時のみ
	;条件をメインマップ時にできなくもないけど面倒
	;現在位置が墓
	ELSEIF CFLAG:MASTER:現在位置 == 150
		IF RAND:5 == 0
			PRINTFORMW “扫墓什么的，一个人就够了”
		ELSEIF RAND:4 == 0
			PRINTFORMW “这里也能连接冥界吗？”
		ELSEIF RAND:3 == 0
			PRINTFORMW “能看到什么吗？虽然我能看到很多东西”
		ELSEIF RAND:2 == 0
			PRINTFORMW “葬式不要戒名不要，那么可能会被这里的僧侣讨厌吧。”
		ELSE
			PRINTFORMW “没有在这里久住的兴趣。”
		ENDIF
	;現在位置が竹林入り口
	ELSEIF CFLAG:MASTER:現在位置 == 410
		IF RAND:5 == 0
			PRINTFORMW “这附近有恐怖的怪物”
			PRINTFORMW “你问我的事情吗？算了，还是不知道为好”
		ELSEIF RAND:4 == 0
			PRINTFORMW “真实存在于主观之中，那么我是否是真实的呢？”
		ELSEIF RAND:3 == 0
			PRINTFORMW “也许是看到了在梦境的延续”
		ELSEIF RAND:2 == 0
			PRINTFORMW “红魔馆的曲奇虽然很好吃，但这里的竹笋也很美味，如果不会迷路的话就好了。”
		ELSE
			PRINTFORMW “雨后春笋的味道太淡了。”
		ENDIF
	;現在位置が彼岸
	ELSEIF CFLAG:MASTER:現在位置 == 670
		IF RAND:5 == 0
			PRINTFORMW “彼岸花……从以前开始就很讨厌”
			PRINTFORMW “是啊，从很久以前开始。”
		ELSEIF RAND:4 == 0
			PRINTFORMW “被阎王大人发现太麻烦了，我讨厌那个人。”
		ELSEIF RAND:3 == 0
			PRINTFORMW “最近有没有偷懒啊？”
		ELSEIF RAND:2 == 0
			PRINTFORMW “与现世的境界，我能用吗？”
		ELSE
			PRINTFORMW “简直像莲台野一样”
		ENDIF
	ELSEIF TALENT:恋人
		IF RAND:5 == 0
			PRINTFORMW “恩，和%CALLNAME:MASTER%在一起就已经十分快乐了……”
			PRINTFORMW “真想去远方看看啊”
		ELSEIF RAND:4 == 0
			PRINTFORMW “对我来说只有一两件内衣”
			PRINTFORMW “%CALLNAME:MASTER%喜欢什么样的？”
		ELSEIF RAND:3 == 0
			PRINTFORMW “嘿嘿，呐，让我听听%CALLNAME:MASTER%的故事。”
		ELSEIF RAND:2 == 0
			PRINTFORMW “%CALLNAME:MASTER%不要死啊”
		ELSE
			PRINTFORMW “能告诉我%CALLNAME:MASTER%喜欢的场景吗？”
			PRINTFORMW “我会尽量帮你实现的”
		ENDIF
	ELSEIF TALENT:恋慕
		IF RAND:5 == 0
			PRINTFORMW “如果说‘时间就是金钱’的话，那么操纵时间的人就是暴发户了。”
		ELSEIF RAND:4 == 0
			PRINTFORMW “所谓结界的概念是内和外，跨越边界的是？进入的时候我们就是存在的”
		ELSEIF RAND:3 == 0
			PRINTFORMW “感觉迟钝就麻烦了，顿感那样的。”
		ELSEIF RAND:2 == 0
			PRINTFORMW “%CALLNAME:MASTER%会做怎样的梦呢？”
		ELSE
			PRINTFORMW “即使在庭院里，尽力讴歌生命，月亮上的人也可能并不知道吧。”
		ENDIF
	ELSEIF TALENT:思慕
		IF RAND:5 == 0
			PRINTFORMW “恩，我没发现什么有意思的事情呢。”
		ELSEIF RAND:4 == 0
			PRINTFORMW “我也有工作，但是大家都嫌烦，本不应该是这样的啊”
		ELSEIF RAND:3 == 0
			PRINTFORMW “接受有需要的东西，排除不需要的东西，无论外部还是内部都不会改变。”
		ELSEIF RAND:2 == 0
			PRINTFORMW “你知道讽刺家吗？就是不知道事物价值的人啊”
		ELSE
			PRINTFORMW “哎呀，%CALLNAME:MASTER%，你能说些有趣的话吗？”
		ENDIF
	ELSE
		IF RAND:5 == 0
			PRINTFORMW “蓝要是能懂得享乐就好了……”
		ELSEIF RAND:4 == 0
			PRINTFORMW “说起来蓝的入浴照，听说被禁止了，真无聊。”
		ELSEIF RAND:3 == 0
			PRINTFORMW “如果说眼睛看到的东西是现实的话，眼睛看不见的东西就是虚构的了”
			PRINTFORMW “即使蝴蝶在世界的另一边振翅飞翔，谁都不会介意。”
		ELSEIF RAND:2 == 0
			PRINTFORMW “如果发生错误，就必须修复相应的地方。”
		ELSE
			PRINTFORMW “想要永远玩下去的东西，是不可能会有的。”
		ENDIF
	ENDIF
