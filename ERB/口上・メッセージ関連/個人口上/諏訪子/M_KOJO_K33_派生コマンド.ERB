;==================================================
@M_KOJO_MESSAGE_SCOM_K33_1
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_SCOM_K33_1_1
RETURN 1
@M_KOJO_MESSAGE_SCOM_K33_1_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(TFLAG:50 + 500)
		PRINTFORMW 
		RETURN 1
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		CALL K33_TATARI_COUNTER(3)
		RETURN 1
	ELSEIF NOWEX:TARGET:Ｃ絶頂
		PRINTDATAW
			DATAFORM 「恩%UNICODE(0x2764)%不行、被欺负了%UNICODE(0x2764)%」
			DATAFORM 「呜……嗯哼……！哎呀……先、先……啊啊啊%UNICODE(0x2764)%」
			DATAFORM 「好、再激烈一点……%UNICODE(0x2764)%」
		ENDDATA
		RETURN 1
	ELSEIF NOWEX:MASTER:射精
		PRINTDATAW
			DATAFORM 「恩、恩……嘿嘿、要去了？好啊、射出来吧♪」
			DATAFORM 「嗯、手停住了？看起来挺不错的……ふふ%UNICODE(0x2764)%」
		ENDDATA
		RETURN 1
	ELSEIF NOWEX:MASTER:射精 && NOWEX:TARGET:Ｃ絶頂
		PRINTDATAW
			DATAFORM 「哎呀……哎、哎呀……%UNICODE(0x2764)%呐、一起……嗯、呜……啊%UNICODE(0x2764)%」
			DATAFORM 「嗯……哎……不妙%UNICODE(0x2764)%也、不忍耐、就那样去了……%UNICODE(0x2764)%」
		ENDDATA
		RETURN 1
	ELSEIF TALENT:恋慕 || TALENT:愛欲 || TALENT:炮友
		PRINTDATAW
			DATAFORM 「唔唔%UNICODE(0x2764)%  ん……不会输的♪嗯、嗯%UNICODE(0x2764)%」
			DATAFORM 「哎、谁、谁先去呢%UNICODE(0x2764)%」
			DATAFORM 「嗯、ちゅ……啊、啊……好舒服啊、再多做点吧……%UNICODE(0x2764)%」
		ENDDATA
		RETURN 1
	ELSE
		PRINTFORMW 「唔唔%UNICODE(0x2764)%  ん……不会输的♪嗯、嗯%UNICODE(0x2764)%」
		RETURN 1
	ENDIF
ENDIF
