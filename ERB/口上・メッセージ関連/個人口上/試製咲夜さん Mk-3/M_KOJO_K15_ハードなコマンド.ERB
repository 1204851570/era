;==================================================
;145,アナル奉仕強制
;==================================================
@M_KOJO_MESSAGE_COM_K15_145
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K15_145_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K15_145_1
;-------------------------------------------------
;填写检查（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORMW 
		RETURN 1
	ENDIF
	;基本セット
	;恋慕
	IF TALENT:恋慕
		PRINTFORMW 「咦、这样的地方？ ……真是没有办法」
		PRINTFORMW 向咲夜传达希望舔舐肛門的欲望、有点不情愿的做了
		PRINTFORMW 「嗯、舔、啾……嗉噜……」
		PRINTFORMW 也许是爱极了%CALLNAME:MASTER%的缘故、咲夜一点也不排斥的舔着肛門……
		RETURN 1
	;屈服3
	ELSEIF MARK:不埒刻印 == 3
		CALL HPH_PRINT("「清洗肛門是吗、我明白了HPH」", "L")
		PRINTFORMW 咲夜一听到希望舔舐肛門的要求、瞬间从背後把舌头伸进%CALLNAME:MASTER%的肛門里咕噜咕噜的旋转着
		CALL HPH_PRINT("「啾噜HPH 咧噜HPH 哈呣、嗯啾……HPH 怎么样HPH」", "L")
		PRINTFORMW 在肛門的周围用舌头如画图般舔着、然后伸进肛門里紧贴着前列腺持续刺激、
		PRINTFORMW 每次舌头拔出的时后都会吸吮一下肛門、口水从咲夜的下巴垂落，湿润的唇舌用各种可以想得到的方式努力的在%CALLNAME:MASTER%的肛門里奉仕着……
		RETURN 1
	;それ以外
	ELSE
		PRINTFORMW 「呃、这样的地方……！ ……知、我知道了」
		PRINTFORMW 咲夜似乎想要拒绝爱抚肛門的要求、在强烈的语气下屈服了
		PRINTFORMW 「咕唔……咧噜、唔、咧噜……」
		PRINTFORMW 咲夜紧紧的闭着眼睛、什么也不想，就这样侍奉着%CALLNAME:MASTER%的肛門……
		RETURN 1
	ENDIF
ENDIF
RETURN 1

