RETURN RESULT

@M_KOJO_MESSAGE_COM_K79_49_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 0
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	IF FIRSTTIME(SELECTCOM)
		IF FLAG:時間停止
			PRINTFORML 
			PRINTFORMW 
			RETURN 1
		ELSEIF TALENT:恋慕
			PRINTFORML 「」
			PRINTFORMW 
		ELSE
			PRINTFORML 「」
			PRINTFORMW 
		ENDIF
		;絶頂判定
		CALL M_KOJO_MESSAGE_EVENT_K79_EX_ORGASM
		RETURN 1
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:時間停止
		PRINTFORMW 
		RETURN 1
		
	ELSEIF TALENT:恋慕
		IF ABL:Ｂ感覚 > 9
			SELECTCASE RAND:5
				CASE 0
					PRINTFORMW 
				CASE 1
					PRINTFORMW 
				CASE 2
					PRINTFORMW 
				CASE 3
					PRINTFORMW 
				CASE 4
					PRINTFORMW 
			ENDSELECT
		ELSEIF ABL:Ｂ感覚 > 4
			PRINTFORMW 
		ELSEIF ABL:Ｂ感覚 > 2
			PRINTFORMW 
		ENDIF
	;屈服3
	ELSEIF MARK:不埒刻印 == 3
		PRINTFORMW 
	;それ以外
	ELSE
		PRINTFORMW 
	ENDIF
	;絶頂判定
	CALL M_KOJO_MESSAGE_EVENT_K79_EX_ORGASM
ENDIF
RETURN 1

