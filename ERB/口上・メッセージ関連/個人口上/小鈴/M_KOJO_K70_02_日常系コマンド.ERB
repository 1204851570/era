;==================================================
;300,会話
;TFLAG:193(1=大成功 0=成功 -1=失敗 -2=大失敗（TARGETがMASTERより教養が4以上高い)
;TCVAR:302(0=会話可能 1>=非恋慕時会話不能)
;MARK:反発刻印(3以上=会話不能)(4以上はバグ
;TCVAR:泥酔(1=通常成功or大失敗)
;==================================================
@M_KOJO_MESSAGE_COM_K70_300
;コマンド共通メッセージ(TRAIN_MESSAGE) 
;削除すると「～話をした。」とかが消えます
;CALL TRAIN_MESSAGE
DRAWLINE
;口上メイン呼出
CALL M_KOJO_MESSAGE_COM_K70_300_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K70_300_1
#DIM GUEST, 2
#DIMS GUESTS
#DIM POINT
#DIM 约会会話, 120
;日が変わってたらデート会話リセット
SIF !TCVAR:70:352
	VARSET 约会会話
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	RESETCOLOR
	;ゲストキャラ判定
	CALL M_KOJO_K70_GUEST(0)
	GUEST = RESULT
	GUEST:1 = RESULT:1
	GUESTS = %RESULTS%
	;ここに口上書いて下さい
	IF ALCOHOL_FACE(70) == "酔酒" || ALCOHOL_FACE(70) == "不省人事"
		PRINTFORML %CALLNAME:PLAYER%は%CALLNAME:TARGET%に話し掛けてみたが、%CALLNAME:TARGET%は呂律が回っておらず何を言っているかも聴き取れない
	;会話不能の地の文は恋慕でない場合は省略できず、恋慕の時は表示されないので、ここではどちらの場合も同じように表示されるようにする
	ELSEIF TCVAR:70:会話不能
		IF TALENT:70:恋慕
			PRINTFORML 虽然想向%CALLNAME:TARGET%搭话可是不知道该说些什么
			PRINTFORMW 四周弥漫着尴尬的气氛…
		ENDIF
	ELSEIF GUESTS != ""
		CLEARLINE 2
		PRINTFORML 三人で会話
		DRAWLINE
		;文章上だけであってソース等は変わりません
		PRINTFORML %CALLNAME:PLAYER%は%CALLNAME:TARGET%と%GUESTS%を交えて三人で说着话
	ELSE
		;デート会話
		IF TALENT:70:恋慕 && FLAG:约会的对象 == 70 && DATENAME_SPOT() != "道中" && !约会会話:(CFLAG:70:現在位置 / 10)
			CLEARLINE 2
			PRINTFORML %DATENAME_SPOT()%で约会
			DRAWLINE
		ENDIF
		PRINTFORM %CALLNAME:PLAYER%は%CALLNAME:TARGET%と
		IF TALENT:70:恋慕
			PRINTDATA
				DATAFORM 隣り合って
				DATAFORM 稍微照れながら
				DATAFORM 在相对微笑着的同时
				DATAFORM 和やかな雰囲気で
				DATAFORM 稍微熱くなりながら
				DATAFORM 玩闹着的同时
			ENDDATA
		ELSE
			PRINTDATA
				DATAFORM くだらない
				DATAFORM たわいのない
				DATAFORM 客套地
				DATAFORM 平和な
				DATAFORM よくある
				DATAFORM 稍微した
				DATAFORM 可笑しな
			ENDDATA
		ENDIF
		IF TFLAG:SELECTCOM的分岐 > 0
			PRINTFORML 話をして盛り上がった
		ELSEIF TFLAG:SELECTCOM的分岐 < 0
			PRINTFORML 話をしていたが話が途切れてしまった……
		ELSE
			PRINTFORML 说着话
		ENDIF
	ENDIF
	PRINTW 
	CALL M_KOJO_COLOR_K70
	;酔ってる
	IF ALCOHOL_FACE(70) == "酔酒" || ALCOHOL_FACE(70) == "不省人事"
		IF !RAND:2
			PRINTFORML 「らぁ～！ らめれすおぉ～、おひゃひぇはぁ～～！」
			PRINTFORMW 「おうあおうんあ～！ おうおえあえん～～！」
		ELSE
			PRINTFORML 「あぁ～～！！ わらひのおうあおんあ～！」
			PRINTFORMW 「いあああらわらひあ～～！！」
		ENDIF
		PRINTFORML 
		PRINTFORMW ほとんど会話にならなかった……
	;会話不能
	ELSEIF TCVAR:70:会話不能
		IF TALENT:70:恋慕 || TALENT:70:思慕
			PRINTFORML 「その話はさっきしましたよ～」
			PRINTFORMW 「もう、%CALLNAME:MASTER%さんは本を読まないから話題が尽きちゃうんです！」
			PRINTFORMW 「ちょうどいい本があります、『むくちをなおすほん』！ あ、これは違うかな？」
		ELSE
			PRINTFORMW 「あはは…… その話はさっきもしましたよね……」
		ENDIF
	;ゲストキャラと同室だと各キャラ1日1回ずつゲスト会話が発生（お茶とは別）
	;4=小鈴が恋慕(同行中だと最優先),3=ゲストと同行中in鈴奈庵,2=ゲストとデート中orゲストが恋慕で同行中,1=それ以外
	ELSEIF GUESTS != ""
		SELECTCASE GUESTS
		CASE "霊夢"
			IF GUEST == 4
				IF INRANGE(CFLAG:70:現在位置, 0, 100)
					PRINTFORML 「霊夢さん、ここの神社ってなんのご利益があるんでしたっけ？」
					PRINTFORMW 「そ、そのー、恋愛成就とかってあります？」
				ELSE
					PRINTFORML 「霊夢さんみたいに強くなるにはどうしたらいいんでしょう」
					PRINTFORMW 「まずはイージーモードで抱え落ち不会うに…？ え？」
				ENDIF
			ELSEIF GUEST == 3
				IF !RAND:2
					PRINTFORML 「ご用は済みましたか？」
					PRINTFORMW 「霊夢さんにはお世話になってますから、ゆっくりしていってください」
				ELSE
					PRINTFORMW 「そうだ！ ちょうどこの本で、お二人の相性占いができますよ！」
					PRINTFORML 「って、霊夢さんはご自己できましたね……」
					PRINTFORMW 「それでもやってみましょうよ！ ささっ！」
				ENDIF
			ELSEIF GUEST == 2
				PRINTFORML 「霊夢さんたち最近仲良いですよねー」
				PRINTFORMW 「ほら、今だって手繋いでますし」
				PRINTFORML 「あれあれ？ 気づいてなかったんですかー？」
				PRINTFORMW 「ふふ、顔赤くなってますよ」
			ELSE
				PRINTFORML 「そうだ、ちょうど霊夢さんも居ますし、話しておきたいことがありまして」
				PRINTFORMW 「最近里で噂になっているんですが――」
			ENDIF
		CASE "魔理沙"
			IF GUEST == 4
				IF INRANGE(CFLAG:70:現在位置, 500, 600) && FLAG:住宿人物 != 70
					IF MAIN_MAP == 5
						PRINTFORMW 「%CALLNAME:MASTER%さんも魔理沙さんもこんな森の中に住んでいるんですよね」
					ELSE
						PRINTFORMW 「魔理沙さんはこんな森の中に住んでいるんですよね」
					ENDIF
					PRINTFORML 「いいなぁ、私もここで暮らしてみたい……」
					PRINTFORMW 「なんたって梆硬の妖気にゾクゾクします～！」
				ELSE
					PRINTFORML 「魔理沙さんの箒いいなぁ～」
					PRINTFORML 「それがあれば私も空を飛べるのに」
					PRINTFORMW （%CALLNAME:MASTER%さんと一緒に空の旅、なんてロマンチック――）
					PRINTL 
					PRINTFORMW 「えっ！？ 魔法使じゃないと飛べないんですか！？ がっくし……」
				ENDIF
			ELSEIF GUEST == 3
				IF !RAND:2
					PRINTFORML 「ご用は済みましたか？」
					PRINTFORMW 「魔理沙さんにはお世話になってますから、ゆっくりしていってください」
				ELSE
					PRINTFORML 「そうだ！ 魔術札っていう外の世界の札玩耍が手に入ったんですよ」
					PRINTFORMW 「魔法の事はよく判らなかったので、魔理沙さんと三人でやってみません？」
				ENDIF
			ELSEIF GUEST == 2
				PRINTFORML 「魔理沙さんたち最近仲良いですよねー」
				PRINTFORMW 「ほら、今だって手繋いでますし」
				PRINTFORML 「あれあれ？ 気づいてなかったんですかー？」
				PRINTFORMW 「ふふ、顔赤くなってますよ」
			ELSEIF GUEST == 1
				PRINTFORML 「そうだ、ちょうど魔理沙さんも居ますし、話しておきたいことがありまして」
				PRINTFORMW 「最近里で噂になっているんですが――」
			ENDIF
		CASE "紫"
			IF GUEST == 4
				PRINTFORML 「紫さんって何者なんでしょう……明らかにオーラが違いますよね……」
				PRINTFORMW 「それなのに大して隠す素振りもないようですし、神出鬼没でいつの間にか消えちゃいますし……」
				PRINTFORMW 「でもきっと悪い妖怪じゃないと、私はそう思ってます」
			ELSEIF GUEST == 3
				PRINTFORML 「えっと、きょ、今日はお二人して何のご用でしょうか……？」
				PRINTFORMW 「私また何かまずいことしちゃいました……？」
			ELSEIF GUEST == 2
				PRINTFORML 「%CALLNAME:MASTER%さんは紫さんの事どれくらい知っているんですか？」
				PRINTFORMW 「なんだか親密のようなので、私よりお詳しいのかなーと……」
			ELSEIF GUEST == 1
				PRINTFORML 「%CALLNAME:MASTER%さんは神隠しって知ってます？」
				PRINTFORMW 「意外と身近な所で起こったりしますので気をつけてくださいね、あはは」
			ENDIF
		CASE "文"
			IF GUEST == 4
				IF INRANGE(CFLAG:70:現在位置, 700, 900)
					PRINTFORML 「妖怪之山って確かに怖い所かもしれないですけど」
					PRINTFORMW 「文さんみたいな天狗が居てくれるとなんだか安心しますね」
				ELSE
					PRINTFORMW 「ご存知かもしれませんが、文さんはあの『文文新闻』を書いてる天狗の文さんなんですよ」
					PRINTFORML 「天狗なのによく人間之里に来ては精心取材してるんです」
					PRINTFORMW 「変わった方ですよねー」
				ENDIF
			ELSEIF GUEST == 3
				;カリスマは景気の指針
				IF CHARISMA_NOW() >= 100
					PRINTFORML 「文さんの新聞、売れ行き好調ですよー」
					PRINTFORMW 「今後の展望次第では増刷してもらおうかなぁ」
				ELSE
					PRINTFORML 「文さんの新聞、ここの所売れ行きが悪くて……」
					PRINTFORMW 「何か新しい流れを取り入れた方が良さそうですね」
				ENDIF
				SIF !FLAG:新聞購読
					PRINTFORMW 「あっ！ %CALLNAME:MASTER%さんも一部どうですか？」
			ELSEIF GUEST == 2
				PRINTFORML 「今日はひょっとしてお二人约会ですかー？」
				PRINTFORMW 「ふふ、良かったら私が写真を撮ってあげますよ♪」
			ELSEIF GUEST == 1
				PRINTFORML 「あれ、ひょっとして文さんの取材ですかー？」
				IF TALENT:70:思慕
					PRINTFORML 「文さん！ %CALLNAME:MASTER%さんの事でしたら私に聞いてください！」
					PRINTFORMW 「\@ CFLAG:70:1005 >= 8 ? ウチのお得意さんですし、 # \@私%CALLNAME:MASTER%さんのこと詳しいですから！」
				ELSE
					PRINTFORML 「%CALLNAME:MASTER%さんの事ですかぁ」
					PRINTFORMW 「確かに%CALLNAME:MASTER%さんって謎めいた所ありますよねー」
				ENDIF
			ENDIF
		CASE "猯藏"
			IF GUEST == 4
				PRINTFORML 「ところで%CALLNAME:MASTER%さんは狸猫妖怪の事どう思ってますか？」
				PRINTFORMW 「親しみやすいだとか、実は良い妖怪だとか――」
				SIF ABL:69:親密 >= 5
					PRINTFORMW 「いやぁ、最近仲睦まじいようなの……あっ！ いえ！ なんでもないです！」
			ELSEIF GUEST == 3
				IF TALENT:69:恋慕
					PRINTFORMW 「それにしてもお二人は相変わらず仲睦まじいですね」
					PRINTFORMW （でも本当に仲良さそう　疑うのは野暮だったかな）
				ELSE
					PRINTFORMW 「今日はお二人どうされたんですか？」
					PRINTFORMW （二人とも楽しそうだし、猯藏さんの事だからそこまで心配しなくてもいいかな）
				ENDIF
			ELSEIF GUEST == 2
				PRINTFORML 「お二人とも、今日も仲良さそうですねー」
				PRINTFORMW 「まるで约会してるみたいです」
			ELSEIF GUEST == 1
				PRINTFORML 「%CALLNAME:MASTER%さんも狐狸には気をつけた方が良いですよ」
				PRINTFORMW 「人类に化けて身近な所で悪事を企んでいたりしますからね」
			ENDIF
		CASE "阿求"
			;8=独占,7=悲恋,6=揶揄,5=信頼,4=敵対,3=恋慕,2=応援,1=平穏
			SELECTCASE M_KOJO_K70_BALANCE(70, 80)
			CASE 8, 5
				IF FLAG:约会的对象 == 80
					PRINTFORML 「ふふ、別に邪魔しようなんて思ってないですよ」
					PRINTFORMW 「その代わり今度は私とも约会してくださいね♪」
				ELSE
					SELECTCASE RAND:3
					CASE 1
						PRINTFORML 「%CALLNAME:MASTER%さんにとっておきの情報を教えてあげましょう」
						PRINTFORMW 「阿求は"幺樂団"という知る人ぞ知る伝説のバンドの大ファンなんですよ～」
					CASE 2
						PRINTFORMW 「阿求ってば普段は堅苦しいのに、自分の趣味の話になると途端に砕けちゃうんですよねー」
						PRINTFORML 「オンオフがはっきりしてるというか、メリハリがあるというか」
						PRINTFORMW 「そういう所は可笑しな娘だなって思ったりー」
					CASEELSE
						PRINTFORML 「%CALLNAME:MASTER%さんから見た阿求ってどんな娘ですか？」
						PRINTFORMW 「私に是つも難しい事ばっか言って馬鹿にしてくるんですよー！」
					ENDSELECT
				ENDIF
			CASE 7
				SELECTCASE RAND:2
				CASE 1
					PRINTFORML 「%CALLNAME:MASTER%さん、いつも阿求と一緒ですよねー」
					PRINTFORMW 「たまには私とも遊んで欲しいなー、なんて」
				CASEELSE
					PRINTFORML 「阿求ってばいつも私に%CALLNAME:MASTER%さんとの惚気話をしてくるんですよー」
					PRINTFORMW 「私、阿求のことを嫌いになりたくないんです……だから……」
				ENDSELECT
			CASE 6
				SELECTCASE RAND:3
				CASE 1
					PRINTFORML 「%CALLNAME:MASTER%さんにとっておきの情報を教えてあげましょう」
					PRINTFORMW 「阿求は幺樂団という知る人ぞ知る伝説のバンドの大ファンなんですよ！」
					PRINTFORMW 「機会があったら聞かせてもらえば、きっと阿求も喜びますよ～」
				CASE 2
					PRINTFORML 「%CALLNAME:MASTER%さんは阿求のどんなところが好きなんですかー？」
					PRINTFORMW 「何か私の知らないような一面とかないです？？」
				CASEELSE
					PRINTFORML 「阿求ってばいつも私に%CALLNAME:MASTER%さんとの惚気話をしてくるんですよー」
					PRINTFORMW 「堅苦しい奴ですけど、大事にしてあげてくださいね」
				ENDSELECT
			CASE 4
				IF FLAG:约会的对象 == 80
					PRINTFORML 「ところで%CALLNAME:MASTER%さん、最近阿求と仲良いですよね」
					PRINTFORMW 「なんかあったんですかー？ よもや変なことしてないですよねー？」
				ELSEIF M_KOJO_K70_SUZUNAAN(CFLAG:70:現在位置) == 1
					PRINTFORML 「ところで阿求、あんた用事が済んだのならそろそろ帰ったらー？」
					PRINTFORMW 「お客さんも来てるし、居座られても困るわ」
				ELSE
					PRINTFORML 「あの、%CALLNAME:MASTER%さん、他所へ行きませんか？」
					PRINTFORMW 「い、いえ、他意はありませんよ！ ただ稍微、ここにいるとなんだか落ち着かなくて……」
				ENDIF
			CASE 3, 1
				PRINTFORML 「阿求は私の友達ですよ」
				PRINTFORMW 「年が近かったせいか、昔からの付き合いなんです」
			CASE 2
				SELECTCASE RAND:2
				CASE 1
					PRINTFORML 「%CALLNAME:MASTER%さんは阿求のことどう思ってますか？」
					PRINTFORMW 「この娘、見栄っ張りで堅物だけど、根っこの部分はほんとに女の子なんですよ～」
				CASEELSE
					PRINTFORML 「阿求ってばいつも私に%CALLNAME:MASTER%さんとの惚気話をしてくるんですよー」
					PRINTFORMW 「堅苦しい奴ですけど、大事にしてあげてくださいね」
				ENDSELECT
			ENDSELECT
		ENDSELECT
		;一日一回
		TCVAR:70:(M_KOJO_K70_GUEST_TCVAR(GUEST:1, 0)) = 1
		;デート同行会話と被る場合があるのでこちらを優先させ発生させないようにする
		SETBIT TCVAR:70:360, 0
	;不機嫌
	ELSEIF TALENT:70:心情 == -1 || CFLAG:70:ブチギレ || MARK:70:反発刻印 >= 3
		IF TALENT:70:恋慕
			PRINTFORMW 「うーん、ごめんなさい、今稍微考え事が……」
		ELSE
			PRINTFORMW 「はぁ…是这様吗ー……」
		ENDIF
	;デート会話
	ELSEIF TALENT:70:恋慕 && FLAG:约会的对象 == 70 && DATENAME_SPOT() != "道中" && !约会会話:(CFLAG:70:現在位置 / 10)
		POINT = CFLAG:70:現在位置 / 10
		;こんなにこだわるつもりはなかったのに、某美人妻がたくさん喋るのに釣られてつい
		;新たに地点ができたら修正します（多分）
		SELECTCASE CFLAG:70:约会中
		CASE 0
			IF POINT == 5
				PRINTFORML 「あれ、ここって妖精の棲家ですか？」
				PRINTFORMW 「普段はただの木に視えてたのに…不思議……」
			ELSEIF POINT == 6
				PRINTFORML 「わー、神社のそばにこんな遺跡があっただなんて」
				PRINTFORMW 「なんだか不思議な造形ですね……ほんとに遺跡なのかなぁ」
			ELSE
				PRINTFORML 「やっぱりここは落ち着きますねー」
				PRINTFORMW 「妖怪たちが玩耍に来ちゃうのも、なんだか判る気がします」
			ENDIF
		CASE 1
			IF POINT == 15
				PRINTFORML 「ひぇ～、なんで墓地なんか……」
				PRINTFORMW 「はやく他所へ行きましょうよ～！」
			ELSEIF POINT == 16
				PRINTFORML 「知ってますよ、この道場は仙界の一部なんですよね」
				PRINTFORMW 「仙人か～、%CALLNAME:MASTER%さんはなってみたいですかー？」
			ELSE
				PRINTFORML 「ここって妖怪もたくさん住んでるんですよね……」
				PRINTFORMW 「うーん、妖怪なのに善行を積んだりなんかして大丈夫なんですかねー？」
			ENDIF
		CASE 2
			IF POINT == 21 || POINT == 22
				PRINTFORML 「%CALLNAME:MASTER%さん%CALLNAME:MASTER%さん！ 今日はあそこに行きましょう！」
				PRINTFORMW 「えへへ、今度约会したとき行こうって決めてたんです♪」
			ELSEIF POINT == 23
				PRINTFORML 「%CALLNAME:MASTER%さん、こんな時間からお酒ですかー？」
				PRINTFORMW 「あんまり呑むと帰れなくなっちゃいますよ～」
			ELSEIF POINT == 24
				PRINTFORML 「私も昔はここに通ってましたよ」
				PRINTFORMW 「あ、なつかしー！ この落書きまだあったんだ」
			ELSEIF POINT == 25
				IF DAY * 1440 + TIME - EX:MASTER:上次的入浴時间 < 360
					PRINTFORML 「ふぅ～！ 清爽しました！」
					PRINTFORMW 「ホカホカです♪」
				ELSE
					PRINTFORML 「ここの銭湯は私もたまに来ますよ」
					PRINTFORMW 「やっぱり广阔お浴室は気持ちいいですよね～！」
				ENDIF
			ELSEIF POINT == 26
				;入った時点で合意取得するのね…,うふふ前か後かは絶頂したかで判断するので事前に絶頂させてるとちょっと齟齬
				IF EX:70:Ｃ絶頂 + EX:70:Ｖ絶頂 + EX:70:Ａ絶頂 + EX:70:Ｂ絶頂 + EX:70:Ｍ絶頂
					IF TALENT:70:処女
						PRINTFORMW 「……\/\/\/」
					ELSE
						PRINTFORMW 「……%UNICODE(0x2764)%」
					ENDIF
				ELSE
					IF TALENT:70:処女
						PRINTFORMW 「あの…… や、温柔的お願いしますね……\/\/\/」
					ELSE
						PRINTFORMW 「じー……%UNICODE(0x2764)%」
					ENDIF
				ENDIF
			ELSE
				PRINTFORML 「こっちは主に居住区ですよ」
				PRINTFORMW 「阿求的家でも玩耍に行きます？」
				;と言いつつ行こうとすると拒否されちゃいます。女の子って難しいね
			ENDIF
		CASE 3
