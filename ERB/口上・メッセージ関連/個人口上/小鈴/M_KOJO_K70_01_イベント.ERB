			PRINTFORMW 「这是%CALLNAME:MASTER%先生不知道的！」
		ELSE
			PRINTFORMW 「…………！」
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;泣かせてしまった　[臆病]持ち
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
		RETURN 1
	ENDIF
ENDIF
RETURN 1

