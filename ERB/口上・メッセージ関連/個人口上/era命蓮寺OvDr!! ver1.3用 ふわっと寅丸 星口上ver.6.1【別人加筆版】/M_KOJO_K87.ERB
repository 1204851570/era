;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(TFLAG:50 + 500)
		SIF CFLAG:1501 == 0
		DRAWLINE
		PRINTFORMW 「呀啊！？刚才,像被…电了一样…」
		PRINTFORMW 「%MASTERNAME:87%…干了,什么啊…？」
		RETURN 1
	ENDIF
	;基本セット
	;※CFLAG:1501 == 1でＧスポ責めしている時、0で選んだコマンドで継続して突いている時の口上
	;過激なイタズラ中
	IF CFLAG:恶作剧 && CFLAG:睡眠
		RETURN 1
	ELSE
		;挿入Ｇスポ責め
		IF CFLAG:1501 == 1
			CFLAG:1501 = 0
			;Ｖ感覚1以下
			IF ABL:Ｖ感覚 <= 1
				;/Ｖ敏感！
				PRINTFORMW 「唔,啊……%UNICODE(0x2665)%　这样,不行…」
				PRINTFORMW 「这个,要是上瘾了的话…我……」
			;恋慕
			ELSEIF TALENT:恋慕
				;これでMASTERが射精
				IF NOWEX:PLAYER:11
					;これでTARGETがＶ絶頂する
					IF NOWEX:1
						PRINTFORML 「里面,噫噫ー…啊啊……%UNICODE(0x2665)%　要,来了…」
						PRINTFORMW 「哈！？　要胀起来为止…！？　不行！如果再在那里出来的话…」
					ELSE
						PRINTFORML 「%MASTERNAME:87%…快要出来了吧…」
						PRINTFORMW 「在我的里面,%MASTERNAME:87%,尽情的出来吧%UNICODE(0x2665)%」
					ENDIF
				ELSE
					IF RAND:2 == 0
						PRINTFORML 「这,里……っ　麻麻的…っ%UNICODE(0x2665)%」
						PRINTFORMW 「呼噫…%UNICODE(0x2665)%　又变强了…呜哇…%UNICODE(0x2665)%」
					ELSE
						PRINTFORML 「哇啊%UNICODE(0x2665)%！不得了的,又来了……」
						PRINTFORMW 「要是真的,上瘾了…　要变成淫乱的孩子了…%UNICODE(0x2665)%」
					ENDIF
				ENDIF
			;不埒3
			ELSEIF MARK:不埒刻印 == 3
				PRINTFORMW 「那里…就是那里…再来一次吧…%UNICODE(0x2665)%」
			;それ以外
			ELSE
				PRINTFORMW 「嗯…　麻麻的,好舒服…」
			ENDIF
		;連続
		ELSE
			DRAWLINE
			;正常位、対面立位
			IF TEQUIP:体位 == 1
