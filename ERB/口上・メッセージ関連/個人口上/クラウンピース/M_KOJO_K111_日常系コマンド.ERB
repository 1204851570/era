	ELSEIF TFLAG:194 == 3
		PRINTFORMW 「哎、不……还是和%CALLNAME:MASTER%两个人独处最好了……」
	;ムード失敗
	ELSEIF TFLAG:194 == 4
		PRINTFORML 「哇哇！别这样！」
		PRINTFORMW 「已经！考虑一下我的感受吧」
	;泥酔失敗
	ELSEIF TFLAG:194 == 5
		PRINTFORMW 「嗯嗯～？%CALLNAME:MASTER%、怎么了？」
		PRINTL 
		PRINTFORMDW 有清醒的目撃者在所以无法实施酔姦
	;泥酔
	ELSEIF TCVAR:111:烂醉 == 1
		PRINTFORMW 「呵呵～～喵～～喵喵喵～～？喂喂~？？」
	;乱交
	ELSEIF GET_TARGETNUM() > 1
		IF FIRSTTIME("乱交")
			PRINTFORMW 「诶！呵、和其他的孩子一起吗……？」
			PRINTFORMW 「嘛、嘛算了……」
		ELSE
			PRINTFORML 「%TOFULL(TOSTR(GET_TARGETNUM()))%ぴーってやつ？」
			PRINTFORMW 「ちゃんとびょーどーに相手してよ？」
		ENDIF
	;お泊り
	ELSEIF (CFLAG:111:現在位置 == 情人用宿屋 || CFLAG:111:現在位置 == OMANEKIBEYA()) && CFLAG:TARGET:诶嘿嘿 != 2
		PRINTFORMW 「きゃっ\/\/\/」
	;それ以外
	ELSEIF !TCVAR:351
		PRINTFORMW 「あっ\/\/\/」
	ENDIF
	;先に押し倒された
	;うふふフラグは押し倒した時点で変更されるんでした
	;押し倒し返しは小傘口上さんを参考にして専用TCVARを作ることに
;	IF !FLAG:70 && !TFLAG:194 && CFLAG:111:うふふ == 2
	IF TCVAR:351
		PRINTFORML 
		PRINTFORMW 「きゃはっ♪ その気になってくれた？」
		PRINTFORMW 「うれしいっ%UNICODE(0x2764)% 随便いいよ%UNICODE(0x2764)%」
	ELSEIF !FLAG:70 && !TFLAG:194
		PRINTFORML 
		SELECTCASE TCVAR:111:初次拥抱
		;普通の初回,キスがまだ,クラウンピースはキスが先だとか後だとか大して気にしてない
		CASE 1, 2
			PRINTFORML 「え……%CALLNAME:MASTER%……？」
			PRINTFORMW （こ、これってひょっとして、そういうこと、だよね……？）
			PRINTFORML 「うん%UNICODE(0x2764)% いいよ、%CALLNAME:MASTER%%UNICODE(0x2764)%」
			PRINTFORMW 「やさしくしてね%UNICODE(0x2764)%」
		;媚薬or発情
		CASE 3
			PRINTFORML 「ふわぁ……%CALLNAME:MASTER%、なんだかあたい、頭がぼうっとして……」
			PRINTFORMW 「はぁ……もう、随便……」
		;時姦
		CASE 4
			PRINTFORML 「なにするの！ いや！」
			PRINTL 
			PRINTFORMDW 口ではそういいつつも%CALLNAME:TARGET%は%CALLNAME:MASTER%抱住了
			PRINTL 
			PRINTFORMW 「えっ！？ なんで……どうして……？」
;		CASE 5
;			PRINTFORML 押しの弱い%CALLNAME:TARGET%は、%CALLNAME:MASTER%を拒むことができなかった…
		;もうちょっと
		CASE 6
			PRINTFORML 「ふぇ……そんな……」
			PRINTFORMW 「いやだ……ごめんね%CALLNAME:MASTER%……」
		;ダメ
		CASE 7
			PRINTFORML 「んわぁっ！ なにするんだよう！」
			PRINTFORMW 「そーゆーのいけないんだよ？」
		;早すぎ
		CASE 8
			PRINTFORML 「きゃぁあっ！！！ なにするの！ ヘンタイ！ ヘンタイ！」
			PRINTFORMW 「ご主人様ーーっ！！ こいつ地獄に堕としてくださーい！！」
		;初回じゃない
		CASEELSE
			SELECTCASE RAND:6
			CASE 1
				PRINTFORML 「嗯♪%CALLNAME:MASTER%的えっち%UNICODE(0x2764)%」
				PRINTFORMW 「ふふ%UNICODE(0x2764)% 随便いいよ%UNICODE(0x2764)%」
			CASE 2
				PRINTFORMW 「きゃはっ♪ もう、しょうがないにゃぁ♪」
			CASE 3
				PRINTFORMW 「ふふっ♪ いいよ%UNICODE(0x2764)% 一緒にえっちな玩耍しよっ%UNICODE(0x2764)%」
			CASE 4
				PRINTFORMW 「いやー%UNICODE(0x2764)% %CALLNAME:MASTER%のケダモノー！」
			CASE 5
				PRINTFORMW 「ん……、%CALLNAME:MASTER%、今日は甘えてもいーい？」
			CASEELSE
				IF TALENT:111:妊娠
					PRINTFORMW 「きょ、今日は稍微、ゆっくりして……%UNICODE(0x2764)%」
				ELSE
					PRINTFORMW 「きょ、今日は稍微、激烈地して……%UNICODE(0x2764)%」
				ENDIF
			ENDSELECT
			;妊娠中
			IF TALENT:111:妊娠 && CFLAG:111:妊娠自覚状態 == 1
				PRINTFORML 
				PRINTFORMW 「あ！ お腹の子もいるんだから、やさしくしてよ？」
			;危険日
			ELSEIF CFLAG:111:生理周期 == 7 && !TALENT:111:妊娠 && CFLAG:111:允许无套 == 2
				PRINTFORML 
				PRINTFORMW （今日はひょっとして……チャンスだわ♪）
			ENDIF
		ENDSELECT
	ENDIF
ENDIF
