	;デート会話
	ELSEIF TALENT:111:恋慕 && FLAG:约会的对象 == 111 && DATENAME_SPOT() != "道中" && !约会会話:(CFLAG:111:現在位置 / 10)
		POINT = CFLAG:111:現在位置 / 10
		SELECTCASE CFLAG:111:约会中
		CASE 0
			IF POINT == 5
				;三月精は保留！
				PRINTFORMW 「哇！这里是……！」
			ELSEIF POINT == 6
				PRINTFORML 「什么啊这里？好大啊……」
				PRINTFORMW 「这是什么啊？哎呀!」
			ELSE
				PRINTFORML 「神社里应该有神明吧？」
				PRINTFORMW 「在这里的话、应该比主人还强吧～？」
			ENDIF
		CASE 1
			IF POINT == 15
				PRINTFORML 「哇~！坟墓啊～♪」
				PRINTFORMW 「大家上地狱去吧！♪」
			ELSEIF POINT == 16
				PRINTFORML 「这里是仙人的道场吗？」
				PRINTFORMW 「仙人吗～、感觉朋友大人好像有说些什么～」
			ELSE
				PRINTFORML 「这里可是有名的妖怪寺呢」
				PRINTFORMW 「寺庙和神社有什么不同？？」
			ENDIF
		CASE 2
			IF POINT == 21 || POINT == 22
