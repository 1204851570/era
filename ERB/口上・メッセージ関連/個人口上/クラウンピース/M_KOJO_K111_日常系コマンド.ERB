;-------------------------------------------------
IF LOCAL
	CALL M_KOJO_COLOR_K111
	;押し倒され値	ENDURE(111) >= RAND:300 + 300で真,しかしRANDの値は変化するのでここでは400を超えたら押し倒されることにする
	IF FLAG:時間停止 == 1
	;野外失敗
	ELSEIF TFLAG:194 == 2
		IF ENDURE(111) > 400
			PRINTFORML 「咦\/\/\/那种……\/\/\/」
			PRINTFORMW 「这、这里是外面……\/\/\/」
		ELSE
			PRINTFORML 「哇！什么！？」
			PRINTFORMW 「这、这里可是外面哦……？」
		ENDIF
	;人目失敗
	ELSEIF TFLAG:194 == 1
		IF ENDURE(111) > 400
			PRINTFORML 「咦\/\/\/那种……\/\/\/」
			PRINTFORMW 「有人在看……\/\/\/」
		ELSE
			PRINTFORML 「哇！什么！？」
			PRINTFORMW 「有人在看呢……？」
		ENDIF
