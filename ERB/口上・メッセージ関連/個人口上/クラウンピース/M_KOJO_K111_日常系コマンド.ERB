@M_KOJO_MESSAGE_SCOM_K111_63_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
;ほぼキスと同じ
IF LOCAL
	CALL M_KOJO_COLOR_K111
	IF FLAG:時間停止
	ELSEIF CFLAG:111:睡眠
		PRINTFORMW 「んみゅ……くぅ……」
	;口上によるCOM成否判定のおかげで強制キャンセルをかけることができました,多謝
;	ELSEIF (TALENT:111:機嫌 == -1 || CFLAG:111:ブチギレ || MARK:111:反発刻印 >= 3) && FIRSTTIME("ファーストキス")
;		IF TALENT:111:恋慕
;			PRINTFORMW 「ふぇぇっ！？ にゃ、そんなやり方、%CALLNAME:MASTER%じゅるい……」
;		ELSE
;			PRINTFORMW 「んんっっ！？ そんな、こんなときに……うぅ……」
;		ENDIF
	ELSEIF TALENT:111:心情 == -1 || CFLAG:111:ブチギレ || MARK:111:反発刻印 >= 3
		IF TALENT:111:恋慕
			PRINTFORMW 「むぁっ……、もー！」
		ELSE
			PRINTFORMW 「んむ…………」
		ENDIF
	;デコちゅーからファーストキスは廃止
;	;ファーストキス
;	ELSEIF FIRSTTIME("ファーストキス")
;		PRINTFORMW 「あっ……\/\/\/」
;		PRINTFORMW 「キス……初めて……\/\/\/」
;		PRINTFORMW 「うれしい……\/\/\/」
	ELSE
		SELECTCASE RAND:6
		CASE 0
			$KISS_ZERO
			PRINTFORMW 「ちゅーちゅっ%UNICODE(0x2764)%」
		CASE 1
			$KISS_ONE
			PRINTFORMW 「ちゅっちゅ%UNICODE(0x2764)% ちゅっちゅ%UNICODE(0x2764)%」
		CASE 2
			$KISS_TWO
			PRINTFORMW 「ちゅっ%UNICODE(0x2764)% あむっ%UNICODE(0x2764)% ぺろっ%UNICODE(0x2764)%」
		CASE 3
			SIF EXP:111:接吻経験 < 20
				GOTO KISS_ZERO
			PRINTFORMW 「はむ…れろ……ん…んぁ%UNICODE(0x2764)% もっとぉ……%UNICODE(0x2764)% んぁ、あむっ%UNICODE(0x2764)%」
		CASE 4
			SIF EXP:111:接吻経験 < 20
				GOTO KISS_ONE
			PRINTFORMW 「ちゅ…ちゅぅぅ……あぅ…れろ……ぁむ%UNICODE(0x2764)% れろ…もっと……ちゅるっ%UNICODE(0x2764)%」
		CASE 5
			SIF !TALENT:111:恋慕
				GOTO KISS_TWO
			PRINTFORMW 「ちゅっ%UNICODE(0x2764)%」
			PRINTFORML 
			PRINTFORMW 「んふ%UNICODE(0x2764)% ね、もっかい！」
			PRINTFORML 
			PRINTFORMW 「ちゅっちゅ%UNICODE(0x2764)%」
			SIF EXP:111:接吻経験 * 2 < RAND:100
				RETURN 1
			PRINTFORML 
			PRINTFORMW 「もっかいもっかい♪」
			PRINTFORML 
			PRINTFORMW 「ちゅっちゅ%UNICODE(0x2764)% ちゅっちゅ%UNICODE(0x2764)%」
			PRINTFORML 
			PRINTFORMW 「嘿嘿%UNICODE(0x2764)%」
		ENDSELECT
	ENDIF
ENDIF
RETURN 1
