@M_KOJO_MESSAGE_COM_K111_413_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL && DISHASSI == 111
	CALL M_KOJO_COLOR_K111
	;ハンバーガー
	IF DISHNAME == "漢堡"
		IF TCVAR:111:356
			PRINTFORML 「诶！？给我做汉堡吗！？」
			IF TALENT:111:恋慕
				PRINTFORML 「嘿嘿、%CALLNAME:MASTER%好温柔～%UNICODE(0x2764)%」
				PRINTFORML 「一起做吧♪」
			ELSEIF TALENT:111:思慕
				PRINTFORML 「%CALLNAME:MASTER%啊、真温柔啊~！」
				PRINTFORML 「也让我来帮忙吧！」
			ELSE
				PRINTFORML 「%CALLNAME:MASTER%很懂的嘛！」
				PRINTFORML 「我也会帮你的！」
			ENDIF
		ELSE
			PRINTFORML 「什么？什么？要做汉堡吗？」
			PRINTFORML 「好啊！我也会帮你的！」
		ENDIF
		IF ABL:111:料理技能 >= 3
			SELECTCASE RAND:4
			CASE 1
				PRINTFORML 「对了！ 做乳酪汉堡吧！」
				DISHTASTE += "乳酪/"
				TCVAR:111:357 = 1
			CASE 2
				PRINTFORML 「对了！ 做豆沙饼海鲜汉堡吧！」
				DISHTASTE += "香料/"
				TCVAR:111:357 = 2
			CASE 3
				PRINTFORML 「对了！试着做像主人那样的阿比斯汉堡吧！」
				DISHTASTE += "香料/新鲜的/" * 3
				TCVAR:111:357 = 3
			CASEELSE
				PRINTFORML 「对了！试着做一个有着妖精形象的妖精汉堡吧！」
				DISHTASTE += "可爱/"
				TCVAR:111:357 = 4
			ENDSELECT
		ELSEIF ABL:111:料理技能 >= 1
			SELECTCASE RAND:4
			CASE 1
				PRINTFORML 「对了！ 做乳酪汉堡吧！」
				DISHTASTE += "乳酪/"
				TCVAR:111:357 = 1
			CASE 2
				PRINTFORML 「对了！ 做豆沙饼海鲜汉堡吧！」
				DISHTASTE += "香料/"
				TCVAR:111:357 = 2
			CASEELSE
				;はずれ
			ENDSELECT
		ENDIF
	;お菓子
	ELSEIF DISHTYPE == 3
		SELECTCASE RAND:2
		CASE 1
			PRINTFORML 「做点心？」
		CASEELSE
			PRINTFORML 「要做甜点吗！？」
		ENDSELECT
		SELECTCASE RAND:3
		CASE 1
			PRINTFORML 「我也想做！」
		CASE 2
			IF TALENT:111:思慕 || TALENT:111:恋慕
				PRINTFORML 「我来帮你吧！」
			ELSE
				PRINTFORML 「我来给你帮忙、你也给一点我吧！」
			ENDIF
		CASEELSE
			IF TALENT:111:思慕 || TALENT:111:恋慕
				PRINTFORML 「一起做吧♪」
			ELSE
				PRINTFORML 「让我也来试试！」
			ENDIF
		ENDSELECT
		;クッキー
		IF DISHNAME == "曲奇"
			PRINTW 
			PRINTFORML 「这是主人、这位是朋友！」
			TCVAR:111:357 = 1
			IF TALENT:111:恋慕 && !RAND:8
				PRINTFORML 「啊啊！那个……虽然是%CALLNAME:MASTER%什么的……」
				TCVAR:111:357 = 2
			ENDIF
			PRINTFORML 「怎么样？像吗？」
		ENDIF
	ELSE
		SELECTCASE RAND:2
		CASE 1
			PRINTFORML 「做饭吗？」
		CASEELSE
			PRINTFORML 「要做饭吗？」
		ENDSELECT
		SELECTCASE RAND:3
		CASE 1
			PRINTFORML 「我也要试试！」
		CASE 2
			IF TALENT:111:思慕 || TALENT:111:恋慕
				PRINTFORML 「我来帮你吧！」
			ELSE
				PRINTFORML 「我来帮你！」
			ENDIF
		CASEELSE
			IF TALENT:111:思慕 || TALENT:111:恋慕
				PRINTFORML 「一起做吧♪」
			ELSE
				PRINTFORML 「让我也来试试！」
			ENDIF
		ENDSELECT
	ENDIF
	PRINTW 
	;野菜を抜く
	;野菜抜き処理は本体側に移動しました
	;主人が抜いてようが表示されるのは仕様ってことで
	IF STRFIND(DISHTASTE, "去掉蔬菜/") >= 0
		PRINTFORML 「哇！这个、有蔬菜……」
		PRINTFORML 「悄悄地丢掉！」
		PRINTW 
	ENDIF
	;野菜入り野菜抜き（？）だといけないので
	IF STRFIND(DISHTASTE, "漢堡") >= 0 && TCVAR:111:356
		PRINTFORML 「哇哇！好像很好吃～！」
		PRINTFORML 「赶紧开吃吧♪开吃吧？可以让我吃吧？？」
	;禁断の料理
	ELSEIF DISHTYPE == 4
		PRINTFORML 「虽然完成了……不过、这是什么？」
		PRINTFORML 「真、真的要吃吗……？」
	ELSE
		IF ABL:111:料理技能 >= 3
			SELECTCASE RAND:4
			CASE 1
				PRINTFORML 「那么～来了♪」
				PRINTFORML 「哇哦！好像很好吃！」
			CASE 2
				PRINTFORML 「让我来干的吧就只是小菜一碟而已啦～）」
				PRINTFORML 「怎么样？做得很好吧？」
			CASE 3
				PRINTFORMW 「做好了！表扬我！」
				PRINTW 　　抚摸抚摸
				PRINTFORML 「嘿嘿♪」
			CASEELSE
				PRINTFORML 「反正！意外的简单」
				PRINTFORML 「我的料理做得好吗？」
			ENDSELECT
		ELSE
			SELECTCASE RAND:4
			CASE 1
				PRINTFORML 「呼、疲惫不堪……」
				PRINTFORML 「做菜真够呛啊……」
			CASE 2
				PRINTFORML 「这样的感觉？」
				PRINTFORML 「不能再偷偷放点别的味道吗？」
			CASE 3
				PRINTFORML 「做好了～！这样就可以了吧！」
				PRINTFORML 「吃了也不会变化了」
			CASEELSE
				PRINTFORML 「咦？完了？」
				PRINTFORML 「不能再偷偷放点别的味道吗？」
			ENDSELECT
		ENDIF
	ENDIF
	WAIT
ENDIF
RETURN 1
