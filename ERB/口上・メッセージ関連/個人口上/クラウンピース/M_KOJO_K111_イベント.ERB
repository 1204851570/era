@M_KOJO_SPEVENT_K111_1(ARG,ARG:1)
;口上主はTARGET
;--------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------
IF LOCAL
	PRINTFORMW 「ねえねえ%CALLNAME:MASTER%、目瞑って」
	PRINTW 
	SETCOLOR 255, 100, 255
	PRINTFORMW ……ちゅ%UNICODE(0x2764)%
	CALL M_KOJO_COLOR_K111
	PRINTL 
	PRINTFORMW 「嘿嘿へ、接吻了呢%UNICODE(0x2764)%」
	IF FIRSTTIME("初吻")
		PRINTFORMW 「我的初吻、给%CALLNAME:MASTER%好了♪」
		IF TALENT:111:恋慕
			CSTR:111:初吻喪失履歴 = %PRINT_DATE_F(DAY)%、%DATENAME_PLACE()%で约会した帰りに大好きな%CALLNAME:MASTER%に初吻を捧げた
		ELSEIF TALENT:111:思慕
			CSTR:111:初吻喪失履歴 = %PRINT_DATE_F(DAY)%、%DATENAME_PLACE()%で约会した帰りに没关注过的%CALLNAME:MASTER%に初吻を捧げた
		ELSE
			CSTR:111:初吻喪失履歴 = %PRINT_DATE_F(DAY)%、%DATENAME_PLACE()%で约会した帰りに%CALLNAME:MASTER%に初吻を捧げた
		ENDIF
	ENDIF
	;キスをキャンセルしたい場合は以下を開放
	;TCVAR:キスキャンセル = 1
ENDIF
RETURN 1

;==================================================
;デート帰りに告白（恋人が付く）
;==================================================
@M_KOJO_SPEVENT_K111_2(ARG,ARG:1)
;口上主はTARGET
;--------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;告白内容
	;この告白内容表示後に受けるかどうかの選択分が表示されます
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 0
		;初回
		IF FIRSTTIME("告白")
			PRINTFORML 「喂、喂%CALLNAME:MASTER%、你、那个……」
			PRINTFORMW 克勞恩皮絲停下脚步、以罕见的紧张态度搭话
			PRINTFORMW 「那个、那个！ %CALLNAME:MASTER%！」
			PRINTFORMW 「我、喜欢你！！很喜欢、真的……超喜欢！！」
			PRINTFORMW 「这是我一直想……、一直都想说出来……」
			PRINTFORMW 「所以、注意到的时候……我就、已经喜欢上%CALLNAME:MASTER%了%UNICODE(0x2764)%」
			PRINTFORMW 「无可奈何的、喜欢上了你………%CALLNAME:MASTER%也喜欢、我吗？」
		;以降
		ELSE
			PRINTFORML 「喂、%CALLNAME:MASTER%……」
			PRINTFORMW 克勞恩皮絲把%CALLNAME:MASTER%叫住、眼神上扬地看了过来
			PRINTFORMW 「%CALLNAME:MASTER%你还也没有和别人交往吧？」
			PRINTFORMW 「那个、之前我也说过……果然我还是不行么？」
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;告白オッケー（恋人が付く）
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
		PRINTFORMW 「真的！？」
		PRINTFORMW 「那么、现在我们是恋人了吧！」
		PRINTFORML 克勞恩皮絲は満脸笑容的跳起来
		PRINTFORMW 就以那样的气势冲到%CALLNAME:MASTER%怀里
		PRINTFORMW 「嘿嘿%UNICODE(0x2764)% 好高兴%UNICODE(0x2764)%」
		PRINTFORMW 「%CALLNAME:MASTER%、喜欢き%UNICODE(0x2764)% 最喜欢了%UNICODE(0x2764)%♪」
		PRINTFORMW 「从今后也、多多指教♪」
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;告白失敗
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
		PRINTFORMW 「是吗……、嗯、没关系的」
		PRINTFORMW 「我、因为能坦率的说出自己的心情、所以很痛快」
		PRINTFORMW 克勞恩皮絲用力的擦拭着快要溢出眼眶的泪水、重新组织起了话语
		PRINTFORMW 「那么我会一直努力直到%CALLNAME:MASTER%喜欢上我为止！」
		PRINTFORMW 「所以、以后也一起玩吧……」
		RETURN 1
	ENDIF

ENDIF
RETURN 1

