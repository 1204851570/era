;※※※※※※※※※※※※※※※※※※※※※※※※※※
;"EVENT"系イベント
;※※※※※※※※※※※※※※※※※※※※※※※※※※
;==================================================
;部屋で遭遇
;==================================================
@M_KOJO_EVENT_K111_1(ARG,ARG:1)
;口上主はTARGET
;--------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;MASTERが入ってきた＆TARGETが入ってきた
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && (ARG == 1 || ARG == 2) && !TCVAR:111:今天有否见面
		;バレンタイン機嫌補正
		SIF DAY:2 == 4 && DAY:3 == 14
			;機嫌補正
			TALENT:111:心情 = MAX(TALENT:111:心情, 0)
		;ハロウィン
		IF DAY:2 == 3 && DAY:3 == 30
			$HALLOWEEN
			PRINTFORMW 「不给糖 ☆ 就 ☆ 捣蛋！！」
			PRINTFORMW 「糖果与点心♪」
			PRINTFORMW 「还是%CALLNAME:MASTER%更喜欢恶作剧呢～？」
			PRINTFORMW 「……两个都不行？」
			TALENT:111:心情 = MAX(TALENT:111:心情, 0)
		;不機嫌
		ELSEIF TALENT:111:心情 == -1 || CFLAG:111:ブチギレ || MARK:111:反発刻印 >= 3
			IF ARG == 1
				PRINTFORMW 「あ、%CALLNAME:MASTER%……」
			ELSEIF ARG == 2
				PRINTFORMW 「ふーんふーん」
			ENDIF
		;12日以上会ってない
		ELSEIF DAY - CFLAG:111:1002 >= 12
			PRINTFORML 「啊、%CALLNAME:MASTER%！」
			;恋人or恋慕
			IF TALENT:111:恋人 || TALENT:111:恋慕
				PRINTFORMW 「最近没有看见你、感觉有点寂寞…不不不、是担心！」
				PRINTFORMW 「喂、只要紧紧地握住你就能好了……吧？」
			ELSE
				PRINTFORMW 「好久不见、还好吗？」
			ENDIF
		;MASTERが入ってきた
		ELSEIF ARG == 1
			PRINTFORML 「啊、%CALLNAME:MASTER%！」
			;？
			IF !RAND:100
				PRINTFORML 「有了！！」
				PRINTFORMW 克勞恩皮絲跑过来的时候狠狠的摔了一跤
			ELSE
				SELECTCASE RAND:4
				CASE 0
					PRINTFORMW 「哦？哦？脸蛋上有饭粒哦！」
				CASE 1
					PRINTFORMW 「今天去哪里玩呢～」
				CASE 2
					PRINTFORMW 「现在有空吗？喂、有空吗？？」
				CASEELSE
					PRINTFORMW 「喂、一起玩吧！」
				ENDSELECT
			ENDIF
		;TARGETが入ってきた
		ELSE
			SELECTCASE RAND:4
			CASE 0
				PRINTFORMW 「哎呀!、去散步了！」
			CASE 1
				PRINTFORMW 「哎呀!、你做了什么吗？」
			CASE 2
				PRINTFORMW 「哎呀!、啊、正在忙着呢？」
			CASEELSE
				PRINTFORMW 「哎呀!、来了来了」
			ENDSELECT
		ENDIF
		;バレンタインチョコ
		IF DAY:2 == 4 && DAY:3 == 14
			$VALENTINE
			SELECTCASE CFLAG:111:好感度
			CASE 1000 TO 3000
				;ブラックサンダー
				PRINTFORMW 「这个是！」
			CASE 3001 TO 5000
				;チョコレート
				PRINTFORMW 「这个是、巧克力！」
			CASE IS > 5000
				IF EXP:111:愛情経験 < 100
					;高級チョコ
					PRINTFORMW 「这个是、情人节～快乐～哦！」
				ELSE
					;手作りチョコ
					PRINTFORMW 「这个是！ 是和主人一起做的！」
					PRINTFORMW 「情人节～快乐☆」
				ENDIF
			ENDSELECT
		;上機嫌
		ELSEIF TALENT:111:心情 == 1 && TALENT:111:恋慕
			PRINTFORMW 「呵呵~今天早上被主人表扬了~♪」
		ENDIF
		GOTO FINISH
	ENDIF
	;-------------------------------------------------
	;TARGETが入ってきた
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------

	;統合しました

	;-------------------------------------------------
	;TARGETが風呂に入ってきて一緒に入る
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	;恥薄いと大体入ってくる
	IF LOCAL:1 && ARG == 3
		PRINTFORML 「啊！%CALLNAME:MASTER%进来了！」
		PRINTFORMW 「嘿嘿\/\/\/ 那一起进去吧\/\/\/」
		GOTO FINISH
	ENDIF
	;-------------------------------------------------
	;TARGETが風呂に入ってきて出て行った
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 4
		PRINTFORML 「啊！%CALLNAME:MASTER%进来了！」
		PRINTFORMW 「对不起啊、打扰你了」
		GOTO FINISH
	ENDIF
	;-------------------------------------------------
	;TARGETが風呂に入ってきて追い出された
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 5
		PRINTFORML 「啊！%CALLNAME:MASTER%进来了！」
		PRINTFORMW 「哼哼♪因为有我、所以%CALLNAME:MASTER%就出现了♪」
		GOTO FINISH
	ENDIF
	;-------------------------------------------------
	;外出先で遭遇した
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 6 && !TCVAR:111:今天有否见面
		;ハロウィン優先
		SIF DAY:2 == 3 && DAY:3 == 30
			GOTO HALLOWEEN
		PRINTFORML 「あ！ %CALLNAME:MASTER%！」
		IF TALENT:111:恋慕
			;月
			IF CFLAG:111:現在位置 >= 1000 && CFLAG:111:現在位置 < 1100
				;恋人
				IF TALENT:111:恋人
					PRINTFORMW 「是来见我的！？」
					PRINTFORMW 「哇ーい♪ %CALLNAME:MASTER%、最喜欢ー%UNICODE(0x2764)%」
				ELSE
					PRINTFORMW 「你来干什么？」
					PRINTFORMW 「难得来一起玩啊ー！」
				ENDIF
			ELSE
				SELECTCASE RAND:4
				CASE 0
					PRINTFORMW 「真没想到在这种地方遇见你！」
				CASE 1
					PRINTFORMW 「喂、一起去什么地方逛逛吧？」
				CASE 2
					PRINTFORMW 「一个人出门？ 那明天咱们一起玩吧～」
				CASEELSE
					PRINTFORMW 「一起玩儿吧♪」
				ENDSELECT
			ENDIF
		ELSE
			PRINTFORMW 「喂、你在干什么呢？」
		ENDIF
		;バレンタイン
		SIF DAY:2 == 4 && DAY:3 == 14
			GOTO VALENTINE
		GOTO FINISH
	ENDIF
	;-------------------------------------------------
	;他キャラとデート中に遭遇した
	;ARG:1にデート相手
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 7 && !TCVAR:111:今天有否见面
		;ハロウィン優先
		SIF DAY:2 == 3 && DAY:3 == 30
			GOTO HALLOWEEN
		PRINTFORML 「あ！ %CALLNAME:MASTER%！」
		IF TALENT:111:恋慕
			;ヘカーティアとデート
			IF FLAG:约会的对象 == 113
				PRINTFORML 「咦、咦主人様……！」
				PRINTFORMW 「嗯、那个、我可以继续待在这里吗……？」
			;純狐とデート
			ELSEIF FLAG:约会的对象 == 112
				PRINTFORML 「唔、友人様……！」
				PRINTFORMW 「嗯、那个、我可以继续待在这里吗……？」
			;妖精とデート
			ELSEIF TALENT:(FLAG:约会的对象):妖精
				PRINTFORMW 「什么啊这家伙、关系很好吗？」
			;それ以外
			ELSE
				PRINTFORMW 「哇、那是那个女孩吗？」
			ENDIF
		ELSE
			;ヘカーティアとデート
			IF FLAG:约会的对象 == 113
				PRINTFORML 「主人様也……！」
				PRINTFORMW 「你们俩在干什么？」
			;純狐とデート
			ELSEIF FLAG:约会的对象 == 112
				PRINTFORML 「友人様也！」
				PRINTFORMW 「你们俩在干什么？」
			ELSE
				PRINTFORMW 「喂、你在干什么呢？」
			ENDIF
		ENDIF
		;バレンタイン
		SIF DAY:2 == 4 && DAY:3 == 14
			GOTO VALENTINE
		GOTO FINISH
	ENDIF
ENDIF
$FINISH
;最後に会った日
CFLAG:111:1002 = DAY
;TCVAR:111:今日会ったか = 1
RETURN 1


;==================================================
;朝起きたとき（おはよう口上）
;==================================================
@M_KOJO_EVENT_K111_2(ARG,ARG:1)
#DIM 分岐
CALL MESSAGE_GOODMONING(ARG,ARG:1)

;口上主はTARGET
;--------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------
IF LOCAL
