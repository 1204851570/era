;==================================================
;5,手淫フェラ
;==================================================
@M_KOJO_MESSAGE_SCOM_K111_5
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_SCOM_K111_5_1
RETURN RESULT
@M_KOJO_MESSAGE_SCOM_K111_5_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
;フェラチオと同じ
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(TFLAG:50 + 500)
		PRINTFORMW 
		RETURN 1
	ENDIF
	IF FLAG:時間停止
		RETURN 1
	ELSEIF NOWEX:MASTER:11
		SELECTCASE RAND:2
		CASE 0
			PRINTFORMW 「嗯~%UNICODE(0x2764)% 嗯~%UNICODE(0x2764)% 呜啊……干劲十足的样子呢」
			PRINTFORMW 「好呀、放到小嘴里吧%UNICODE(0x2764)% 嗯~嗯~%UNICODE(0x2764) * 2% 嗯～～～っっ%UNICODE(0x2764)%」
		CASE 1
			PRINTFORMW 「嗯~%UNICODE(0x2764)% 嗯~%UNICODE(0x2764)% 啊っ、汁液喷出来了……%UNICODE(0x2764)%」
			PRINTFORMW 「差不多要去了？ 那么就这样……哼哼、哼、じゅっ、嗯～～っっ%UNICODE(0x2764)%」
		ENDSELECT
	ELSE
		SELECTCASE RAND:3
		CASE 0
			PRINTFORMW 「刚才、很舒服吗？%UNICODE(0x2764)% 黏黏的%UNICODE(0x2764)%」
			PRINTFORMW 「啊、热热的…慢慢的……一直在磨蹭着%UNICODE(0x2764)%」
		CASE 1
			PRINTFORMW 「啊ー嗯%UNICODE(0x2764)%」
			PRINTFORMW 「慢慢的……磨磨蹭蹭的%UNICODE(0x2764)% ひもひいい？ 慢慢慢慢……%UNICODE(0x2764)%」
		CASE 2
			PRINTFORMW 「嗯好热、ちゅ…じゅるる……%UNICODE(0x2764)%」
			PRINTFORMW 「%CALLNAME:MASTER%のおちんちん、おいひいよ…%UNICODE(0x2764)%」
		ENDSELECT
	ENDIF
	RETURN 1
ENDIF
