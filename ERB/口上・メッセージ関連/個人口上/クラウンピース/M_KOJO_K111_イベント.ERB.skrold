;==================================================
;オナバレ
;==================================================
@M_KOJO_EVENT_K111_26(ARG,ARG:1)

;--------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------
IF LOCAL
	SELECTCASE ARG
		;-------------------------------------------------
		CASE 0
		;見られた瞬間
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1
			IF FIRSTTIME("オナバレ")
				PRINTFORMW 「わ…%CALLNAME:MASTER%……い、いつからそこに……」
				PRINTFORML 「な、なによ！ 別にいいでしょ！ あたいだって女の子なんだから！」
				PRINTFORMW 「それより勝手に入ってこないでよ！」
				TCVAR:111:352 = 1
			ELSEIF TALENT:111:恋慕 && GETBIT(CFLAG:111:既成事実, 1)
				;10日以上会ってない
				IF DAY - CFLAG:111:1002 >= 10
					PRINTFORML 「あ…%CALLNAME:MASTER%……！」
					PRINTFORMW 「これは…だって、その、最近会えなくて、さみしくて……」
					PRINTFORMW 「我慢できなくて……だから、うう……」
					TCVAR:111:352 = 4
				ELSE
					PRINTFORML 「なんだ%CALLNAME:MASTER%か〜、居るなら居るって言ってよ〜〜」
					PRINTL 
					PRINTFORML クラウンピースは下半身を濡らしたまま半開きの眼で堂々と玄関に出てきた
					PRINTFORMW 周囲には熱気とお菓子のような甘い香りが漂っている……
					PRINTL 
					PRINTFORML 「わざわざ会いに来てくれたの〜？ 嬉しい〜♪」
					PRINTFORMW 「何しゅる〜？ デート？ お泊り？ そうだ、一緒にご飯食べようよ〜」
					TCVAR:111:352 = 3
				ENDIF
			ELSE
				PRINTFORMW 「う…%CALLNAME:MASTER%……今いいところだったのに……」
				PRINTFORMW 「じゃなくて！ また覗きにきたの！？ 変態！」
				TCVAR:111:352 = 2
			ENDIF
;			PRINTFORMW 「あーっっ！！ だめっ！ イくッ%UNICODE(0x2764)%」
;			PRINTFORML ……どうやら見られたことに興奮して絶頂してしまったようだ
;			PRINTFORMW しかしクラウンピースは平然と開き直っている
;			PRINTFORML 「なによ！ 別にいいでしょ！」
;			PRINTFORMW 「それより勝手に入ってこないでよ！」
			GOTO FINISH
		ENDIF
		;-------------------------------------------------
		CASE 1
		;何もしない
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1
			PRINTL 
			;初回,恋慕なしor合意まだ
			IF TCVAR:111:352 == 1 || TCVAR:111:352 == 2
				PRINTFORMW 「ばか！ でてけ！」
			;恋慕＋合意
			ELSEIF TCVAR:111:352 == 3
				PRINTFORMW 「ふぇぇ〜？ どうしちゃったの〜？」
			;久しぶり
			ELSEIF TCVAR:111:352 == 4
				PRINTFORMW 「あっ！ 待って…ぁ……」
			ENDIF
			GOTO FINISH
		ENDIF
		;-------------------------------------------------
		CASE 2
		;そのまま続行
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1
			PRINTL 
			;初回,恋慕なしor合意まだ
			IF TCVAR:111:352 == 1 || TCVAR:111:352 == 2
				PRINTFORML 「え、続けるの……？」
				PRINTFORMW 「うん、じゃあ見てて♪」
			;恋慕＋合意
			ELSEIF TCVAR:111:352 == 3
				PRINTFORML 「え、そのままって……続きをするの……？」
				PRINTFORMW 「せっかく%CALLNAME:MASTER%がいるのにぃ……」
				PRINTFORMW 「んもう、それなら見せるだけだからね！」
			;久しぶり
			ELSEIF TCVAR:111:352 == 4
				PRINTFORML 「え、そのままって……続きをするの……？」
				PRINTFORMW 「うう…そんな……生殺しだよぉ……」
				PRINTFORMW 「それじゃあ、あたいのオナニー見てて下さい……」
			ENDIF
;			PRINTFORMW 「え……続けるの……？」
;			PRINTFORMW 「うん、じゃあ見てて♪」
;			PRINTFORMW 「……はあっ%UNICODE(0x2764)% んっ%UNICODE(0x2764)%」
;			PRINTFORMW 「やぁ%UNICODE(0x2764)% %CALLNAME:MASTER%に見られてるのイイかも……%UNICODE(0x2764)%」
			GOTO FINISH
		ENDIF
		;-------------------------------------------------
		CASE 3
		;追い出し
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1
			PRINTL 
			PRINTFORMW 「ばーか！！ 地獄に堕ちろ！」
			GOTO FINISH
		ENDIF
		;-------------------------------------------------
		CASE 4
		;やっちゃう
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1
			PRINTL 
			PRINTFORML 「なにそれ、%CALLNAME:MASTER%も溜まってるんじゃない%UNICODE(0x2764)%」
			PRINTFORMW 「いいよ、一緒にしよ%UNICODE(0x2764)%」
			GOTO FINISH
		ENDIF
		;-------------------------------------------------
		CASE 5
		;事後
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1
			PRINTL 
			PRINTFORML 「はあはあはあ……気持ちよかったぁ……%UNICODE(0x2764)%」
			PRINTFORMW 「ま、またしてあげてもいいんだからねっ！」
			GOTO FINISH
		ENDIF
		;-------------------------------------------------
		CASE 6
		;事後お泊り
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 0
		;-------------------------------------------------
		IF LOCAL:1
			PRINTL 
			PRINTFORML 「」
			PRINTFORMW 「」
			RETURN 1
		ENDIF
	ENDSELECT
ENDIF
$FINISH
;最後に会った日
CFLAG:111:1002 = DAY
TCVAR:111:今日会ったか = 1
RETURN 1


