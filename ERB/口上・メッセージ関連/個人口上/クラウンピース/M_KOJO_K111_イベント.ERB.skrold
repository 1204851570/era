;※※※※※※※※※※※※※※※※※※※※※※※※※※
;"EVENT"系イベント
;※※※※※※※※※※※※※※※※※※※※※※※※※※
;==================================================
;部屋で遭遇
;==================================================
@M_KOJO_EVENT_K111_1(ARG,ARG:1)
;口上主はTARGET
;--------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;MASTERが入ってきた＆TARGETが入ってきた
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && (ARG == 1 || ARG == 2) && !TCVAR:111:今日会ったか
		;バレンタイン機嫌補正
		SIF DAY:2 == 4 && DAY:3 == 14
			;機嫌補正
			TALENT:111:機嫌 = MAX(TALENT:111:機嫌, 0)
		;ハロウィン
		IF DAY:2 == 3 && DAY:3 == 30
			$HALLOWEEN
			PRINTFORMW 「トリック ☆ オア ☆ トリート！！」
			PRINTFORMW 「お菓子ちょーだいっ♪」
			PRINTFORMW 「それとも%CALLNAME:MASTER%はイタズラの方がいいのかな〜？」
			PRINTFORMW 「……どっちもじゃダメ？」
			TALENT:111:機嫌 = MAX(TALENT:111:機嫌, 0)
		;不機嫌
		ELSEIF TALENT:111:機嫌 == -1 || CFLAG:111:ブチギレ || MARK:111:反発刻印 >= 3
			IF ARG == 1
				PRINTFORMW 「あ、%CALLNAME:MASTER%……」
			ELSEIF ARG == 2
				PRINTFORMW 「ふーんふーん」
			ENDIF
		;12日以上会ってない
		ELSEIF DAY - CFLAG:111:1002 >= 12
			PRINTFORML 「あ、%CALLNAME:MASTER%！」
			;恋人or恋慕
			IF TALENT:111:恋人 || TALENT:111:恋慕
				PRINTFORMW 「最近見かけなかったから寂し…じゃなくて、心配だったよ！」
				PRINTFORMW 「ねえ、ぎゅってしていい……？」
			ELSE
				PRINTFORMW 「久しぶりだね、元気してた？」
			ENDIF
		;MASTERが入ってきた
		ELSEIF ARG == 1
			PRINTFORML 「あ、%CALLNAME:MASTER%！」
			;？
			IF !RAND:100
				PRINTFORML 「あたっ！！」
				PRINTFORMW クラウンピースは駆け寄ってこようとして盛大にすっ転んだ
			ELSE
				SELECTCASE RAND:4
				CASE 0
					PRINTFORMW 「およ？ ほっぺにごはんつぶが付いてるよっ！」
				CASE 1
					PRINTFORMW 「今日は何して遊ぼうかな〜」
				CASE 2
					PRINTFORMW 「いまひま？ ねえひま？？」
				CASEELSE
					PRINTFORMW 「ねえねえ遊ぼー！」
				ENDSELECT
			ENDIF
		;TARGETが入ってきた
		ELSE
			SELECTCASE RAND:4
			CASE 0
				PRINTFORMW 「やっほー、お散歩してたの！」
			CASE 1
				PRINTFORMW 「やっほー、なにしてたの？」
			CASE 2
				PRINTFORMW 「やっほー、あ、お取り込み中だった？」
			CASEELSE
				PRINTFORMW 「やっほー、きちゃった」
			ENDSELECT
		ENDIF
		;バレンタインチョコ
		IF DAY:2 == 4 && DAY:3 == 14
			$VALENTINE
			SELECTCASE CFLAG:111:好感度
			CASE 1000 TO 3000
				;ブラックサンダー
				PRINTFORMW 「はいこれ！」
			CASE 3001 TO 5000
				;チョコレート
				PRINTFORMW 「はいこれ、チョコレート！」
			CASE IS > 5000
				IF EXP:111:愛情経験 < 100
					;高級チョコ
					PRINTFORMW 「はいこれ、ハッピ〜バレンタイ〜ン！」
				ELSE
					;手作りチョコ
					PRINTFORMW 「はいこれ！ ご主人様と一緒に作ってきたの！」
					PRINTFORMW 「ハッピ〜バレンタインっ☆」
				ENDIF
			ENDSELECT
		;上機嫌
		ELSEIF TALENT:111:機嫌 == 1 && TALENT:111:恋慕
			PRINTFORMW 「えへへ〜、今朝ご主人様に褒められちゃった〜♪」
		ENDIF
		GOTO FINISH
	ENDIF
	;-------------------------------------------------
	;TARGETが入ってきた
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------

	;統合しました

	;-------------------------------------------------
	;TARGETが風呂に入ってきて一緒に入る
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	;恥薄いと大体入ってくる
	IF LOCAL:1 && ARG == 3
		PRINTFORML 「あ！ %CALLNAME:MASTER%入ってたの！」
		PRINTFORMW 「えへ\/\/\/ じゃあ一緒に入ろっ\/\/\/」
		GOTO FINISH
	ENDIF
	;-------------------------------------------------
	;TARGETが風呂に入ってきて出て行った
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 4
		PRINTFORML 「あ！ %CALLNAME:MASTER%入ってたの！」
		PRINTFORMW 「ごめんねー、お邪魔しましたー」
		GOTO FINISH
	ENDIF
	;-------------------------------------------------
	;TARGETが風呂に入ってきて追い出された
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 5
		PRINTFORML 「あ！ %CALLNAME:MASTER%入ってたの！」
		PRINTFORMW 「ふふーん♪ あたいが入るから%CALLNAME:MASTER%は出てってね♪」
		GOTO FINISH
	ENDIF
	;-------------------------------------------------
	;外出先で遭遇した
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 6 && !TCVAR:111:今日会ったか
		;ハロウィン優先
		SIF DAY:2 == 3 && DAY:3 == 30
			GOTO HALLOWEEN
		PRINTFORML 「あ！ %CALLNAME:MASTER%！」
		IF TALENT:111:恋慕
			;月
			IF CFLAG:111:現在位置 >= 1000 && CFLAG:111:現在位置 < 1100
				;恋人
				IF TALENT:111:恋人
					PRINTFORMW 「会いに来てくれたの！？」
					PRINTFORMW 「わーい♪ %CALLNAME:MASTER%大好きー%UNICODE(0x2764)%」
				ELSE
					PRINTFORMW 「なにしにきたの？」
					PRINTFORMW 「せっかくだし遊ぼうよー！」
				ENDIF
			ELSE
				SELECTCASE RAND:4
				CASE 0
					PRINTFORMW 「こんなところで会うとは奇遇ね！」
				CASE 1
					PRINTFORMW 「ねえねえ、一緒にどこか行かない？」
				CASE 2
					PRINTFORMW 「ひとりでおでかけ？ あたいと遊ぼうよ〜」
				CASEELSE
					PRINTFORMW 「遊ぼっ遊ぼっ♪」
				ENDSELECT
			ENDIF
		ELSE
			PRINTFORMW 「ねえねえ、なにしてるのー？」
		ENDIF
		;バレンタイン
		SIF DAY:2 == 4 && DAY:3 == 14
			GOTO VALENTINE
		GOTO FINISH
	ENDIF
	;-------------------------------------------------
	;他キャラとデート中に遭遇した
	;ARG:1にデート相手
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 7 && !TCVAR:111:今日会ったか
		;ハロウィン優先
		SIF DAY:2 == 3 && DAY:3 == 30
			GOTO HALLOWEEN
		PRINTFORML 「あ！ %CALLNAME:MASTER%！」
		IF TALENT:111:恋慕
			;ヘカーティアとデート
			IF FLAG:デート相手 == 113
				PRINTFORML 「ご、ご主人様……！」
				PRINTFORMW 「えっと、あの、あたいはここに居ても良いですか……？」
			;純狐とデート
			ELSEIF FLAG:デート相手 == 112
				PRINTFORML 「ゆ、友人様……！」
				PRINTFORMW 「えっと、あの、あたいはここに居ても良いですか……？」
			;妖精とデート
			ELSEIF TALENT:(FLAG:デート相手):妖精
				PRINTFORMW 「なによそいつ、仲良いの？」
			;それ以外
			ELSE
				PRINTFORMW 「むっ、だーれその娘？」
			ENDIF
		ELSE
			;ヘカーティアとデート
			IF FLAG:デート相手 == 113
				PRINTFORML 「ご主人様も！」
				PRINTFORMW 「お二人でなにしてたんですか？」
			;純狐とデート
			ELSEIF FLAG:デート相手 == 112
				PRINTFORML 「友人様も！」
				PRINTFORMW 「お二人でなにしてたんですか？」
			ELSE
				PRINTFORMW 「ねえねえ、なにしてるのー？」
			ENDIF
		ENDIF
		;バレンタイン
		SIF DAY:2 == 4 && DAY:3 == 14
			GOTO VALENTINE
		GOTO FINISH
	ENDIF
ENDIF
$FINISH
;最後に会った日
CFLAG:111:1002 = DAY
;TCVAR:111:今日会ったか = 1
RETURN 1


;==================================================
;朝起きたとき（おはよう口上）
;==================================================
@M_KOJO_EVENT_K111_2(ARG,ARG:1)
#DIM 分岐
;口上主はTARGET
;--------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;非同棲かつ相手が先に起きてる
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
		;-------------------------------------------------
		;TARGET私室にMASTERが入っている（恋慕時添い寝中就寝時のみ？
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1 && !ARG:1
			PRINTL 
			PRINTFORMW 「おはよう！ よく眠れた？」
			分岐 = RAND:10
			IF GROUPMATCH(分岐, 1, 2, 3) || TALENT:111:機嫌 == 1 || CFLAG:111:1001 == DAY - 1
;				;昨晩満足,ズレ修正したけどまだ懸念アリ
				IF CFLAG:111:1001 == DAY - 1
					PRINTFORML 「えへ%UNICODE(0x2764)% 昨日はいっぱい愛でてもらっちゃった♪」
					PRINTFORMW 「また今度えっちしようね♪」
					TALENT:111:機嫌 = 1
				ELSE
					PRINTFORMW 「ふふ♪ %CALLNAME:MASTER%の寝顔おもしろかったよ♪」
				ENDIF
			ELSEIF 分岐 == 0 || TALENT:111:機嫌 == -1
				PRINTFORML 「寝言で女の子の名前呼んでたよ？」
				PRINTFORMW 「どういうことかな？ うん？」
				TALENT:111:機嫌 = -1
			ELSEIF GROUPMATCH(分岐, 4, 5, 6)
				PRINTFORML 「ふふ♪ %CALLNAME:MASTER%ったらニヤニヤしながら寝てるんだもの」
				PRINTFORMW 「一体どんな夢を見たのかしら？」
			ELSE
				PRINTFORML 「クスクス……きゃははは♪」
				PRINTFORMW 「ごめんね、お顔に落書きしちゃったの！」
			ENDIF
			RETURN 1
		ENDIF
		;-------------------------------------------------
		;MASTER私室にTARGETが入ってきてる(恋慕無しでも入ってくる可能性有り
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 0
		;-------------------------------------------------
		IF LOCAL:1 && ARG:1 == 1
			PRINTFORMW 
			RETURN 1
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;同棲かつ相手が起きている
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
		PRINTFORMW 
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;同棲かつ相手が寝ている
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 3
		PRINTFORMW 
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;MASTERが起きていて、相手が起きてきた
	;添い寝時TARGET起床はこちら（TARGET衰弱or添い寝中TARGET睡魔負け後そのままMASTERが起き続けた上で相手が起床した場合限定
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 4
		;-------------------------------------------------
		;非同棲
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1 && !ARG:1
			PRINTL 
			PRINTFORML 「おはよ〜〜ふわぁ〜〜ぁ」
			PRINTFORMW 「…………あれ？ なんで%CALLNAME:MASTER%がいるの？」
			PRINTFORMW 「あそっか、あたい寝ちゃったんだっけ」
			RETURN 1
		ENDIF
		;-------------------------------------------------
		;同棲中
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1 && ARG:1 == 1
			PRINTL 
			PRINTFORML 「おはよ〜〜ふわぁ〜〜ぁ」
			IF !RAND:2
				PRINTFORMW 「さーて、今日も一日遊ぶぞ〜」
			ELSE
				PRINTFORMW 「…………もう一眠りしていい？」
			ENDIF
			RETURN 1
		ENDIF
		;両方
		RETURN 1
	ENDIF
	;その他
	PRINTFORML  
ENDIF
RETURN 1

;==================================================
;夜寝るとき（おやすみ口上）
;==================================================
@M_KOJO_EVENT_K111_3(ARG,ARG:1)
;口上主はTARGET
;--------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------

IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;自室に帰る（定時になって寝る）
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1 && !CFLAG:111:うふふ
		;クラウンピースの部屋
		IF CFLAG:111:現在位置 == OMANEKIBEYA()
			PRINTFORMW 「ふわぁ〜ぁ……眠くなってきちゃった」
			PRINTFORMW 「おやすみ%CALLNAME:MASTER%……」
			;泊まれる
			SIF !TCVAR:(CFLAG:MASTER:お招き):連泊禁止 && NEMUKE() < 720
				PRINTFORMW 「……それとも朝まで居てくれる……？」
		;それ以外
		ELSE
			PRINTFORMW 「ふわぁ〜ぁ……眠くなってきちゃった」
			PRINTFORMW 「バイバイ%CALLNAME:MASTER%、またね」
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;眠そうにしている
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
		PRINTFORMW 「ふわぁ〜ぁ……眠いや……」
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;睡眠中（起きていないので会話というよりは寝言？）
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 3
		SELECTCASE RAND:3
		CASE 0
			PRINTFORMW 「すぅ……すぅ……」
		CASE 1
			PRINTFORMW 「ん〜…むにゃむにゃ……」
		CASE 2
			PRINTFORMW 「いっつるなてぃっくた〜いむ……」
		ENDSELECT
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;イタズラ中
	;記入チェック（=0, 非表示、1, 表示）
	;LOCAL:1 = 0
	;-------------------------------------------------
	;IF LOCAL:1 && ARG == 4
		;PRINTFORMW 
		;RETURN 1
	;ENDIF
	;-------------------------------------------------
	;衰弱中
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 5
		PRINTFORMW 「はあはあはあ……」
		PRINTFORMW 「ごめんね%CALLNAME:MASTER%、疲れちゃった…おやすみ……」
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;MASTERがダウン（部屋まで運ばれる）
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 6
		PRINTFORMW 「あーらら、%CALLNAME:MASTER%大丈夫？」
		PRINTFORML 「んーと、どうしよう……」
		PRINTFORMW 「%CALLNAME:MASTER%の部屋に運べばいいかな」
		PRINTFORMW 「んしょっ！ お、重たいー！」
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;TARGETが自室まで戻ってきた
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 7
		;-------------------------------------------------
		;そのまま着替えて寝た
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1 && ARG:1 == 0
			RETURN 1
		ENDIF
		;-------------------------------------------------
		;部屋から追い出された
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1 && ARG:1 == 1
			RETURN 1
		ENDIF
	ENDIF
	;-------------------------------------------------
	;非在住TARGETがMAIN_MAPで休憩
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 8
		PRINTFORMW 
		RETURN 1
	ENDIF
	;その他
	PRINTFORML  
ENDIF
RETURN 1

