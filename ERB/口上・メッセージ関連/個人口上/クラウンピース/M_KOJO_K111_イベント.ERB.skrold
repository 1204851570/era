			PRINTFORML 「こらこら、%CALLNAME:MASTER%はあたいのオモチャなんだから、そんなことしたって無駄だよ」
			PRINTFORMW 「オモチャはオモチャらしく愉快な声で鳴いていなさい！」
			RETURN 1
		CASE 2
		;欲求不満が高くて抜け出せなかった
			SELECTCASE RAND:3
			CASE 0
				PRINTFORMW 「だぁ〜めぇ%UNICODE(0x2764)% まだまだこれからだよ♪」
			CASE 1
				PRINTFORML 「ちょっと暴れないで！」
				PRINTFORMW 「もっとあたいにゴホーシしなさい%UNICODE(0x2764)%」
			CASEELSE
				PRINTFORMW 「あー！ 待って待って！ だめぇ！ もっとするのー！」
			ENDSELECT
			RETURN 1
		ENDSELECT
	ENDIF
	;その他
	PRINTFORML  
ENDIF
RETURN 1

