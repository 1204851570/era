			;そんなこといいつつ好き勝手しちゃいます
		;危険日
		ELSEIF CFLAG:111:生理周期 == 7 && !TALENT:111:妊娠 && CFLAG:111:生解禁 == 2
			PRINTFORML 
			PRINTFORMW （んふふ……%UNICODE(0x2764)% 狂わせちゃおっかなー♪）
			;ちょっとヤンデレかも
		ENDIF
		TCVAR:111:351 = 1
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;なだめられた（終了時）
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
		SELECTCASE ARG:1
		CASE 0
			;抜け出した
			IF TALENT:111:恋慕
				;妊娠中
				IF TALENT:111:妊娠 && CFLAG:111:妊娠自覚状態 == 1
					PRINTFORML 「ふぇ……、まぁしかたないか……」
					PRINTFORMW 「今日は我慢する……」
