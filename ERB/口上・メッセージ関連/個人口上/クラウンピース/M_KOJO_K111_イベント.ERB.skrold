						PRINTFORMW 「って、何でそっちがのいいみたいな顔してんの！？ ヘンタイ！」
					ELSE
						PRINTFORML 「え？ パンツ？」
						PRINTFORMW 「あげたらこのあと何履けばいいの？」
						PRINTFORMW 「まぁいいや、%CALLNAME:MASTER%が欲しいならあげる%UNICODE(0x2764)%」
					ENDIF
					RETURN 1
				ENDIF
				;-------------------------------------------------
				CASE 2
				;OK（渋々
				;記入チェック（=0, 非表示、1, 表示）
				LOCAL:1 = 1
				;-------------------------------------------------
				IF LOCAL:1
					IF TCVAR:111:350
						PRINTFORML 「えー……今日はちょっと……」
						PRINTFORMW 「その、汗かいちゃったし……」
						PRINTFORMW 「って、何でそっちがのいいみたいな顔してんの！？ ヘンタイ！」
					ELSE
						PRINTFORML 「え？ パンツ？」
						PRINTFORMW 「そんなの貰ってどうするの……？」
						PRINTFORMW 「まぁいいや、%CALLNAME:MASTER%が欲しいならあげるけど……」
					ENDIF
					RETURN 1
				ENDIF
				;-------------------------------------------------
				CASE 3
				;嫌です
				;記入チェック（=0, 非表示、1, 表示）
				LOCAL:1 = 1
				;-------------------------------------------------
				IF LOCAL:1
					PRINTFORML 「え？ パンツ？」
					PRINTFORMW 「やだよ！ きもい！」
					RETURN 1
				ENDIF
				RETURN 1
			ENDSELECT
		ENDIF
	ENDSELECT
ENDIF
RETURN 1

