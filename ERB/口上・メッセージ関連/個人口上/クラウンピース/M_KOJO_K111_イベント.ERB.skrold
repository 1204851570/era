;相手からデートのお誘い
;==================================================
@M_KOJO_EVENT_K111_27(ARG,ARG:1)
;口上主はLOCAL
;ARG:1は目的地
;--------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------
IF LOCAL
	SELECTCASE ARG
	CASE 0
		;-------------------------------------------------
		;誘い文句
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1
			PRINTFORML 「ねえねえ%CALLNAME:MASTER%、おでかけしよ♪」
			SELECTCASE STR:(5000 + ARG:1)
			CASE "博麗神社"
				PRINTFORMW 「博麗神社に行きたいなぁ〜♪」
			CASE "命蓮寺"
				PRINTFORMW 「命蓮寺に行きたいなぁ〜♪」
			CASE "人間の里"
				PRINTFORMW 「里でデートしよっ！ デート！」
			CASE "霧の湖〜紅魔館"
				PRINTFORMW 「霧の湖に行きたいなぁ〜♪」
			CASE "迷いの竹林"
				PRINTFORMW 「迷いの竹林に行きたいなぁ〜♪」
			CASE "魔法の森"
				PRINTFORMW 「魔法の森に行きたいなぁ〜♪」
			CASE "三途の川〜冥界"
				PRINTFORML 「三途の川、一緒に渡ろ！」
				PRINTFORMW 「大丈夫よ〜%CALLNAME:MASTER%とならあたい地獄に堕とされたって〜」
			CASE "妖怪の山 (山麓)"
				PRINTFORMW 「妖怪の山に行きたいなぁ〜♪」
			CASE "妖怪の山 (山頂)"
				PRINTFORMW 「山登りがしたいなぁ〜♪」
			CASE "地底"
				PRINTFORMW 「地底世界に行ってみたいなぁ〜♪」
			CASE "夢の世界〜月"
				PRINTFORMW 「月に行こっ♪ 遠いと思う？ここからな近いのよ！」
			ENDSELECT
			RETURN 1
		ENDIF
	CASE 1
		;-------------------------------------------------
		;受けた
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1
			PRINTFORMW 「わーい♪ %CALLNAME:MASTER%やさしー♪」
			RETURN 1
		ENDIF
	CASE 2
		;-------------------------------------------------
		;断った
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1
			PRINTFORML 「ぶー、いじわる」
			PRINTFORMW 「ま、いっか♪ じゃあ今度ね♪」
			RETURN 1
		ENDIF
	CASE 3
		;-------------------------------------------------
		;部屋に誘われた
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1
			PRINTFORML 「ねえねえ、今日はあたいのうちに来ない？」
			IF FIRSTTIME("お招き", 1, 111)
				PRINTFORML 「今日は両親がいないのよ！」
				PRINTFORMW 「……いつもいないけど」
			ELSE
				SELECTCASE RAND:3
				CASE 0
					PRINTFORMW 「%CALLNAME:MASTER%に見せたいものがあるの♪」
				CASE 1
					PRINTFORMW 「%CALLNAME:MASTER%の手料理が食べたいなー、なーんて」
				CASEELSE
					PRINTFORMW 「えっと、その、二人っきりがいいなぁって……」
				ENDSELECT
			ENDIF
			RETURN 1
		ENDIF
	CASE 4
		;-------------------------------------------------
		;部屋に誘われたのを受けた
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1
			IF FIRSTTIME("お招き", 0, 111)
				PRINTFORML 「ほんと！？ わーい♪」
				PRINTFORMW 「ちょっと散らかってるかもしれないけど、きっと%CALLNAME:MASTER%なら気に入るわ♪」
			ELSE
				;晴れ
				IF TIME:5 <= 1
					PRINTFORML 「やったー！」
					PRINTFORML 「じゃあ手を繋いで！ ひとっ飛びで行くよー！」
					;TIME += -1 * TIME_REQUIRED(GET_MAPID(CFLAG:MASTER:現在位置), 10) / 2
					;遠いから時間短縮してあげようと思ったけど直後に出る地の文の表示と齟齬が出るのでやめ。-X分ってのは変だよね…
				ELSE
					PRINTFORML 「あーよかった！」
					PRINTFORMW 「こうなると思って部屋を片付けておいたのよ♪」
					;とか言ってるけど部屋の汚れは変化しません（ここではできない）
					;本人は精一杯部屋を片付けたつもりでいるのです！
				ENDIF
			ENDIF
			RETURN 1
		ENDIF
	CASE 5
		;-------------------------------------------------
		;部屋に誘われたのを断った
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1
			PRINTFORML 「そっか……」
			PRINTFORMW 「やっぱりお月さまは遠いものね……」
			RETURN 1
		ENDIF
	ENDSELECT
ENDIF
RETURN 1

