		PRINTFORMW 「ごめんね%CALLNAME:MASTER%%UNICODE(0x2764)%、もういいよ♪」
		TCVAR:111:351 = 0
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
	;満足して解放
