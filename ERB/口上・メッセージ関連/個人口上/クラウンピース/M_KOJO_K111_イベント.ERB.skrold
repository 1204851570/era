					CASEELSE
						PRINTFORML 「ふぇ……、そんなぁ……」
						PRINTFORMW 「まだまだこれからだったのにー！」
					ENDSELECT
				ENDIF
			ELSE
				PRINTFORML 「ふぇ……、そんなぁ……」
				PRINTFORMW 「いいじゃない、減るもんじゃないんだからー！」
			ENDIF
		CASE 1
		;抱き着きモード満足終了
			RETURN 1
		CASE 2
		;抱き着きモード強制終了
			RETURN 1
		ENDSELECT
		TCVAR:111:351 = 0
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;抜け出す失敗
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 3
		SELECTCASE ARG:1
		CASE 1
