@M_KOJO_MESSAGE_COM_K65_20_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORMW 
		RETURN 1
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMW 
		RETURN 1
	;Ｖ挿入中
	ELSEIF TEQUIP:50 == PLAYER
		;こなれてない
		IF ABL:Ｖ感覚 < 2
			IF TCVAR:発情
				IF TALENT:恋慕 && RAND:2
					CALL HPH_PRINT,@"「%K65_AEGI()%」","L"
					CALL HPH_PRINT,@"「んぅ…%CALLNAME:MASTER%さん、もっと…%K65_AEGI("控えめ",2)%HPH」","L"
					PRINTFORMW 椛は媚びるように身体を揺らし的同时、%CALLNAME:MASTER%の唇を求めた…
				ELSE
					CALL HPH_PRINT,@"「%K65_AEGI()%」","L"
					CALL HPH_PRINT,@"「%K65_AEGI("控えめ",2)%、ぷぁっ…%CALLNAME:MASTER%さん…HPH」","L"
					PRINTFORMW 椛は瞳を潤ませながら%CALLNAME:MASTER%の顔を覗き込んでいる…
				ENDIF
			ELSE
				CALL HPH_PRINT,@"「%K65_AEGI("控えめ")%」","L"
				CALL HPH_PRINT,@"「%K65_AEGI("控えめ",1)%、ぷぁ…%CALLNAME:MASTER%さん…」","L"
				IF TALENT:恋慕 && RAND:2
					PRINTFORMW 「温柔的してくださるんですね…嬉しい、です…」
				ELSE
					PRINTFORMW 「わ、私は…大丈夫、です、から……動いて、ください…」
				ENDIF
			ENDIF
			RETURN 1
		;イキすぎ
		ELSEIF EX:Ｖ絶頂 > 15 && BASE:体力 < MAXBASE:体力/2 && BASE:気力 <= 0
			;SELECTCASE RAND:2
				;CASE 0
					CALL HPH_PRINT,@"「はぁっ…ん…%K65_AEGI("控えめ")%」","L"
					PRINTFORML 椛は蕩け切った表情で%CALLNAME:MASTER%の接吻と阴茎を受け入れている…
					CALL HPH_PRINT,@"「%K65_AEGI()%」","L"
					PRINTFORMW 互相用温もりを交換しあい的同时、二人は片刻后幸せに満ちた時間を堪能した…
				;CASE 1
			;ENDSELECT
			RETURN 1
		;挿入中かつ発情
		ELSEIF TCVAR:発情
			;後背位、背面座位でない
			IF !GROUPMATCH(TEQUIP:体位２,2,5)
				SELECTCASE RAND:2
					CASE 0
						CALL HPH_PRINT,@"「%K65_AEGI()%」","W"
					CASE 1
						CALL HPH_PRINT,@"「%K65_AEGI()%」","L"
						CALL HPH_PRINT,@"「んっ…ふふっHPH…%K65_AEGI("控えめ")%」","L"
						PRINTFORML 舌を絡め合う濃厚的接吻が終わると、今度は顔じゅうに接吻の雨が降ってきた。
						PRINTFORMW 発情した椛の甘やかな唾液の香りに鼻腔を犯され、陰茎が硬さを増していく。
						CALL HPH_PRINT,@"「んんっHPHんっ…HPH…%K65_AEGI("控えめ")%」","L" 
						PRINTFORMW 椛は%CALLNAME:MASTER%の反応に気を良くしたのか、一層热情地接吻を繰り返すのだった…
				ENDSELECT
			;後背位or背面座位
			ELSE
				SELECTCASE RAND:2
					CASE 0
						CALL HPH_PRINT,@"「%K65_AEGI()%」","W"
					CASE 1
						CALL HPH_PRINT,@"「%K65_AEGI(,4)%…～～～っHPH」","L"
						PRINTFORML 最奥をぐりぐりと押しつぶしながら接吻をすると、膣壁がキュッと収縮して
						PRINTFORML %CALLNAME:MASTER%の射精欲を煽ってくる。
						CALL HPH_PRINT,@"「んうぅっHPH%K65_AEGI()%」","W"
				ENDSELECT
			ENDIF 
		;挿入中かつ非発情
		ELSE
			;後背位、背面座位でない
			IF TEQUIP:体位２ != 2 && TEQUIP:体位２ != 5
				SELECTCASE RAND:2
					CASE 0
						CALL HPH_PRINT,@"「%K65_AEGI()%」","W"
					CASE 1
							CALL HPH_PRINT,@"「%K65_AEGI(,2)%…%CALLNAME:MASTER%さん…HPH」","L"
							PRINTFORML 椛は%CALLNAME:MASTER%に抱き着き、舌を絡めながら\@ TALENT:恋慕&&RAND:2 ? 幸せそうな吐息を漏らし # 目を細め \@ている…
							CALL HPH_PRINT,@"「%K65_AEGI("控えめ",2)%…%K65_AEGI(,3)%」","W"
				ENDSELECT
			;後背位or背面座位
			ELSE
				SELECTCASE RAND:2
					CASE 0
						CALL HPH_PRINT,@"「%K65_AEGI()%」","W"
					CASE 1
						CALL HPH_PRINT,@"「はあっ、あっ…%K65_AEGI(,2)%」","L"
						PRINTFORML %CALLNAME:MASTER%は小只的椛に覆いかぶさるように腰を折り曲げ、
						PRINTFORML 彼女の狭窄膣内をかき混ぜながら接吻を交わした…
						CALL HPH_PRINT,@"「%K65_AEGI("控えめ",2)%…%K65_AEGI(,3)%」","L"
				ENDSELECT
			ENDIF
		ENDIF
		RETURN 1
	;通常
	ELSE
		IF TCVAR:発情
			IF TALENT:恋慕 && RAND:2
				SELECTCASE RAND:2
					CASE 0
						CALL HPH_PRINT,@"「%K65_AEGI()%」","L"
						PRINTFORML 椛は%CALLNAME:MASTER%の頬を両手で押さえつけ、一心不乱に舌を絡ませている…
						CALL HPH_PRINT,@"「んむ、ふーっ…%CALLNAME:MASTER%さん…HPH%K65_AEGI(,3)%」","W"
						PRINTFORML ……顔を離そうとしても、捕らえられた頬にぐっと圧力が加わって動きを制される。
						PRINTFORML これはもはや接吻というよりは、捕食と言った方が近いかもしれない。
						CALL HPH_PRINT,@"「…%K65_AEGI(,2)%…っHPH…%K65_AEGI(,2)%」","W"
					CASE 1
						CALL HPH_PRINT,@"「%K65_AEGI(,2)%…ふーっHPH…%K65_AEGI(,2)%」","L"
						PRINTFORML 椛は息を荒らげ的同时、%CALLNAME:MASTER%の唾液を求めている…
				ENDSELECT
			ELSE
				SELECTCASE RAND:2
					CASE 0
						CALL HPH_PRINT,@"「%K65_AEGI()%」", "W"
					CASE 1
						CALL HPH_PRINT,@"「ん、ちゅ…%K65_AEGI("控えめ,2")%…%K65_AEGI(",2")%」","L"
						CALL HPH_PRINT,@"「%CALLNAME:MASTER%さん、もっと…HPH%K65_AEGI("控えめ,4")%」","W"
				ENDSELECT
			ENDIF
		ELSE
			IF TALENT:恋慕 && RAND:2
				SELECTCASE RAND:2
					CASE 0
						CALL HPH_PRINT,@"「%K65_AEGI(,3)%」", "L"
						PRINTFORML 「っ…あの、あんまり…見ないでください…」
						PRINTFORMW 「きっといま私、すっごくだらしない顔してますから…」
					CASE 1
						CALL HPH_PRINT,@"「%K65_AEGI()%」", "L"
						PRINTFORML 「ふふ。接吻は\@ EXP:接吻経験>20 ? 何度も # 何度か \@してるはずなのに、やっぱりいつもドキドキしちゃいます」
						CALL HPH_PRINT,@"「……はい。もう、一回…ん、%K65_AEGI("控えめ",2)%」","L"
						PRINTFORMW %CALLNAME:MASTER%と椛は抱きしめあい、互相用鼓動を感じながら慢慢地接吻をした…
				ENDSELECT
			ELSE
				SELECTCASE RAND:2
					CASE 0
						CALL HPH_PRINT,@"「%K65_AEGI()%」", "W"
					CASE 1
						CALL HPH_PRINT,@"「%K65_AEGI(,2)%…んっ…」", "L"
						PRINTFORMW 接吻を交わしながら指街道の良い髪を撫でると、椛は\@ TALENT:恋慕 && RAND:2 ? トロンとした目で見つめてきた # 开心地微笑んだ \@。
				ENDSELECT
			ENDIF
		ENDIF
		RETURN 1
	ENDIF
ENDIF
RETURN 1


