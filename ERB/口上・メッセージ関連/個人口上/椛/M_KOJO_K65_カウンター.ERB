			CASE 0
				CALL HPH_PRINT,@"「あは…HPH乳頭、気持ちいいです…HPH」", "L"
			CASE 1
				CALL HPH_PRINT,@"「嗯っ…%CALLNAME:MASTER%桑、可以再更多得抚摸我吗…HPH」", "L"
