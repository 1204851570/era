				CALL HPH_PRINT,@"「私は、すっごく興奮してます…HPH」", "L"
		ENDSELECT
	ELSEIF TALENT:胸圍 == 0
		SELECTCASE RAND:3
			CASE 0
				CALL HPH_PRINT,@"「あは…HPH乳頭、気持ちいいです…HPH」", "L"
			CASE 1
				CALL HPH_PRINT,@"「嗯っ…这里还想要更多…HPH」", "L"
				PRINTFORML 硬く尖った乳頭を%CALLNAME:MASTER%の手のひらで押しつぶし的同时、椛は熱い吐息を漏らしている…
			CASE 2
				CALL HPH_PRINT,@"「あふHPHはぁHPHえへ…%CALLNAME:MASTER%さんも、興奮してくれてますか…HPH」", "L"
		ENDSELECT
	;ひんにゅー以下
	ELSE
		SELECTCASE RAND:3
			CASE 0
				CALL HPH_PRINT,@"「あは…HPH乳頭、気持ちいいです…HPH」", "L"
			CASE 1
				CALL HPH_PRINT,@"「嗯っ…这里还想要更多…HPH」", "L"
				PRINTFORML 硬く尖った乳頭を%CALLNAME:MASTER%の手のひらで押しつぶし的同时、椛は熱い吐息を漏らしている…
			CASE 2
				CALL HPH_PRINT,@"「哈っ、呜…HPH%CALLNAME:MASTER%的手、でぇっ…HPH」", "L"
				PRINTFORML 椛は上体を突き出し、自慰をするかのように%CALLNAME:MASTER%の手のひらへ单薄的胸を擦りつけている…
		ENDSELECT
	ENDIF
ELSE
	;きょにゅー以上
	IF TALENT:胸圍 > 0
		SELECTCASE RAND:4
			CASE 0
				PRINTFORML 「柔らかいですか？えへ、%CALLNAME:MASTER%さんに喜んでもらえるなら嬉しいです…」
			CASE 1
				CALL HPH_PRINT,@"「嗯っ…这里还想要更多…HPH」", "L"
				PRINTFORML %CALLNAME:MASTER%の指が、小只的体に釣り合わないほどの巨乳に沈み込む…
			CASE 2
				CALL HPH_PRINT,@"「ふぅ、ん…HPHえへへ、大きいでしょう…？」", "L"
		ENDSELECT
	ELSEIF TALENT:胸圍 == 0
		SELECTCASE RAND:3
			CASE 0
				CALL HPH_PRINT,@"「ふふ、%CALLNAME:MASTER%さんの手、好きです…HPH」", "L"
			CASE 1
				CALL HPH_PRINT,@"「嗯っ…这里还想要更多…HPH」", "L"
				PRINTFORML 体格の割にふっくらとした椛の胸は、%CALLNAME:MASTER%の指に従ってふにふにと形を変えている…
			CASE 2
				CALL HPH_PRINT,@"「っはぁHPHそんなふうにやさしく触られると、せつなくなっちゃいます…」", "L"
		ENDSELECT
	;ひんにゅー以下
	ELSE
		SELECTCASE RAND:3
			CASE 0
				PRINTFORML 「实在没什么自信...…」
				PRINTFORML 「%CALLNAME:MASTER%さんに触れてもらいたくて、せつないんです…」
			CASE 1
				CALL HPH_PRINT,@"「んっ…もっとたくさん、可愛がってください…HPH」", "L"
			CASE 2
				PRINTFORML 掌に押し付けられる单薄的胸の奥で、椛の心臓がトクトクと早鐘を打っているのが感じられる…
		ENDSELECT
	ENDIF
ENDIF
RETURN 1

