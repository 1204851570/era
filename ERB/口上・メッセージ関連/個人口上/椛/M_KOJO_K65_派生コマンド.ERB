;==================================================
;12,パイズリフェラ
;==================================================
@M_KOJO_MESSAGE_SCOM_K65_12
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_SCOM_K65_12_1
RETURN 1
@M_KOJO_MESSAGE_SCOM_K65_12_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		IF TCVAR:発情
			PRINTFORML 「…お口と胸部で一緒に…？」
			CALL HPH_PRINT,@"「あは、%CALLNAME:MASTER%さんのえっち…HPH」","L"
			PRINTFORML 椛は%CALLNAME:MASTER%の反応を上目づかいに伺い的同时、慢慢地阴茎を胸の間に収めた。
			PRINTFORMW 滑らかな肌が敏感な部分へむにゅむにゅと絡みつき、実に良い眺めだ。
			CALL HPH_PRINT,@"「……HPH%K65_AEGI("控えめ",4)%」","L"
			PRINTFORMW %CALLNAME:MASTER%の反応に気を良くした椛は蠱惑的な笑みを浮かべ、亀頭に接吻を繰り返した…
		ELSE
			PRINTFORML 「…挟んだまま、舐めるんですか…？」
			PRINTFORMW 「えっと、わかりました、やってみます…」
			PRINTFORML 椛は%CALLNAME:MASTER%の顔を覗き込み、慢慢地阴茎を胸の間に収めた。
			PRINTFORMW 滑らかな肌が敏感な部分へむにゅむにゅと絡みつき、実に良い眺めだ。
			CALL HPH_PRINT,@"「ん…これで、いいみたいですね…%K65_AEGI("控えめ",3)%」","L"
			PRINTFORMW 椛は%CALLNAME:MASTER%の反応を上目づかいに伺い的同时、やや遠慮がちに亀頭へ接吻し始めた…
		ENDIF
		RETURN 1
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMW 
		RETURN 1
	;技量不足
	ELSEIF ABL:TARGET:胸 < 2
		IF TCVAR:発情
			SELECTCASE RAND:2
				CASE 0
					PRINTFORML 「んん…っ、んっ？んむぅ…」
					PRINTFORML 椛は%CALLNAME:MASTER%の亀頭に吸い付こうとしているが、
					PRINTFORMW 胸の間で跳ねまわるソレを中々捕らえられず、不満そうに撅着嘴巴いる…
				CASE 1
					PRINTFORML 「んしょっ、くっ、ふぁぅ…っ」
					PRINTFORML 椛はもどかしげに阴茎を胸へと擦りつけるが、彼女が焦るほどに阴茎の扱いが雑になり、
					PRINTFORMW かえって%CALLNAME:MASTER%は絶頂から遠のいてしまう…
			ENDSELECT
		ELSE
			SELECTCASE RAND:2
				CASE 0
					PRINTFORML 「んしょ、…ん、ちゅ、ちゅぅっ……」
					PRINTFORML 椛は胸を阴茎に擦りつけたり、亀頭に接吻してみたりと頑張っている。
					PRINTFORMW 申し訳ないくらい頑張っているのは分かるのだが…
				CASE 1
					PRINTFORMW 「これ、結構難しいんですね……」
			ENDSELECT
		ENDIF
	;発情
	ELSEIF TCVAR:発情
		;これでMASTERが射精する
		IF NOWEX:MASTER:11
			IF ABL:精液中毒 > 3 && RAND:2
				SELECTCASE RAND:3
					CASE 0
						CALLFORM M_KOJO_MESSAGE_COM_K65_{SELECTCOM}_1
					CASE 1
						CALL HPH_PRINT,@"「っ…、じゅるるっHPH%K65_AEGI("口交音",4)%」","L"
						CALL HPH_PRINT,@"（くるっ…HPH%CALLNAME:MASTER%さんの%TEXTR("とろとろ/えろえろ/")%%TEXTR("せーえき/せーし/")%HPH）","W"
					CASE 2
						CALL HPH_PRINT,@"「%K65_AEGI("口交音")%」","L"
						PRINTFORML %CALLNAME:MASTER%の絶頂を予感して、椛は胸での圧迫をさらに強めた。
						PRINTFORMW ぷにぷにとした乳肉が陰茎にピッチリと密着し、逃げ場のない快楽が襲ってくる。
						CALL HPH_PRINT,@"「ふっHPHふーっHPH%K65_AEGI("口交音",3)%…んんっ…HPH」","L"
						PRINTFORML 椛は舌を尖らせ、精嚢からせり上がってくるモノを掘り返すかのように鈴口を舐め上げる。
						PRINTFORMW %CALLNAME:MASTER%は激烈な快感に身体を強張らせ的同时、椛の口腔へ充分熱泥を吐き出した…
				ENDSELECT
			ELSE
				SELECTCASE RAND:2
					CASE 0
						CALLFORM M_KOJO_MESSAGE_COM_K65_{SELECTCOM}_1
					CASE 1
						CALL HPH_PRINT,@"「あっ…HPH%CALLNAME:MASTER%さん、イッちゃいそうなんですか…？」","L"
						CALL HPH_PRINT,@"「…おちんちんにちゅーしててあげますから、このまま胸部でイッてくださいね…HPH」","W"
				ENDSELECT
			ENDIF
		;勃起しきってない
		ELSEIF BASE:MASTER:勃起 < 1000
			;SELECTCASE RAND:2
			;	CASE 0
					CALLFORM M_KOJO_MESSAGE_COM_K65_{SELECTCOM}_1
			;	CASE 1
			;		
			;ENDSELECT
		ELSE
			SELECTCASE RAND:4
				CASE 0
					CALLFORM M_KOJO_MESSAGE_COM_K65_82_1
				CASE 1
					CALL HPH_PRINT,@"「%K65_AEGI("口交音")%」","L"
					PRINTFORMW 柔らかな乳房に阴茎を根元から擦り上げられ、竿先に向かって強烈な快感がせり上がる。
					CALL HPH_PRINT,@"「ふっHPHんんっ…HPH%K65_AEGI("口交音")%」","L"
					PRINTFORMW 昇り上がる快楽は、糖玉を舐め溶かすような舌遣いで亀頭全体へと塗り広げられていく。
					CALL HPH_PRINT,@"「んっHPHあは…おちんちんのニオイ凄くて、ボーッとしてきちゃいます…HPH」","L"
					PRINTFORMW 熱を帯びたような椛の声に、%CALLNAME:MASTER%の思考までも溶かされていくようだった…
				CASE 2
					CALL HPH_PRINT,@"「%K65_AEGI("控えめ")%」","L"
					PRINTFORMW 椛は小気味良い音を立てて、胸の谷間から覗く亀頭へリズムよく接吻を繰り返している…
				CASE 3
					CALL HPH_PRINT,@"「%K65_AEGI("口交音")%」","L"
					CALL HPH_PRINT,@"「はあHPHんっ…HPH%K65_AEGI("口交音",3)%」","L"
					CALL HPH_PRINT,@"「ん、%CALLNAME:MASTER%さん…%K65_AEGI("控えめ",2)%…もっと、おつゆ、飲ませてください…HPH」","L"
			ENDSELECT
		ENDIF
	;非発情
	ELSE
		;これでMASTERが射精する
		IF NOWEX:MASTER:11
			IF ABL:精液中毒 > 3 && RAND:2
				SELECTCASE RAND:2
					CASE 0
						CALLFORM M_KOJO_MESSAGE_COM_K65_{SELECTCOM}_1
					CASE 1
						CALL HPH_PRINT,@"「%K65_AEGI("口交音")%」","L"
						PRINTFORML 搾精に夢中になっているのか、椛は言葉を発することもなく%CALLNAME:MASTER%の阴茎にしゃぶりついている。
						PRINTFORMW こぼれ落ちた唾液で滑りのよくなったモチモチの肌が、竿の根元から先端までにゅるにゅると絡みついて気持ち良い。
						PRINTFORML 膨れ上がる快楽が頂点に向かう瞬間、椛はパンパンに膨らんだモノを温かな口腔内の奥まで深々と迎え入れた…
						CALL HPH_PRINT,@"「%K65_AEGI("口交音",3)%…んっ…んんっ…HPH」","W"
						PRINTFORMW 限界まで濃くなった牡臭を口いっぱいに含んだ椛は、自らも達したかのようにぶるっと震えた…
				ENDSELECT
			ELSE
				SELECTCASE RAND:3
					CASE 0
						CALLFORM M_KOJO_MESSAGE_COM_K65_{SELECTCOM}_1
					CASE 1
						PRINTFORML 「…あ、イっちゃいそうなんですね、%CALLNAME:MASTER%さん…」
						CALL HPH_PRINT,@"「それじゃあ、このまま胸部で擦り的同时、舐めてあげますね…HPH」","L" 
						CALL HPH_PRINT,@"「ん、ちゅ…HPH%K65_AEGI("口交音")%」","W"
					CASE 2
						PRINTFORML 「…ん、ぷぁ…胸部、気持ちいいですか…？」
						CALL HPH_PRINT,@"「ふふ、我慢しなくていいですから、いっぱい射精してくださいね…HPH」","L" 
						CALL HPH_PRINT,@"「ちゅむ…%K65_AEGI("口交音")%…HPH」","W"
				ENDSELECT
			ENDIF
		;勃起しきってない
		ELSEIF BASE:MASTER:勃起 < 1000
			;SELECTCASE RAND:2
			;	CASE 0
					CALLFORM M_KOJO_MESSAGE_COM_K65_{SELECTCOM}_1
			;	CASE 1
			;		
			;ENDSELECT
		ELSE
			SELECTCASE RAND:4
				CASE 0
					CALLFORM M_KOJO_MESSAGE_COM_K65_82_1
				CASE 1
					CALL HPH_PRINT,@"「%K65_AEGI("口交音")%」","L"
					PRINTFORMW （%CALLNAME:MASTER%さん、きもちよさそう…）
					;[献身的]だからね。セフレだろうともっと気持ちよくなってほしいはずだよね。
					SIF RAND:2
						CALL HPH_PRINT,@"（もっと、悦んでもらいたいな…HPH）","W"
				CASE 2
					CALL HPH_PRINT,@"「%K65_AEGI("控えめ")%」","L"
					PRINTFORMW 椛は小気味良い音を立てて、胸の谷間から覗く亀頭へリズムよく接吻を繰り返している…
				CASE 3
					CALL HPH_PRINT,@"「わ、ぁ…胸の間で、ぴくぴくして……んむっ…%K65_AEGI("控えめ",2)%」","L" 
					PRINTFORML 椛は跳ねまわる阴茎の先端を唇で捕らえ、肉竿を精心地胸で扱き動作著…
					CALL HPH_PRINT,@"（うう…コスるたびに、%CALLNAME:MASTER%さんのにおいが強くなって…おかしくなっちゃいそう…HPH）","W"
			ENDSELECT
		ENDIF
	RETURN 1
	ENDIF
ENDIF
