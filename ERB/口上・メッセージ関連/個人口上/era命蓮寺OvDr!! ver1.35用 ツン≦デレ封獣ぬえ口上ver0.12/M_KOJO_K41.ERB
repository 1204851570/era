;------------------------------------------
;名前を教えるイベ
;-------------------------------------------
@NAME_SET_K41
;/見事な響子口上の丸パクだと感心するがどこもおかしくはない
PRINTL 请输入名称(尽管是直接输入,但是也请选一下選択项)
IF NAME:MASTER != "你"
	LOCALS = %NAME:MASTER%
ELSE
	LOCALS = %CALLNAME:MASTER%
ENDIF
PRINTBUTTON @"[名字就叫(%LOCALS%)]", @"%LOCALS%"
PRINTL 　
;名前を入力するループ
$INPUT_LOOP_ONAMAE
INPUTS
; 行頭行尾の空白文字削除
LOCALS = %RESULTS%
REPLACE LOCALS, "\(\^\\s\+\|\\s\+\$)", ""
;10文字以上は受け付けない
IF STRLENS(LOCALS) > 10
	CLEARLINE 1
	REUSELASTLINE 「……那么长吗？告诉我昵称之类的吧」
	GOTO INPUT_LOOP_ONAMAE
;空白は受け付けない
ELSEIF RESULTS == ""
	PRINTL 请输入名称
	GOTO INPUT_LOOP_ONAMAE
ELSE
	; 同姓同名
	IF FLAG:扮演 != 41 && STRCOUNT(LOCALS, "鵺")
		CLEARLINE 1
		REUSELASTLINE 「嘘付け、%MASTERNAME:41%のどこが正体不明だよ」
		GOTO INPUT_LOOP_ONAMAE
	; 白蓮が主人ではなく、白蓮というフレーズが含まれている。もしくは
	; 星が主人ではなく、寅丸もしくは星というフレーズが含まれている
	ELSEIF (FLAG:扮演 != 55 && STRCOUNT(LOCALS, "聖\*\\s\*白蓮")) || (FLAG:扮演 != 87 && STRCOUNT(LOCALS, "\(寅丸\)\*\\s\*星\$"))
		CLEARLINE 1
		REUSELASTLINE 「好吧好吧,那样就行」
		GOTO INPUT_LOOP_ONAMAE
	; 命蓮寺勢はじき
	; うんそうだね多々良勝五郎とか教える人もいるかもしれないもんね
	ELSEIF (FLAG:扮演 != 39 && LOCALS == "納茲琳") || (FLAG:扮演 != 40 && STRCOUNT(LOCALS, "\(多々良\)\*\\s\*小傘")) || (FLAG:扮演 != 85 && STRCOUNT(LOCALS, "\(雲居\)\*\\s\*一輪")) || (FLAG:扮演 != 86 && STRCOUNT(LOCALS, "\(\(村紗\)*\\s\*水蜜\|村紗\)"))
		CLEARLINE 1
		REUSELASTLINE 「叫这个名字的人,已经有了的样子呢。愛称的话怎么样？」
		GOTO INPUT_LOOP_ONAMAE
	ELSEIF FLAG:扮演 != 69 && STRCOUNT(LOCALS, "\(二ツ岩\)\*\\s\*猯藏")
		CLEARLINE 1
		REUSELASTLINE 「猯藏？不知不觉变年轻了」
		GOTO INPUT_LOOP_ONAMAE
	; 道教勢力はじき
	ELSEIF (FLAG:扮演 != 89 && STRCOUNT(LOCALS, "\(宮古\)\*\\s\*芳香")) || (FLAG:扮演 != 90 && STRCOUNT(LOCALS, "\^霍\*\\s\*青娥")) || (FLAG:扮演 != 91 && STRCOUNT(LOCALS, "\(蘇我\)\*\\s\*屠自古")) || (FLAG:扮演 != 92 && STRCOUNT(LOCALS, "\(物部\)\*\\s\*布都")) || (FLAG:扮演 != 56 && STRCOUNT(LOCALS, "\(\(豊聡耳\)\*\\s\*神子\|聖徳太子\)"))
		CLEARLINE 1
		REUSELASTLINE 
		GOTO INPUT_LOOP_ONAMAE
	; 博麗霊夢というフレーズが含まれてる。もしくは
	; 東風谷早苗というフレーズが含まれている
	ELSEIF STRCOUNT(LOCALS, "\(博麗\)\*\\s\*霊夢") || STRCOUNT(LOCALS, "\(東風谷\)\*\\s\*早苗")
		PRINTFORML 「啊咧,巫女的親戚？为什么会在这里」
	; 霧雨魔理沙というフレーズが含まれてる。
	ELSEIF STRCOUNT(LOCALS, "\(霧雨\)\*\\s\*魔理沙")
		PRINTFORML 「那个魔法使的亲戚？为什么进佛门？」
	; 直接入力ならいけると思ったか？弾くぜ！
	ELSEIF FIRSTTIME("U41-1") && ABL:親密 < 4 && STRCOUNT(LOCALS, "\(お\(\(兄\|にい\)\|\(姉\|ねえ\)\)\(さん\|ちゃん\)\)\$")
		CLEARLINE 1
		REUSELASTLINE 「有好好的在听人说话么？」
		GOTO INPUT_LOOP_ONAMAE
	ELSEIF FIRSTTIME("U41-1") && ABL:親密 < 7 && STRCOUNT(LOCALS, "\(お\(\(兄\|にい\)\|\(姉\|ねえ\)\)\(様\|さま\)\)\$")
		CLEARLINE 1
		REUSELASTLINE 「有好好的在听人说话么？」
		GOTO INPUT_LOOP_ONAMAE
	ELSEIF FIRSTTIME("U41-1") && (ABL:従順 < 7 || ABL:奉仕精神 < 7) && STRCOUNT(LOCALS, "\(\(御\|ご\)\(主人\|しゅじん\)\(様\|さま\)\)$")
		CLEARLINE 1
		REUSELASTLINE 「有好好的在听人说话么？」
		GOTO INPUT_LOOP_ONAMAE
	ELSEIF FIRSTTIME("U41-1") && (!TALENT:恋人 || !TALENT:恋慕) && STRCOUNT(LOCALS, "\(\(旦那\|だんな\)\(様\|さま\)\)\$")
		CLEARLINE 1
		REUSELASTLINE 「有好好的在听人说话么？」
		GOTO INPUT_LOOP_ONAMAE
	ELSE
		PRINTFORML 「%RESULTS%可以么？」
	ENDIF
	PRINTL [0] 是
	PRINTL [1] 否
	;入力した名前を確定するループ
	$INPUT_LOOP_KAKUTEI_1
	INPUT
	IF RESULT == 0
		;あなたの名前を%MASTERNAME:41%に格納
		MASTERNAME:41 = %RESULTS%
		PRINTFORML 
	ELSEIF RESULT == 1
		PRINTL 请输入名称
		GOTO INPUT_LOOP_ONAMAE
	ELSE
		PRINTL 请输入名称
		GOTO INPUT_LOOP_ONAMAE
	ENDIF
ENDIF
