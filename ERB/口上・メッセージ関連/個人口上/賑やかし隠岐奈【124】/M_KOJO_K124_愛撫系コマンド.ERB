@M_KOJO_MESSAGE_COM_K124_0_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
			PRINTFORML 「あ、その、触るのなら、温柔的……」
			PRINTFORML 「慣れてないんだ、こういう、ことは…」
			PRINTFORMW 隠岐奈は害羞に頬を染めている。
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;挿入中
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	;V挿入
	IF LOCAL:1 && TEQUIP:50 == PLAYER
		;射精した
		IF NOWEX:PLAYER:11
			PRINTFORML 「はあっ、はあっ%UNICODE(0x2764)% いっぱい、出たな……？」
			PRINTFORMW 手を繋ぎ接吻を求めて顔を近づけると、隠岐奈は誘われるように主动舌を絡めてきた…。
			RETURN 1
		ELSE
			PRINTFORMW 「ひぃ、んッ！？　まて、そこはっ、あぁっ%UNICODE(0x2764)%」
			RETURN 1
		ENDIF
	;A挿入
	ELSEIF LOCAL:1 && TEQUIP:51 == PLAYER
		;射精した
		IF NOWEX:PLAYER:11
			PRINTFORML 「はあっ、はあっ%UNICODE(0x2764)% いっぱい、出たな……？」
			PRINTFORMW 手を繋ぎ接吻を求めて顔を近づけると、隠岐奈は誘われるように主动舌を絡めてきた…。
			RETURN 1
		ELSE
			PRINTFORMW 「ひうっ、い、いいっ%UNICODE(0x2764)% きもちいいッ%UNICODE(0x2764)%」
			RETURN 1
		ENDIF
	ENDIF
	;-------------------------------------------------
	;ふたなり
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && (TALENT:2 & 3) == 3
		SELECTCASE RAND:2
		CASE 0
			PRINTFORMW 「ひゃ、やあっ！気持ちいいぃ…ッ%UNICODE(0x2764)%」
		CASE 1
			PRINTFORMW 「んっ……ふふっ、%CALLNAME:MASTER%、もっとしていいぞ？」
		ENDSELECT
		RETURN 1
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		RETURN 1
	;恋慕
	ELSEIF TALENT:恋慕
		SELECTCASE RAND:3
		CASE 0
			PRINTFORML 「んん…っ、やっ、稍微……やめ、ああぁっ%UNICODE(0x2764)%」
			PRINTFORMW 隠岐奈は快楽で思わず声をあげてしまっている。
		CASE 1
			PRINTFORML 「待てっ！もういいから、や、やだぁっ%UNICODE(0x2764)%」
			PRINTFORMW 隠岐奈は拒むように%CALLNAME:MASTER%の手を掴むが、全く力が入っていない。
		CASE 2
			PRINTFORML 「そ、そこ弄らないで…やああぁっ！」
			PRINTFORMW 隠岐奈は快楽を我慢するかのように目を瞑っている。
		ENDSELECT
		RETURN 1
	;それ以外
	ELSE
		SELECTCASE RAND:2
		CASE 0
			PRINTFORML 轻轻地陰核を引っ掻いてやると、隠岐奈はびくびくと体を震わせた。
			PRINTFORMW 「あぁっ、や、やめっ！そこは弱いから…っ」
		CASE 1
			PRINTFORMW 「んっ、気持ちよくなんて……あっ%UNICODE(0x2764)%」
		ENDSELECT
		RETURN 1
	ENDIF
ENDIF
RETURN 1
