;40,ローター
;==================================================
@M_KOJO_MESSAGE_COM_K124_40
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K124_40_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K124_40_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;時姦中
	IF FLAG:70
		RETURN 1
	;睡姦中
	ELSEIF CFLAG:MASTER:恶作剧 == 2
		RETURN 1
	ELSEIF TCVAR:124:烂醉 == 1
		RETURN 1
	;恋慕or恋人
	ELSEIF TALENT:124:恋人 || TALENT:124:恋慕 || TALENT:124:愛欲
		SELECTCASE RAND:2
			CASE 0
				PRINTFORMW 「こ、こんな玩具で……ふ、ううんっ」
			CASEELSE
				PRINTFORMW 「ううっ、き、気持ちいいけど……なんか釈然としない…っ！」
		ENDSELECT
	;それ以外
	ELSE
		SELECTCASE RAND:2
			CASE 0
				PRINTFORMW 「く、んあぁっ！お、おいっ！いつまで押し付けてるつもりだ……っ！」
			CASEELSE
				PRINTFORMW 「ひううっ、やっ、し、しつこいぞ！もうやめろ……っ！」
		ENDSELECT
	ENDIF
ENDIF

RETURN 1
