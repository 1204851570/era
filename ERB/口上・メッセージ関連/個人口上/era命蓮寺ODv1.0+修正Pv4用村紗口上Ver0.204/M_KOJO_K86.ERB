		ENDIF
		;墓場プレイ経験回数+1(最大3)
		SIF CFLAG:1201 < 3
			CFLAG:1201 ++
	;進行度2
	ELSEIF TCVAR:352 == 2
		;カウント10で進行度3へ
		IF TCVAR:351 == 10
			PRINTFORML %CALLNAME:MASTER%和%CALLNAME:TARGET%的痴态吸引了更多幽灵聚集了过来……
			PRINTFORML 幽灵们聚集在%CALLNAME:MASTER%们身边,在周围飞来飞去.
			PRINTL
			;初回
			IF CFLAG:1201 == 1
				PRINTFORML 「……啊…呀… 又…变…多……去了…啊」
				PRINTW
			ENDIF
			TCVAR:352 = 3
		ELSE
			PRINTFORML 幽灵们在%CALLNAME:MASTER%和%CALLNAME:TARGET%的周围轻轻漂浮着…
			PRINTL
			;カウント+1
			TCVAR:351 ++
		ENDIF
	;進行度3
	ELSEIF TCVAR:352 == 3
		PRINTFORML 幽灵们聚集在%CALLNAME:MASTER%们身边,在周围飞来飞去.
		IF RAND:4 == 0
			;上半身裸
			IF TEQUIP:1 == 0
				PRINTFORML %CALLNAME:TARGET%汗流浃背的白色躯体暴露在了亡者们的视线下…
			ELSE
				PRINTFORML %CALLNAME:MASTER%将%CALLNAME:TARGET%的外衣脱下,摘下胸罩,将肌肤展示给亡者们欣赏...
				;ターゲット上半身脱衣
				EQUIP:上半身外衣１ = 0
				EQUIP:上半身外衣２ = 0
				EQUIP:連体内衣 = 0
				EQUIP:連衣裙 = 0
				EQUIP:和服 = 0
				EQUIP:緊身内衣 = 0
				EQUIP:上半身内衣１ = 0
				EQUIP:上半身内衣２ = 0
				CALL CLOTHES_SETTING_TRAIN(TARGET)
			ENDIF
		ELSEIF RAND:3 == 0
			PRINTFORML 在远处没能挤进来的幽灵们也早目不转睛地看着这边……
		ELSEIF RAND:2 == 0
			PRINTFORML 若干个幽灵像要贴近%CALLNAME:MASTER%的肌肤般缠绕在身边……
		ELSE
			PRINTFORML 每次%CALLNAME:MASTER%和%CALLNAME:TARGET%发出娇喘声，都能感到幽灵们在无声无息的呻吟着…
		ENDIF
		PRINTL
	ENDIF
ENDIF

