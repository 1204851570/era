	;-------------------------------------------------
	IF LOCAL:1 && ARG == 6
		PRINTFORML 
		IF TALENT:恋人
			PRINTFORML 「%CALLNAME:MASTER%ーー！」
			PRINTFORML %CALLNAME:MASTER%看到阿空的一瞬間
