			SELECTCASE TFLAG:193
			CASE 0
				PRINTFORMW 「呜…请不要这么突然」
			CASE 1
				IF ( TALENT:恋慕 || TALENT:愛欲 ) && ( ABL:露出癖 >= 6 || ( GET_TARGETNUM() == 1 && !DATE_HITOGOMI(CFLAG:PLAYER:現在位置) && !WITH_MOB() && ABL:露出癖 >= 4 ) )
