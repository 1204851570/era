						IF CFLAG:没穿内裤 && !EQUIP:緊身内衣 && !EQUIP:連体内衣
							PRINTFORMD 充分被灌进了%K2_EXPRESSION_VARIATION("SPERM")%裸露在外
							SIF SOAKED_OR_NOT("V_SEMINAL")
								PRINTFORMD %K2_EXPRESSION_VARIATION("VULVA")%
							SIF SOAKED_OR_NOT("A_SEMINAL")
								PRINTFORMD \@ SOAKED_OR_NOT("V_SEMINAL") ?と # \@%K2_EXPRESSION_VARIATION("ANUS")%
							PRINTFORMDL 咕嘟咕嘟地流出来，滴落在%CALLNAME:TARGET%的脚下。
						ELSE
							SIF SOAKED_OR_NOT("V_SEMINAL")
								PRINTFORMD %K2_EXPRESSION_VARIATION("VULVA")%
							SIF SOAKED_OR_NOT("A_SEMINAL")
								PRINTFORMD \@ SOAKED_OR_NOT("V_SEMINAL") ?と # \@%K2_EXPRESSION_VARIATION("ANUS")%
							PRINTFORMDL 从里面溢出来的%K2_EXPRESSION_VARIATION("SPERM")%浸湿了\@ PANTIES_TRANSPARENCY(TARGET) ?连阴唇的形状都能看出来 # \@内衣湿透了。
						ENDIF
					;愛液でびしょびしょの場合
					ELSEIF SOAKED_OR_NOT("SECRETIONAL")
						PRINTFORMD 淫乱的分泌液，
						;パンツなしかつ全身下着なし
						IF CFLAG:没穿内裤 && !EQUIP:緊身内衣 && !EQUIP:連体内衣
							PRINTFORMDL 从赤裸的秘处流到大腿上形成一条线。
						ELSE
							PRINTFORMDL \@ PANTIES_TRANSPARENCY(TARGET) ?连陰唇的形状都能看出来 # \@内衣湿透了。
						ENDIF
					ENDIF
				ENDIF
				PRINTW
			ENDSELECT
		ENDIF
	ENDIF
ENDIF
RETURN 1
