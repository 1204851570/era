;310,尻を撫でる
;TFLAG:193(1=不快(TALENT:臆病なら涙目) 2&&3=恥ずかしがる(TALENT:男嫌い兼MASTERが男なら、身をすくめる) 4=されるがまま)
;CFLAG:うふふ==2&&TCVAR:20(パンツ無し顔面騎乗(70=クンニ強制 71=フェラチオ強制 72=アナル奉仕) 35=スカート内見せつけ 52=素股)
;CFLAG:うふふ==2&&TEQUIP:シックスナイン(パンツありTCVAR:20(70～72)はこちら)
;PREVCOM(305=膝枕
;TFLAG:400(1=デート先or地底のモブが反応する)
;==================================================
@M_KOJO_MESSAGE_COM_K2_310
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K2_310_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K2_310_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	$CARESS_HIP_LOOP
	IF CFLAG:诶嘿嘿 != 2
		IF K2_CONSCIOUS_AND_TOLERANT()
			SELECTCASE TFLAG:193
			CASE 1
				PRINTFORMW 「哎呀，太过分了！不要这样！」
			CASE 2,3
				IF TALENT:恋慕 || TALENT:愛欲
					SELECTCASE RAND:2
					CASE 1
						CALL K2_HPH_PRINT,@"「如果不是在这里做恶作剧…而是到床上去…HPH」","W"
					CASEELSE
						CALL K2_HPH_PRINT,@"「呀HPH 心情变得好奇怪…已经没法好好工作了HPH」","W"
					ENDSELECT
				ELSE
					SELECTCASE RAND:2
					CASE 1
						PRINTFORMW 「喂，突然要做什么呢？」
					CASEELSE
						PRINTFORMW 「啊！？哎呀，不行」
					ENDSELECT
				ENDIF
			CASE 4
				;無知でない、かつ恋慕or愛欲、かつ欲望10以上or人目がない
				IF !TALENT:無知 && ( TALENT:恋慕 || TALENT:愛欲 ) && ( ABL:欲望 >= 10 || !EYEWITNESS() )
					SELECTCASE RAND:3
					CASE 1
						IF BASE:情緒 >= 750 && BASE:理性 < 800 && CFLAG:积攒度 >= 500
							CALL K2_HPH_PRINT,@"「不只是触碰、您可以继续…HPH」","W"
						ELSE
							GOTO CARESS_HIP_LOOP
						ENDIF
					CASE 2
						CALL K2_HPH_PRINT,@"「啊HPH 又想做恶作剧了吗？HPH」","W"
					CASEELSE
						CALL K2_HPH_PRINT,@"「嗯HPH %CALLNAME:MASTER%大人真是的HPH」","W"
					ENDSELECT
				ELSE
					SELECTCASE RAND:2
					CASE 1
						PRINTFORMW 「啊。真是没办法啊」
					CASEELSE
						PRINTFORMW 「呀、啊。%CALLNAME:MASTER%大人太过分了」
					ENDSELECT
				ENDIF
			ENDSELECT
		ELSEIF SHIRAHU(TARGET) && TFLAG:193 == 1
			PRINTFORMW 「喂，喂，你在干什么 真是奇怪！」
		ENDIF
	ENDIF
ENDIF
RETURN 1
