;==================================================
;相手が押し倒しを解除
;==================================================
@M_KOJO_EVENT_K2_30(ARG,ARG:1)
;口上主はTARGET
;--------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------

IF LOCAL
	;-------------------------------------------------
	;疲れた
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
		IF TALENT:恋人 || TALENT:恋慕 || TALENT:愛欲 || TALENT:風騷
			PRINTFORMW 「哈…虽然还想再多做一点……但我累了」
		ELSE
			PRINTFORML 「哈……哈……对不起……」
			PRINTFORMDW %CALLNAME:TARGET%疲惫地离开了%CALLNAME:MASTER%的身体。
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;満足して解放
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
		IF TALENT:恋人 || TALENT:恋慕 || TALENT:愛欲 || TALENT:風騷
			IF K2_SEXUAL_FRANKNESS() && !RAND:3
				CALL K2_HPH_PRINT,@"「はぁHPH 真棒HPH」","L"
			ELSEIF RAND:2
				CALL K2_HPH_PRINT,@"「はぁ、はぁHPH 我爱你HPH」","L"
			ELSE
				CALL K2_HPH_PRINT,@"「感谢您对我的爱HPH」","L"
			ENDIF
			PRINTFORMDW %CALLNAME:TARGET%满足地微笑着，最后亲吻了%CALLNAME:MASTER%的脸颊，放开了身体。
		ELSE
			PRINTFORML 「实在很抱歉。我真的……」
			PRINTFORMDW %CALLNAME:TARGET%为自己因%TEXTR("欲望/劣情")%而对%CALLNAME:MASTER%的采取的袭击行为道歉了。
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;満足したところを押し倒された
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 3
		PRINTFORML
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;壊れるまでしてあげる
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 4
		PRINTFORML
		RETURN 1
	ENDIF
ENDIF
RETURN 1




