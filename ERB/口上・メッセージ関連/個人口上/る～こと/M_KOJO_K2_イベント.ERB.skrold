;==================================================
;押し倒した
;==================================================
@M_KOJO_EVENT_K2_10(ARG,ARG:1)
;口上主はTARGET
;--------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------

IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;うふふ==2（開始時）
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
		;-------------------------------------------------
		;MASTERを押し倒した（開始時）
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1 && !ARG:1
			IF K2_CONSCIOUS_AND_TOLERANT() && !TALENT:無知
				;初めて
				IF FIRSTTIME("MASTERを押し倒した",0,2)
					;うふふ既成事実なし
					IF ( CFLAG:既成事実 & 2 ) == 0
						;処女ビッチ
						IF TALENT:ヤリマン && !CFLAG:1001 && ( TALENT:処女 == 1 || TALENT:処女 == -1 )
							CALL K2_HPH_PRINT,@"「もう我慢できませんHPH 襲っちゃいますねHPH」","W"
							PRINTFORMDW %CALLNAME:TARGET%はそう言うと突然%CALLNAME:MASTER%を押し倒した。
							;両刀でも男嫌いでもなく、かつ主人が男
							IF TALENT:性別嗜好 != -1 && TALENT:性別嗜好 != 1 && TALENT:MASTER:性別 == 2
								PRINTFORMW 「わたし、ずっとえっちなことに興味があったんですが、博麗神社には女の人ばっかりで……」
								CALL K2_HPH_PRINT,@"「ですから、%CALLNAME:MASTER%さま\@ GET_MAPNAME(MAIN_MAP) == "博麗神社" ?がいらした #とお会いした \@ときから、こういう風に襲っちゃう想像をしてたんですHPH」","W"
							ELSE
								PRINTFORMW 「わたし、ずっとえっちなことに興味があったんですが、今までそういうことに縁が無くて……」
								CALL K2_HPH_PRINT,@"「でも、%CALLNAME:MASTER%さまって何だかとってもえっちな雰囲気があるものですから……たまらなくなっちゃいましたHPH」","W"
							ENDIF
						;真正ヤリマン
						ELSEIF CFLAG:1001
							CALL K2_HPH_PRINT,@"「%CALLNAME:MASTER%さまが悪いんですよHPH とってもイヤラシイ雰囲気してるんですものHPH」","W"
							CALL K2_HPH_PRINT,@"「わたしみたいなお股のゆるい女は、おまんこしたくてしたくて我慢できなくなっちゃいますHPH」","W"
						;その他
						ELSE
							PRINTFORMW 「はしたない女だとお思いですよね……」
							PRINTFORMW 「でも、どうしても我慢できないんです。どうかゆるしてください」
							PRINTFORMDW %CALLNAME:TARGET%はそう言うやいなや%CALLNAME:MASTER%にむしゃぶりついてきた。
						ENDIF
					;うふふ既成事実あり
					ELSE 
						;ヤリマン
						IF TALENT:ヤリマン
							CALL K2_HPH_PRINT,@"「えへへHPH なんだかムラムラしちゃって。今日はわたしからヤっちゃいますねHPH」","W"
						;非ヤリマン
						ELSE
							PRINTFORMW 「%CALLNAME:MASTER%さまのせいで……わたし…いやらしい女になってしまいました」
						ENDIF
					ENDIF
					RETURN 1
				ENDIF
				;二回目以降
				IF TALENT:恋人 || TALENT:恋慕
					CALL K2_HPH_PRINT,@"「%CALLNAME:MASTER%さまっHPH」","L"
					SIF !K2_EXCESSIVE_STATURE_GAP()
						PRINTFORMDL %CALLNAME:TARGET%が腕をつかんで体を寄せてきた。
					IF !FLAG:甲斐性無 && RAND:2
						CALL K2_HPH_PRINT,@"「今日はわたしからシたいですHPH」","W"
					ELSE
						CALL K2_HPH_PRINT,@"「えへへHPH おまんこしたくなっちゃいましたHPH」","W"
					ENDIF
				ELSEIF TALENT:ヤリマン || TALENT:愛欲
					CALL K2_HPH_PRINT,@"「またムラムラしてきちゃいましたHPH %CALLNAME:MASTER%さまに鎮めていただきたいですHPH」","W"
				ELSE
					PRINTFORMW 「申しわけありません…… また…我慢できなくなってしまいました」
				ENDIF
			ENDIF
			RETURN 1
		ENDIF
		;-------------------------------------------------
		;MASTERに抱きついた（開始時）
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 0
		;-------------------------------------------------
		IF LOCAL:1 && ARG:1 == 1
			PRINTFORML
			RETURN 1
		ENDIF
		
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;なだめられた（終了時）
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
		;-------------------------------------------------
		;抜け出した
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1 && !ARG:1
			IF K2_CONSCIOUS_AND_TOLERANT() && !TALENT:無知
				IF TALENT:恋人 || TALENT:恋慕 || TALENT:愛欲 || TALENT:ヤリマン
					PRINTFORML 「あれっ　もう終わっちゃうんですか？」
					IF CFLAG:溜まってる度 >= 300
						CALL K2_HPH_PRINT,@"「う〜HPH もっともっとおまんこしていたかったです…HPH」","L"
						PRINTFORMDW %CALLNAME:TARGET%はまだ息をあらくして目をうるませている。
					ELSE
						CALL K2_HPH_PRINT,@"「…わかりました。次はもっとしましょうねHPH」","W"
					ENDIF
				ELSEIF TALENT:セフレ
					PRINTFORML 「あっ… もう…終わりなんですか…？」
					SIF CFLAG:溜まってる度 >= 300
						PRINTFORMDL %CALLNAME:TARGET%は物足りなさそうな様子だ。
					WAIT
				ELSE
					PRINTFORML 「あ…お嫌でしたよね…申しわけありませんでした…」
					SIF CFLAG:溜まってる度 >= 300
						PRINTFORMDL %CALLNAME:TARGET%は俯いて引き下がったものの、まだ劣情がおさまっていない様子でモジモジとしている。
					WAIT
				ENDIF
			ENDIF
			RETURN 1
		ENDIF
		;-------------------------------------------------
		;抱き着きモード満足終了
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 0
		;-------------------------------------------------
		IF LOCAL:1 && ARG:1 == 1
			PRINTFORML
			RETURN 1
		ENDIF
		;-------------------------------------------------
		;抱き着きモード強制終了
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 0
		;-------------------------------------------------
		IF LOCAL:1 && ARG:1 == 2
			PRINTFORML
			RETURN 1
		ENDIF
	ENDIF
	;-------------------------------------------------
	;抜け出す失敗
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 3
		;-------------------------------------------------
		;好きにして状態から抜け出せなかった
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 0
		;-------------------------------------------------
		IF LOCAL:1 && ARG:1 == 1
			PRINTFORML
			RETURN 1
		ENDIF
		;-------------------------------------------------
		;欲求不満が高くて抜け出せなかった
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 0
		;-------------------------------------------------
		IF LOCAL:1 && ARG:1 == 2
			PRINTFORML
			RETURN 1
		ENDIF
		
		IF K2_CONSCIOUS_AND_TOLERANT() && !TALENT:無知
			IF TALENT:恋人 || TALENT:恋慕 || TALENT:愛欲 || TALENT:ヤリマン
				SELECTCASE RAND:2
				CASE 1
					CALL K2_HPH_PRINT,@"「やんHPH 逃げちゃダメですHPH まだまだおまんこしたりませんよぉHPH」","W"
				CASEELSE
					CALL K2_HPH_PRINT,@"「ダメですぅHPH もっともっと愛してくださいHPH」","W"
				ENDSELECT
			ELSE
				PRINTFORML 「…すみません…まだ欲しいんです…」
				PRINTFORMDW %CALLNAME:TARGET%はギュっと抱き付いてはなそうとしない。
			ENDIF
		ENDIF
		RETURN 1
	ENDIF
ENDIF
RETURN 1

