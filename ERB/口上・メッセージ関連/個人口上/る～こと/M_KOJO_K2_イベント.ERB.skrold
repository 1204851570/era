;==================================================
;相手が押し倒しを解除
;==================================================
@M_KOJO_EVENT_K2_30(ARG,ARG:1)
;口上主はTARGET
;--------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------

IF LOCAL
	;-------------------------------------------------
	;疲れた
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
		IF TALENT:恋人 || TALENT:恋慕 || TALENT:愛欲 || TALENT:ヤリマン
			PRINTFORMW 「はぁ… もっとシたかったんですが…疲れちゃいました」
		ELSE
			PRINTFORML 「はぁ…はぁ……申しわけ…ありませんでした」
			PRINTFORMDW %CALLNAME:TARGET%は疲れた様子で%CALLNAME:MASTER%の体から離れた。
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;満足して解放
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
		IF TALENT:恋人 || TALENT:恋慕 || TALENT:愛欲 || TALENT:ヤリマン
			IF K2_SEXUAL_FRANKNESS() && !RAND:3
				CALL K2_HPH_PRINT,@"「はぁHPH とってもよかったですHPH」","L"
			ELSEIF RAND:2
				CALL K2_HPH_PRINT,@"「はぁ、はぁHPH とっても愛してもらっちゃいましたHPH」","L"
			ELSE
				CALL K2_HPH_PRINT,@"「いっぱい愛してくださって、ありがとうございましたHPH」","L"
			ENDIF
			PRINTFORMDW %CALLNAME:TARGET%は満足げに微笑み、最後に%CALLNAME:MASTER%の頬にキスをすると体を離した。
		ELSE
			PRINTFORML 「申しわけございませんでした。わたしったら本当に…」
			PRINTFORMDW %CALLNAME:TARGET%は%TEXTR("欲望のままに/劣情にとらわれて")%%CALLNAME:MASTER%を襲ってしまったことを謝罪した。
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;満足したところを押し倒された
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 3
		PRINTFORML
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;壊れるまでしてあげる
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 4
		PRINTFORML
		RETURN 1
	ENDIF
ENDIF
RETURN 1




