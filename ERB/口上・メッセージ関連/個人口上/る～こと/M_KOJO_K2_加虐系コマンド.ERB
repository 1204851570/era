;==================================================
;100,スパンキング
;==================================================
@M_KOJO_MESSAGE_COM_K2_100
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K2_100_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K2_100_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORMW 
		RETURN 1
	ENDIF

	$SPANKING_LOOP
	IF ( TALENT:恋慕 || TALENT:愛欲 || TALENT:思慕 || TALENT:炮友 ) && K2_BASIC_CONDITION_IN_MATING()
		IF ( ABL:受虐属性 >= 4 || ( TALENT:受虐狂 && ABL:受虐属性 >= 2 ) ) && ( !TALENT:施虐狂 || ABL:欲望 >= 20 )
			SELECTCASE RAND:4
			CASE 1
				IF CFLAG:1001 && !RAND:3
					IF TALENT:恋慕 || TALENT:恋人
						CALL K2_HPH_PRINT,@"「没有把处女献给%CALLNAME:MASTER%大人、实在不好意思HPH」","L"
						CALL K2_HPH_PRINT,@"「请随意品尝这个被各式各样的人使用过的二手机器人吧HPH」","W"
					ELSEIF ABL:清掃技能 + ABL:料理技能 < 4
						CALL K2_HPH_PRINT,@"「好好惩罚一下这个忘记了工作、只会记着你的事的无耻女仆吧HPH」","W"
					ELSE
						CALL K2_HPH_PRINT,@"「嗯、哈HPH 无论是谁都可以的淫乱女仆、真抱歉HPH」","W"
					ENDIF
				ELSE
					GOTO SPANKING_LOOP
				ENDIF
			CASE 2
				CALL K2_HPH_PRINT,@"「啊哈HPH 请惩罚我这个废物女仆HPH」","W"
			CASE 3
				CALL K2_HPH_PRINT,@"「呼HPH 哈HPH 好痛HPH」","W"
			CASEELSE
				CALL K2_HPH_PRINT,@"「啊呀HPH 呃哈HPH 嗯，哈…可以…结束了吗?HPH」","W"
			ENDSELECT
		ELSE
			SELECTCASE RAND:2
			CASE 1
				PRINTFORMW 「呜……屁股疼得要命」
			CASEELSE
				PRINTFORMW 「呜……屁股痛得发麻」
			ENDSELECT
		ENDIF
	ELSE
		SELECTCASE RAND:2
		CASE 1
			PRINTFORMW 「呜呜……好痛啊，请停止吧」
		CASEELSE
			PRINTFORMW 「噫、嗯、哇、哈……不，不要，请……」
		ENDSELECT
	ENDIF
ENDIF
RETURN 1

