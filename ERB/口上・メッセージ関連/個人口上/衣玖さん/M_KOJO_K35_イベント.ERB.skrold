		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1
			PRINTFORML 「良かったぁ！今日は色々と考えてきたんですから任せてくださいね！」
			PRINTFORML  嬉しそうに笑みを浮かべあなたの腕を引く
			RETURN 1
		ENDIF
		;-------------------------------------------------
		CASE 2
		;断った
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1
			PRINTFORML 「そうですか……ま、またの機会に……」
			PRINTFORML  悲しそうに眼を伏せた後、無理をするように顔を上げ笑みを浮かべる
			RETURN 1
		ENDIF
		;-------------------------------------------------
		CASE 3
		;部屋に誘われた
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1
			PRINTFORML 「あっ%CALLNAME:MASTER%!」
			PRINTFORML 「あの……私の家に遊びに来ませんか……？」
			RETURN 1
		ENDIF
		;-------------------------------------------------
		CASE 4
		;部屋に誘われたのを受けた
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1
			PRINTFORML 「ふふんっ……部屋もきちんと片付いてるんですよ！」
			PRINTFORML  どこか誇らしげに語る衣玖についていき家へと上がり込む
			RETURN 1
		ENDIF
		;-------------------------------------------------
		CASE 5
		;部屋に誘われたのを断った
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1
			PRINTFORML 「そ、そうですか……ごめんなさい……いきなり誘ってしまって……」

			RETURN 1
		ENDIF
	ENDSELECT
ENDIF
RETURN 1

