;==================================================
;デート帰りに告白（恋人が付く）
;==================================================
@M_KOJO_SPEVENT_K35_2(ARG,ARG:1)
;口上主はTARGET
;地の文表示
CALL SPEVENT_MESSAGE_2(ARG,ARG:1)
;--------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;告白内容
	;この告白内容表示後に受けるかどうかの選択分が表示されます
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 0
		PRINTFORML 「あの……%CALLNAME:MASTER%……」
		PRINTFORMW  呼び止められたあなたが衣玖の方へ見ると彼女は緊張した面持ちであなたと見つめ合う
		PRINTFORMW 「こういうのって男性から言うのが普通なのでしょうか……ですけど他の子に%CALLNAME:MASTER%をとられるのは嫌なんですッ！」
		PRINTFORMW 「%CALLNAME:MASTER%っ！わ、私とケ、結婚してくださいッ！」
		PRINTFORMW 「%CALLNAME:MASTER%とこれからも一緒に……永久に一緒に居たいんです！」
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;告白オッケー（恋人が付く）
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
		PRINTFORMW  あなたは前々から買って置いた指輪を衣玖に見せ先に告白されてしまったなと返す
		PRINTFORMW 「あぁなんと幸せなのでしょうか……」
		PRINTFORMW  よほど嬉しいのか衣玖の目には涙が見える
		PRINTFORML 「私達二人で良いですから……ドレスも会場も我が儘は言いません……ここで小さな小さな誓いを立てていただけますか？」
		PRINTFORMW  あなたは笑顔で頷く
		PRINTFORML 「それでは……汝%CALLNAME:MASTER%は私永江衣玖を妻とし」
		PRINTFORML 「良き時も悪き時も、富める時も貧しき時も」
		PRINTFORML 「病める時も健やかなる時も、共に歩み、他の者に依らず」
		PRINTFORML 「死が……二人を分かつまで」
		PRINTFORML 「愛を誓い、夫を想い、妻のみに添うことを」
		PRINTFORMW 「誓いますか……？」
		PRINTFORMW  あなたは誓うと返すと衣玖は自分の誓いを言い出す
		PRINTFORML 「私永江衣玖は%CALLNAME:MASTER%を夫とし」
		PRINTFORML 「良き時も悪き時も、富める時も貧しき時も」
		PRINTFORML 「病める時も健やかなる時も、共に歩み、他の者に依らず」
		PRINTFORML 「死が……二人を分かつまで」
		PRINTFORML 「愛を誓い、夫を想い、夫のみに添うことを」
		PRINTFORMW 「ここに誓います」
		PRINTFORMW 「この指輪私の指にはめてもらってもよろしいですか？」
		PRINTFORMW 「結婚式は上げずとも二人が永遠を誓い合った仲であると……」
		PRINTFORMW 「私%CALLNAME:MASTER%の妻であると……私の指に刻み付けて下さい」
		PRINTFORML 
		PRINTFORMW  あなたは衣玖の手を取り薬指に指輪をはめる
		PRINTFORMW  その後衣玖はあなたの手を取り薬指に指輪をはめる
		PRINTFORMW 「それでは誓いのキ……キスを……」
		PRINTFORMW  二人は永い永いキスをし、愛を誓い合った
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;告白失敗
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
		PRINTFORMW  あなたはすまないが衣玖の気持ちには答えられないと返す
		PRINTFORMW 「あはは……そうですよね……冗談……冗談ですから……」
		PRINTFORML 「なんてことのない遊びですから……忘れて下さい……」
		PRINTFORMW  衣玖の目からはポロポロと涙が流れる
		PRINTFORMW 「ぐすっ……泣いてなんで……冗談なのに……」
		PRINTFORMW 「さようなら……ごめんなさい……」
		RETURN 1
	ENDIF

ENDIF
RETURN 1

;==================================================
;デート帰り
;==================================================
@M_KOJO_SPEVENT_K35_3(ARG,ARG:1)
;口上主はTARGET
;地の文表示
CALL SPEVENT_MESSAGE_3(ARG,ARG:1)
;--------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;通常帰宅
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 0
		IF TALENT:恋人
			PRINTFORMW 「今日もとっても楽しかったですね」
			PRINTFORMW 「この後はお暇でしたら一緒にシャワーでも……%UNICODE(0x2764)%」
		ELSEIF TALENT:恋慕
			PRINTFORMW 「今日はお付き合いいただきありがとうございます」
			PRINTFORMW 「この後の予定ってありますか？無いのでしたらもう少しご一緒しても……？」
		ELSE
			PRINTFORMW 「今日は貴重な時間を割いていただきありがとうございました」
			PRINTFORMW 「それではまた機会があれば何時でも誘ってくださいね」
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;反発刻印消去イベント
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
		PRINTFORML
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;反発刻印2>1レベルダウンイベント
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
		PRINTFORML
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;反発刻印3>2レベルダウンイベント
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 3
		PRINTFORML
		RETURN 1
	ENDIF
	PRINTFORML
ENDIF
RETURN 1

;※※※※※※※※※※※※※※※※※※※※※※※※※※
;"EVENT"系イベント
;※※※※※※※※※※※※※※※※※※※※※※※※※※
;==================================================
;部屋で遭遇
;==================================================
@M_KOJO_EVENT_K35_1(ARG,ARG:1)
;口上主はTARGET
;--------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;MASTERが入ってきた
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
		IF TALENT:恋慕
			PRINTFORML 「%CALLNAME:MASTER%？何か御用ですか？」
		ELSE
			PRINTFORML 「こんにちは……何か御用ですか？」
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;TARGETが入ってきた
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
		IF TALENT:恋慕
			PRINTFORML 「あっ%CALLNAME:MASTER%！お時間少しいいですか？」
		ELSE
			PRINTFORML 「こんにちは……何をしていらっしゃるのですか？」
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;TARGETが風呂に入ってきて一緒に入る
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 3
		PRINTFORML 「湯加減どうですか？お背中流しますね%UNICODE(0x2764)%」
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;TARGETが風呂に入ってきて出て行った
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 4
		PRINTFORML 「あっ！すいません！あなたが居るのは知らなくて……」
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;TARGETが風呂に入ってきて追い出された
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 5
		PRINTFORML 「な、今の時間帯は女性専用です！出てってください！」
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;外出先で遭遇した
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 6
		PRINTFORML 「おや？こんな所で奇遇ですね」
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;他キャラとデート中に遭遇した
	;ARG:1にデート相手
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 7
		IF TALENT:恋人
			PRINTFORML 「%CALLNAME:MASTER%……誰ですかその子は……」
			PRINTFORMW  背中から急に衣玖に声をかけられゾクリとする
			PRINTFORML 「説明してください……誰なんですか……きちんと答えてもらえないと……」
			PRINTFORMW  背中に硬く鋭利な物が押し当てられながらあなたは衣玖に少しばかり誤魔化しながら説明した
			PRINTFORMW 「知りません！帰ったらお説教ですから覚悟しててくださいね」
		ELSE
			PRINTFORML 「あっ！こんにちは！ってお邪魔でしたね」
			PRINTFORMW （ああいう子が好みなのでしょうか……）
		ENDIF
		RETURN 1
	ENDIF
	;その他
	PRINTFORML  
ENDIF
RETURN 1

;==================================================
;朝起きたとき（おはよう口上）
