;==================================================
;300,会話
;==================================================
@M_KOJO_MESSAGE_COM_K49_300
;コマンド共通メッセージ(TRAIN_MESSAGE) 
;削除すると「～話をした。」とかが消えます
IF CFLAG:1496 == 2 && !RAND:2
	CALL PET49()
ELSEIF CFLAG:1496 == 1 && !RAND:2
	CALL ONRYO49()
ENDIF
SIF 陥落状態() < 3 && !TCVAR:302 && (BASE:酒気 < (MAXBASE:酒気 / 10) * 4)
	CALL TRAIN_MESSAGE
;口上メイン呼出
CALL M_KOJO_MESSAGE_COM_K49_300_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K49_300_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	IF (BASE:酒気 > (MAXBASE:酒気 / 10) * 4)
		DRAWLINE
		SIF FLAG:口上色
			CALLFORM M_KOJO_COLOR_K49
		CALL DRUNKER49()
	ELSEIF TCVAR:302
		DRAWLINE
		SIF FLAG:口上色
			CALLFORM M_KOJO_COLOR_K49
		IF 陥落状態() >= 4
			IF 陥落状態() == 5
				PRINTFORML 「主様聲音感覺真不錯、偶然都來隨便聊天吧？」
				PRINTFORML 「『很無聊吧』？　只要在主様身邊我都覺得很開心」
				PRINTFORML 「『你這傢伙真可愛』……、因為主様實在太棒了」
			ELSE
				PRINTFORML 「主様、不用勉強自己開口也可以哦」
				PRINTFORML 「哼哼、這樣、試圖尋找話題這一點、真是非常棒」
				PRINTFORML 「而且、能讓我看見你的心我也很開心」
			ENDIF
		ELSEIF 陥落状態() == 3
			PRINTFORML 「我很喜歡%CALLNAME:MASTER%的聲音、也很喜歡看你的心」
			PRINTFORML 「毋須強行跟我說話也可以的」
		ELSEIF 陥落状態() == 2
			PRINTFORML 「沒有話要說的話請你閉嘴」
		ELSEIF 陥落状態() == 1
			PRINTFORML 「不打算開口的話、毋須強行跟我說話也可以哦」
		ELSE
			PRINTFORML 「明明沒有話想說、卻強行讓自己開口嗎？」
		ENDIF
		PRINTL
		RESETCOLOR
	ELSEIF 陥落状態() >= 4
		IF 陥落状態() == 5
			DRAWLINE
			PRINTFORM 覺一邊聽著%CALLNAME:MASTER%的話語、
			IF CFLAG:TARGET:陪睡中
				PRINTFORML 一邊帶著神魂傾倒的表情、緊緊地抱著%CALLNAME:MASTER%
			ELSEIF CHK_DATENOW(CFLAG:TARGET:约会中)
				PRINTFORML 一邊和%CALLNAME:MASTER%彼此十指相扣、面露燦爛如鮮花一般的笑容
			ELSE
				PRINTFORML 一邊滿臉笑容緊緊地抱著%CALLNAME:MASTER%
			ENDIF
			PRINTL
			SIF FLAG:口上色
				CALLFORM M_KOJO_COLOR_K49
			SELECTCASE RAND:2
				CASE 1
					PRINTFORML 「然後到底怎樣了？」
					PRINTFORMDL 覺兩眼放光地催促著%CALLNAME:MASTER%繼續剛才的話題
					PRINTFORML 「『不會厭倦嗎』？　不會哦ー、因為主様的聲音和心的”聲音”不一樣」
					PRINTFORMDL 覺臉上帶著爽朗的笑容
					PRINTFORML 「主様真是非常棒」
					PRINTFORMDL 如此直接充滿好意的奉承、令%CALLNAME:MASTER%感到有點害羞
				CASE 0
					PRINTFORML 「主様真是知道不少新奇的事呢」
					PRINTFORMDL 貼近著%CALLNAME:MASTER%的覺看起來像感到不可思議一樣
					PRINTFORML 「『你早就知道了吧』？　即是我早就擁有這些知識、但從經驗的人說出來是兩回事」
					PRINTFORMDL 覺臉上帶著歡喜的笑容
					PRINTFORML 「而且、能夠聽到我最喜歡的主様的聲音我也很開心」
					PRINTFORMDL 見到覺的笑容後、決定不再鑽牛角尖了
			ENDSELECT
		ELSE
			DRAWLINE
			PRINTFORM 覺一邊聽著%CALLNAME:MASTER%的話語、
			IF CFLAG:TARGET:陪睡中
				PRINTFORML 一邊帶著神魂傾倒的表情、緊貼著%CALLNAME:MASTER%
			ELSEIF CHK_DATENOW(CFLAG:TARGET:约会中)
				PRINTFORML 一邊和%CALLNAME:MASTER%彼此十指相扣、面露神魂飄蕩的笑容
			ELSE
				PRINTFORML 一邊滿臉笑容、一邊捉住%CALLNAME:MASTER%的胳膊緊貼著這邊
			ENDIF
			PRINTL
			SIF FLAG:口上色
				CALLFORM M_KOJO_COLOR_K49
			SELECTCASE RAND:3
				CASE 2
					PRINTFORML 「主様的聲音、真的很棒呢」
					PRINTFORMDL 看見現在的覺沒有初次見面的冷淡痕跡後、自然的笑出來了
					PRINTFORMW 「……因為那時、我還不信任家人以外的人」
					PRINTFORMDL 現在背叛的話或許會很有趣呀、這樣想著同時、覺害羞地呵呵笑著
					PRINTFORML 「時停所得不到的、被人奉仕帶來的悦楽都捨棄掉也沒有所謂嗎？」
					PRINTFORMDW 現在好想被覺奉仕、%CALLNAME:MASTER%開始這樣想著、目光濕潤的覺只是笑著看著這邊
				CASE 1
					PRINTFORML 「主様真的很喜歡說話呢」
					PRINTFORMDL 覺笑著說道、%CALLNAME:MASTER%自然地苦笑了
					PRINTFORMW 「『這可不一定』？　哼哼、我可是知道的哦、主様你在說謊了」
					PRINTFORMDL 要是我說謊就被雷劈、%CALLNAME:MASTER%這樣回答了
					PRINTFORML 「主様心裏所想的和心聲不一樣。任何謊言都騙不過我的哦」
					PRINTFORMDW 覺語畢、微紅的面带害羞的神情、什麼也不說抱住%CALLNAME:MASTER%
				CASE 0
					PRINTFORML 「主様和我說話時、真的很開心呢」
					PRINTFORMDL 沒有理由會感到開心吧、這樣想著同時、覺笑容變得更深
					PRINTFORMW 「哼哼、那為什麼還要跟我說話呢？」
					PRINTFORMDL 純粹只是一時興起而已、%CALLNAME:MASTER%在內心答道
					PRINTFORML 「哼哼哼、你說是就是啦」
					PRINTFORMDW 覺愉快地說著、%CALLNAME:MASTER%不知怎麼也開始感覺很開心
			ENDSELECT
		ENDIF
		RESETCOLOR
	ELSEIF 陥落状態() == 3
		DRAWLINE
		IF CFLAG:TARGET:陪睡中
			PRINTFORML 才能夠感受到對方體溫的距離下、與歡笑著的覺說話
		ELSEIF CHK_DATENOW(CFLAG:TARGET:约会中)
			PRINTFORML 與歡喜地笑著依偎着%CALLNAME:MASTER%的覺說話了
		ELSE
			PRINTFORML 向看起來很開心地笑著的覺說話了
		ENDIF
		PRINTL
		SIF FLAG:口上色
			CALLFORM M_KOJO_COLOR_K49
		SELECTCASE RAND:3
			CASE 2
				PRINTFORML 「……哼哼、%CALLNAME:MASTER%的聲音聽起來真是美妙」
				PRINTFORMDL 我說的話到底有什麼好、這樣問之前覺已經回答了
				PRINTFORML 「我想是因為所思和所說的一樣吧、感覺很暢快」
				PRINTFORMDW 覺說道後笑著、%CALLNAME:MASTER%沒有再追問下去了
			CASE 1
				PRINTFORML 「%CALLNAME:MASTER%這樣說的話那應該就是這樣吧」
				PRINTFORMDL 看見覺愉快地笑著後、%CALLNAME:MASTER%内心自問道實際上覺又是怎麼想的呢
				PRINTFORML 「哼哼、我所想的包含了你剛才說的一部分、我是這樣想的哦」
				PRINTFORMDW 發現自己被戲弄後、%CALLNAME:MASTER%只能報以苦笑
			CASE 0
				PRINTFORML 「原來如此、真是充滿%CALLNAME:MASTER%風格的話語呢」
				PRINTFORMDL 覺欽佩地點頭、%CALLNAME:MASTER%內心想道那麼簡單就被我騙了
				PRINTFORML 「……雖然戀愛是盲目的、但我的”眼”沒有盲、看穿謊言這種程度還是能做到的」
				PRINTFORMDW 覺竊笑地說道、%CALLNAME:MASTER%見到後嘆氣了
		ENDSELECT
		RESETCOLOR
	ELSEIF 陥落状態() == 2
		PRINTFORML 「……給我滾開混蛋」
		PRINTFORMDL 一開口、覺就這樣說著、%CALLNAME:MASTER%只能收聲嘿嘿地笑著
		PRINTFORML 「『家人真的那麼重要嗎』？　……看來你這個下劣卑賤汚穢之輩根本不懂義理啊」
		PRINTFORMDW 看見眼神冷淡壓抑著憤怒生氣露出咬牙切齒的表情的覺、讓%CALLNAME:MASTER%沒憋住笑意
	ELSEIF 陥落状態() == 1
		SELECTCASE RAND:3
			CASE 2
				PRINTFORML 「原來如此、也有這種事呀」
				PRINTFORMDW 看見覺溫柔的表情、%CALLNAME:MASTER%臉上自然地浮現笑容了
			CASE 1
				PRINTFORML 「嘛、就算是%CALLNAME:MASTER%以外的人也試過失敗呢」
				PRINTFORMDW 對著看上去有點開心的覺、%CALLNAME:MASTER%報以苦笑
			CASE 0
				PRINTFORML 「這可真是、值得去見識一下呢」
				PRINTFORMDW 這樣說著的覺、罕見地嘴角上揚地笑著
		ENDSELECT
	ELSE
		SELECTCASE RAND:3
			CASE 2
				PRINTFORML 「……沒有必要說出來吧？」
				PRINTFORMDW 只是一時興起而已、內心這樣想著時、覺冷淡地看過來
			CASE 1
				PRINTFORML 「嗯、已經聽到了哦。我的”眼”早就已經看見了」
				PRINTFORMDW 覺這樣說著、即使如此還是感覺到覺沒有無視%CALLNAME:MASTER%
			CASE 0
				PRINTFORML 「真是無聊的話題、只有這樣嗎？」
				PRINTFORMDW 周圍蔓延著完全無法繼續話題的氣氛
		ENDSELECT
	ENDIF
	PRINTFORMW 
ENDIF
RETURN 1
;==================================================
;301,お茶を淹れる
;==================================================
@M_KOJO_MESSAGE_COM_K49_301
IF CFLAG:1496 == 2 && !RAND:2
	CALL PET49()
ELSEIF CFLAG:1496 == 1 && !RAND:2
	CALL ONRYO49()
ENDIF
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K49_301_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K49_301_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	IF (BASE:酒気 > (MAXBASE:酒気 / 10) * 4)
		CALL DRUNKER49()
	ELSEIF 陥落状態() >= 4
		IF GETBIT (CFLAG:1490,3)
			IF TALENT:恋慕
				IF 陥落状態() == 5
					PRINTFORML 「主様、今日沒有下藥嗎？　真可惜」
					PRINTFORMDL 聽到覺希望下藥後感到意外的%CALLNAME:MASTER%對覺苦笑
					PRINTFORMW 「因為、主様下藥後就會和我Ｈ了」
					PRINTFORMDL 覺滿臉笑容低聲嘟嚷
					PRINTFORML 「雖然沒有下藥的茶也很好喝」
					PRINTFORMDW 覺語畢開始喝茶了
				ELSE
					PRINTFORML 「主様、茶沒有下藥嗎？」
					PRINTFORMDL 你想我下藥嗎？　、%CALLNAME:MASTER%這樣問道後、覺開始苦笑
					PRINTFORMW 「如果可以的話我敬謝不敬、但如果主様想這樣做的話我沒所謂哦」
					PRINTFORMDL 我覺得現在不是說這種話題的時候
					PRINTFORML 「哼哼、如此任性的主様我也不討厭哦」
					PRINTFORMDW 覺低聲嘟嚷然後開始喝茶了
				ENDIF
			;恋慕も持たずに恋人とな？
			ELSE
				PRINTFORML 「太好了、沒有下藥呢」
				PRINTFORMDL 下了藥你就不會喝了呀、%CALLNAME:MASTER%這樣想著同時、覺微笑開始說道
				PRINTFORMW 「当然。就算我們是戀人關係、也有分可以做的事和不能做的事」
				PRINTFORML 「非常親密的話姑且不論、希望能不要做這種會被人發現的事」
				PRINTFORMDL %CALLNAME:MASTER%沒有回答、悄悄地將自己秘藏的茶點拿出來
				PRINTFORMDW 覺開心地收下了
			ENDIF
		ELSE
			IF 陥落状態() == 5
				PRINTFORML 「嗯哼哼、主様的茶、好好喝好喜歡」
				PRINTFORMDL 覺滿面笑容地說著、%CALLNAME:MASTER%感覺不到任何惡意
				PRINTFORML 「我明明是真的覺得開心、你到底在想什麼呀？」
				PRINTFORMDL 我無法讀覺的心、所以我就只能靠觀察才能知道你是在說真心話還是在奉承我了、%CALLNAME:MASTER%這樣回答
				PRINTFORML 「我討厭這樣的主様」
				PRINTFORMDL %CALLNAME:MASTER%苦笑地撫摸著嘟起嘴的覺的頭
			ELSE
				PRINTFORML 「主様、謝謝你。我很開心」
				PRINTFORMDL 覺收下茶和茶點、愉快地望過來
				PRINTFORMW 「『純粹只是一時興起』？　哼哼、就算只是一時興起我也很開心」
				PRINTFORMDL 覺一邊竊笑一邊拿起茶和茶點
				PRINTFORML 「你泡的茶和那群孩子泡的茶味道不同、能喝到你泡的茶我很開心哦」
				PRINTFORMDW 聽到覺的話後、不打算再說反話了
			ENDIF
		ENDIF
	ELSEIF 陥落状態() == 3 || TALENT:184 || TALENT:185
		IF GETBIT (CFLAG:1490,3)
			PRINTFORML 「啊啦、今天沒有泡藥嗎？」
			PRINTFORMDL 覺竊笑地嘟嚷
			PRINTFORML 「『現在就不要了』？　哼哼、哼哼哼哼、%CALLNAME:MASTER%真是任性呢」
			PRINTFORMDW 覺不知怎麼很開心、一邊竊笑、一邊接過茶開始喝了
		ELSE
			PRINTFORML 「真好喝呢。是%CALLNAME:MASTER%你幫我泡對吧」
			PRINTFORMDL 誰來泡茶也一樣吧、%CALLNAME:MASTER%這樣嘟嚷瞬間、覺笑容變成苦笑了
			PRINTFORML 「不、就算是我家的孩子們、阿燐的話很擅長泡茶、阿空就有待改善了」
			PRINTFORMDW 那兩人的話的確很可能是這樣呢、%CALLNAME:MASTER%内心這樣想著同時、覺苦笑地點頭了
		ENDIF
	ELSEIF 陥落状態() == 2
		IF GETBIT (CFLAG:1490,3)
			PRINTFORML 「『我沒有下藥』……、這不是理所當然嗎」
			PRINTFORMDL 覺冷淡地瞪著這邊
			PRINTFORML 「『今次沒有』？　真是可惜、沒用的哦」
			PRINTFORMDW 覺深深地嘆息、表情呆然
		ELSE
			PRINTFORML 「你不會覺得斟茶遞水這種程度就能籠絡我吧」
			PRINTFORMDL 覺一邊不快地嘟嚷、一邊伸出手接過茶和茶點
			PRINTFORML 「……什麼？」
			PRINTFORMDW 看見接過茶的覺帶有一絲快樂後、%CALLNAME:MASTER%嘿嘿地奸笑著
		ENDIF
	ELSEIF 陥落状態() == 1
		IF GETBIT (CFLAG:1490,3)
			PRINTFORML 「太好了、今日的茶沒有下藥啊」
			PRINTFORMDW 覺微笑含糊不清地說道
		ELSEIF TFLAG:193 == 1
			PRINTFORML 「%CALLNAME:MASTER%很擅長泡茶呢」
			PRINTFORMDW 覺開心地笑著、%CALLNAME:MASTER%也以笑容作為回應了
		ELSEIF TFLAG:193 == -1
			PRINTFORML 「……那個、雖然謝謝你、但這個有點」
			PRINTFORMDW 見到覺困惑後、低頭嚐一嚐茶味道的%CALLNAME:MASTER%因感到難喝如僵住了
		ELSE
			PRINTFORML 「哼哼、謝謝你」
			PRINTFORMDW 覺微笑地接過茶後開始喝茶了
		ENDIF
	ELSE
		IF GETBIT (CFLAG:1490,3)
			PRINTFORML 「以後、也請你不要再下藥了」
			PRINTFORMDW 覺一邊接過茶一邊不愉快地低聲嘟嚷
		ELSEIF TFLAG:193 == 1
			PRINTFORML 「謝謝你……、很好喝」
			PRINTFORMDW 覺少見地稍微有點開心地微笑著
		ELSEIF TFLAG:193 == -1
			PRINTFORML 「……謝了」
			PRINTFORMDW 覺喝了一口茶的瞬間、露出難以形容的表情
		ELSE
			PRINTFORML 「謝謝你」
			PRINTFORMDW 覺表情毫無變化、接過茶並開始喝
		ENDIF
	ENDIF
	CLEARBIT CFLAG:1490,3
ENDIF
RETURN 1
;==================================================
;302,スキンシップ
;==================================================
@M_KOJO_MESSAGE_COM_K49_302
IF CFLAG:1496 == 2 && !RAND:2
	CALL PET49()
ELSEIF CFLAG:1496 == 1 && !RAND:2
	CALL ONRYO49()
ENDIF
SIF (BATHCHECK(CFLAG:MASTER:現在位置) && IN_HOME(CFLAG:MASTER:現在位置) && 陥落状態() < 4) || !BATHCHECK(CFLAG:MASTER:現在位置) || (BATHCHECK(CFLAG:MASTER:現在位置) && (CFLAG:MASTER:現在位置 == 大衆浴場 || CFLAG:MASTER:現在位置 == 旧地獄温泉))
	CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K49_302_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K49_302_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	IF (BASE:酒気 > (MAXBASE:酒気 / 10) * 4)
		CALL DRUNKER49()
	ELSEIF BATHCHECK(CFLAG:MASTER:現在位置) && IN_HOME(CFLAG:MASTER:現在位置)
		IF 陥落状態() >= 4
			DRAWLINE
			IF !RAND:2
				PRINTFORML %CALLNAME:PLAYER%被覺擦背中
				PRINTL
				SIF FLAG:口上色
					CALLFORM M_KOJO_COLOR_K49
				IF 陥落状態() == 5
					PRINTFORML 「主様的後背真美」
					PRINTFORMDL 覺陶醉地說著、%CALLNAME:MASTER%回頭一看就注意到覺含情脈脈地看著這邊
					PRINTFORML 「『已經想要了嗎』？　是、沒ー錯哦。見到主様を見小穴裏面就覺得好熱」
					PRINTFORMDL 聽到那麼赤裸裸的發言%CALLNAME:MASTER%只能苦笑了
					PRINTFORML 「那麼、我來幫你擦背吧。嗯、呼、哈呼……」
					PRINTFORMDL 身上充滿泡泡的覺摩擦著身體、好像在用%CALLNAME:MASTER%的身體自慰但由於背對著覺完全看不見
				ELSE
					PRINTFORML 「那麼、失禮了」
					PRINTFORMDL 覺禮貌地低語說著、用自己肌膚上充滿泡泡的身體壓過來
					PRINTFORMW 「哼哼、嘴上雖然這麼說、但其實覺得很棒吧主様？」
