;==================================================
;311,抱き付く
;TFLAG:193(1=大成功 0=成功 -1=失敗)
;CFLAG:うふふ==2&&TCVAR:20(32=前から抱きつく 60=ディープキス 73=ぱふぱふ 74=乳合わせ 77=乳を吸わせる ＶＡ性交(85=Ｖ挿入する 87=Ａ挿入する 88=腰振りＶ 89=腰振りＡ) 86=だいしゅきホールド)
;TFLAG:400(1=デート先or地底のモブが反応する)
;==================================================
@M_KOJO_MESSAGE_COM_K78_311
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K78_311_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K78_311_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	IF K78_BASIC_CONDITION_IN_MATING()
		IF CFLAG:诶嘿嘿 == 2
			
		ELSEIF CHK_DATENOW(CFLAG:LOCAL:约会中) && CFLAG:MASTER:現在位置 != OMANEKIBEYA()
			
		ELSE
			IF BASE:情緒 >= 500 && BASE:理性 < 975
				;嬉しそうに抱き返してきた…
				IF TALENT:恋慕
					SELECTCASE RAND:2
					CASE 1
						CALL K78_HPH_PRINT,@"「我也会紧紧地抱着你HPH」","W"
					CASEELSE
						CALL K78_HPH_PRINT,@"「呵呵HPH总觉得安心下来了……HPH」","W"
					ENDSELECT
				;照れくさそうにしながらも、満更でもないようだ…
				ELSEIF TALENT:思慕
					SELECTCASE RAND:2
					CASE 1
						PRINTFORML 「唔，动不了了…」
						PRINTFORMDW 这么说着的%CALLNAME:TARGET%也轻轻抱了过来。
					CASEELSE
						PRINTFORMW 「又粘过来了… 哈，就没有一点羞耻感吗」
					ENDSELECT
				;恥ずかしそうにしている…
				ELSE
					IF TALENT:愛欲
						SELECTCASE RAND:2
						CASE 1
							CALL K78_HPH_PRINT,@"「如果寂寞的话我来安慰你吧HPH」","W"
						CASEELSE
							CALL K78_HPH_PRINT,@"「就这样被逆推了么HPH」","W"
						ENDSELECT
					ELSE
						PRINTFORMW 「突，突然干什么呀……羞死人了…」
					ENDIF
				ENDIF
			ELSE
				IF TALENT:恋慕 || TALENT:愛欲
					PRINTFORMW 「突然间干什么呢？」
				ELSEIF TALENT:思慕 || TALENT:炮友
					PRINTFORMW 「唔姆……好热呀」
				ELSE
					PRINTFORMW 「别，别这样　啊啊，好热」
				ENDIF
			ENDIF
		ENDIF
	ENDIF
ENDIF
RETURN 1
;==================================================
;312,キスする
;TFLAG:193(99=拒絶(キス不可) 1=大成功 0=成功 -1=失敗)
;CFLAG:うふふ==2&&TCVAR:20(32=前から抱きつく 50=手淫 60=キス 性交(85=Ｖ挿入する 87=Ａ挿入する 88=腰振りＶ 89=腰振りＡ) 86=だいしゅきホールド)
;PREVCOM(305=膝枕
;TFLAG:400(1=デート先or地底のモブが反応する)
;==================================================
@M_KOJO_MESSAGE_COM_K78_312
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K78_312_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K78_312_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	IF K78_BASIC_CONDITION_IN_MATING()
		IF CFLAG:诶嘿嘿 == 2
			IF TALENT:恋慕 || TALENT:愛欲
				SELECTCASE RAND:3
				CASE 1
					PRINTFORMDL %CALLNAME:MASTER%和%CALLNAME:TARGET%宛如软体动物交尾一般互相缠着舌头，互相交换彼此的唾液。
					CALL K78_HPH_PRINT,@"「ちゅ…ぴちゃ……ちゅぱ……ちゅる…んはぁHPH %CALLNAME:MASTER%，最喜欢了，爱你哦HPH」","W"
				CASE 2
					PRINTFORMDL 被%CALLNAME:TARGET%香甜的吐息所迷住了，只是无意识的自己动着舌头。
					CALL K78_HPH_PRINT,@"「はむ…んちゅっ、ん、んふぅ…れろ……ちゅぷ…はぁHPH 还，不够HPH」","W"
				CASEELSE
					PRINTFORMDL %CALLNAME:MASTER%已经无暇关心其他，如同%CALLNAME:TARGET%的俘虏一般持续渴求着她如同果实一般香甜的唾液。
					CALL K78_HPH_PRINT,@"「嗯唔…啾噜…啾HPH …啾噜，啾噜噜……啾哈HPH」","W"
				ENDSELECT
			ELSE
				SELECTCASE RAND:2
				CASE 1
					PRINTFORMW 「啾唔……唔……哈……唔姆…啾…唔」
				CASEELSE
					PRINTFORMW 「嗯…啾噗……嗯啾……嗯哈……哈唔…啾」
				ENDSELECT
			ENDIF
		;キス拒絶ではない
		ELSEIF TFLAG:193 != 99
			IF BASE:情緒 >= 650 && BASE:理性 < 875
				IF TALENT:恋慕 || ( TALENT:愛欲 && ( ABL:親密 >= 5 || ABL:欲望 >= 5 ) )
					SELECTCASE RAND:2
					CASE 1
						CALL K78_HPH_PRINT,@"「ちゅっ、ちゅぱ……んちゅ、んむ……んふHPH 结束了吗？HPH」","W"
					CASEELSE
						CALL K78_HPH_PRINT,@"「あむっ……んちゅ…ぺろ……ちゅ…ちゅぱHPH 嘿嘿，kiss的感觉真好呐HPH」","W"
					ENDSELECT
				ELSEIF ABL:親密 >= 5 || ABL:欲望 >= 5
					SELECTCASE RAND:2
					CASE 1
						PRINTFORMW 「嗯……嗯唔…啾……哈…」
					CASEELSE
						PRINTFORMW 「嗯啾……啾…嗯……啾…」
					ENDSELECT
				ELSE
					SELECTCASE RAND:2
					CASE 1
						PRINTFORML 「嗯唔，啾……嗯哈……哈，啾……」
						PRINTFORMDW %CALLNAME:TARGET%说不出话，脸上一片通红。
					CASEELSE
						PRINTFORML 「嗯唔……嗯啾…唔……啾…」
						PRINTFORMDW %CALLNAME:TARGET%虽然一瞬间想抵抗，但被%CALLNAME:MASTER%抱着肩膀一动不动只能默许了。
					ENDSELECT
				ENDIF
			ELSE
				IF TALENT:恋慕 || ( TALENT:愛欲 && ( ABL:親密 >= 5 || ABL:欲望 ) )
					PRINTFORMW 「嗯……啾…啾噜……唔，太乱来了」
				ELSE
					PRINTFORMW 「嗯嗯！？ 突，突然干什么啊！」
				ENDIF
			ENDIF
		ENDIF
	ENDIF
ENDIF
RETURN 1
