			;関係普通　女あなたのみ…？
			PRINTFORMW 「ちょっとちょっと、何入ってきてるのよ！」
		CASE 13,14,23,24
			;関係普通:不機嫌か怒ってる　女あなたのみ…？
			PRINTFORMW 「しれっと入ってきてんじゃないわよ！」
		CASE 31,32,61,62
			;関係思慕　女あなたのみ…？
			PRINTFORMW 「ちょっと、入ってこないで！」
		CASE 33,63
			;関係思慕:不機嫌　女あなたのみ…？
			PRINTFORMW 「なんで入ってこようとしてるの！」
		CASE 34,64
			;関係思慕:怒ってる　女あなたのみ…？
			PRINTFORMW 「でてけーーーー！！！」
		CASE 41,42,71,72,51,52
			;関係恋慕以上　
			PRINTFORMW 「あ、%CALLNAME:MASTER%！？ あ、その、じゃ、なくって、出てって！」
		CASE 43,73,53
			;関係恋慕以上:不機嫌
			PRINTFORMW 「あ、%CALLNAME:MASTER%！？ 出て行ってよ、もう！」
		CASE 44,74,54
			;関係恋慕以上:怒ってる
			PRINTFORMW 「あ、%CALLNAME:MASTER%！？ は、早く出ていきなさいよ！！」
		ENDSELECT
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;外出先で遭遇した
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 6
		PRINTFORML 
		PRINTFORM 「あら
		SIF ABL:親密 >= 5
			PRINTFORM %CALLNAME:MASTER%
		PRINTFORMW 、%M_KOJO_K103_AISATSU()%」
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;他キャラとデート中に遭遇した
	;ARG:1にデート相手
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 7
		PRINTFORML 
		IF ARG:1 == 102
			PRINTFORMW 「あ、神綺様、それと%CALLNAME:MASTER%、%M_KOJO_K103_AISATSU()%\@ TIME <= CLOCK_TO_TIME(10, 0) ? "ございます" # \@」
		ELSEIF ARG == 17
			PRINTFORMW 「あ、アリスと%CALLNAME:MASTER%、%M_KOJO_K103_AISATSU()%」
		ELSE
			PRINTFORMW 「あ、%CALLNAME:MASTER%と…%CALLNAME:ARG%？ …%M_KOJO_K103_AISATSU()%」
		ENDIF
		RETURN 1
	ENDIF
	;その他
	PRINTFORML 
	;普通に遭遇した場合は時止め後処理をリセットする
	CALL M_KOJO_K103_RESETTW
ENDIF
RETURN 1

