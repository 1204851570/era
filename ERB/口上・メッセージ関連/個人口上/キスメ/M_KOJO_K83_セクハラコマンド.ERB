;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	CALL M_KOJO_COLOR_K83
	;時間停止中
	IF FLAG:時間停止
	;睡眠中
	ELSEIF CFLAG:睡眠
	;押し倒され中
	ELSEIF CFLAG:[[琪斯美]]:诶嘿嘿 == 2
		SIF !M_KOJO_K83_UFUFU([[琪斯美]])
			RETURN 1
		PRINTFORMW 「ふふ……まってて……」
	;酔ってる
	ELSEIF M_KOJO_K83_YOIDORE([[琪斯美]])
		CALL M_KOJO_K83_TALKSET("酔酒")
	;不機嫌
	ELSEIF M_KOJO_K83_FUKIGEN([[琪斯美]])
		IF TALENT:[[琪斯美]]:恋慕
			PRINTFORMW 「%CALLNAME:MASTER%のいじわる……」
		ELSE
			PRINTFORMW 「うぇぇ……ひ、ひどい……」
		ENDIF
	;通常
	ELSE
		IF TALENT:[[琪斯美]]:恋慕
			PRINTFORMW 「%M_KOJO_K83_AUTO("悲鳴：弱")%」
			SELECTCASE RAND:5
			CASE 1
				PRINTFORMW 「え……えっち！」
			CASE 2
				PRINTFORMW 「……ばか」
			CASE 3
				PRINTFORMW 「きゅ、急にしないで……」
			CASE 4
				PRINTFORMW 「び、びっくりした……」
			CASEELSE
				PRINTFORMW 「もう……」
			ENDSELECT
		ELSEIF TALENT:[[琪斯美]]:思慕
			PRINTFORMW 「%M_KOJO_K83_AUTO("悲鳴：強")%」
			SELECTCASE RAND:3
			CASE 1
				PRINTFORMW 「な、なに……？」
			CASE 2
				PRINTFORMW 「な、なんで……？」
			CASEELSE
				PRINTFORMW 「や、やだ……」
			ENDSELECT
		ELSE
			PRINTFORMW 「ひゃぁああっ！！」
			PRINTFORMW 「や、やめて…………」
		ENDIF
	ENDIF
ENDIF
RETURN 1
