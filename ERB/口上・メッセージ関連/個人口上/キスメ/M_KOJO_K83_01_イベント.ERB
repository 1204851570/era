;==================================================
;相手が押し倒しを解除
;==================================================
@M_KOJO_EVENT_K83_30(ARG,ARG:1)
;口上主はTARGET
;--------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = M_KOJO_K83_UFUFU([[琪斯美]])
;-------------
IF LOCAL
	;-------------------------------------------------
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
	;疲れた
		PRINTFORML 「はぁ、はぁ……つ、疲惫ぁ……」
		PRINTFORMW 「ご、ごめんね……%CALLNAME:MASTER%……」
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
	;満足して解放
		PRINTFORMW 「はぁ、はぁ……も、もう、十分……」
		PRINTFORMW 「%CALLNAME:MASTER%……あ、ありがとう……」
		TCVAR:[[琪斯美]]:351 = 0
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 3
	;満足したところを押し倒された
		PRINTFORMW 「ひゃっ！！」
		IF TALENT:[[琪斯美]]:恋人
			SELECTCASE RAND:2
			CASE 1
				CALL HPH_PRINT(@"「ん……おしおきして……HPH」", "W")
			CASEELSE
				CALL HPH_PRINT(@"「ん……かわりばんこね……HPH」", "W")
			ENDSELECT
		ELSE
			SELECTCASE RAND:2
			CASE 1
				PRINTFORMW 「うぅ……ご、ごめんなさいぃ……！」
			CASEELSE
				PRINTFORMW 「うぅ……ゆ、ゆるしてぇ……！」
			ENDSELECT
		ENDIF
		TCVAR:[[琪斯美]]:351 = 0
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 4
	;壊れるまでしてあげる
		IF TALENT:[[琪斯美]]:恋人
			SELECTCASE RAND:2
			CASE 1
				CALL HPH_PRINT(@"「%CALLNAME:MASTER%……好きなんだね……HPH」", "W")
			CASEELSE
				CALL HPH_PRINT(@"「じゃあ、いっぱい遊んであげる……HPH」", "W")
			ENDSELECT
		ELSE
			SELECTCASE RAND:2
			CASE 1
				CALL HPH_PRINT(@"「後悔しても遅いよ……HPH」", "W")
			CASEELSE
				CALL HPH_PRINT(@"「ふふ……それじゃあ、遠慮不会……HPH」", "W")
			ENDSELECT
		ENDIF
		RETURN 1
	ENDIF
ENDIF
RETURN 1

