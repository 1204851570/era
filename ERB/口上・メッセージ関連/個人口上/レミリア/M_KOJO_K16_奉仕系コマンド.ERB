;80,手淫
;==================================================
@M_KOJO_MESSAGE_COM_K16_80
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K16_80_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K16_80_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		IF TALENT:無知
			PRINTFORMW 「何よコレ……？これ触ればいいの？」
			PRINTFORMW  蕾米莉亜は粗末な物の様に触れる
			PRINTFORML 「こんな褒美で良いなんて変な奴ね……」
		ELSE
			PRINTFORML 「ふぅ……ふふっ……ビクンってしたわね」
			PRINTFORMW 「まったく……こんな粗末な物触って欲しいなんて……とんだ無礼者ね……%UNICODE(0x2764)%」
		ENDIF
		RETURN 1
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMW 
		RETURN 1
	;恋慕
	ELSEIF TALENT:無知
		PRINTFORML 「また手でいいの？」
		IF TALENT:恋慕
			SELECTCASE RAND:2
			CASE 0
				PRINTFORML 「わかったわ%UNICODE(0x2764)%」
				PRINTFORMW 「ホラ！早く白いのびゅーって出しなさい」
			CASE 1
				PRINTFORML 「まだなの？早く出しなさいよ！」
			ENDSELECT
		ELSE
			PRINTFORML 「本当にこんなので良いの？」
			SELECTCASE RAND:2
			CASE 0
				PRINTFORMW 「握りつしちゃいそうだわ……これ」
			CASE 1
				PRINTFORMW 「仕方ないわね……これでいいかしら？」
			ENDSELECT
		ENDIF
	ELSE
		PRINTFORML 「手でしてほしいの？」
		IF TALENT:恋慕
			SELECTCASE RAND:3
			CASE 0
				PRINTFORML 「いいわ……」
				PRINTFORMW 「%CALLNAME:MASTER%のリアクションもたっぷり見れるしね%UNICODE(0x2764)%」
			CASE 1
				PRINTFORML 「ふふっ……こんな粗末な物シコシコして欲しいだなんて……%UNICODE(0x2764)%」
				PRINTFORMW 「ホラっ%UNICODE(0x2764)%無駄射精しろ%UNICODE(0x2764)%」
			CASE 2
				PRINTFORML 「こんな凶悪的物触らせて……%UNICODE(0x2764)%」
				SELECTCASE RAND:2
				CASE 0
					PRINTFORMW 「これで私を犯したいのでしょう……ならもっと硬くしなさい……%UNICODE(0x2764)%」
				CASE 1
					PRINTFORML 「先っぽ弄るだけでこんなに暴れるなぁんて……」
					PRINTFORMW 「普段と違って暴れん坊ね……%UNICODE(0x2764)%」
				ENDSELECT
			ENDSELECT
		ELSE
			SELECTCASE RAND:3
			CASE 0
				PRINTFORML 「フフッ……私の手に余る大きさね……%UNICODE(0x2764)%」
			CASE 1
				PRINTFORML 「手だけで満足かしら……？」
			CASE 2
				PRINTFORML 「ほら……溜まっているの出してしまいない……%UNICODE(0x2764)%」
			ENDSELECT
		RETURN 1
		ENDIF
		RETURN 1
	ENDIF
ENDIF
RETURN 1

