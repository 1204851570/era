IF (MAIN_MAP !=9 && ARG == 960) || (IN_HOME(ARG) && !CHK_FOCUS(927,CFLAG:MASTER:現在位置,948) && (ROOMDATA:(CFLAG:MASTER:現在位置 % 100) & 場所_モブ))
	IF GROUPMATCH(TIME:2,1,6,7)
		PRINTFORML 在附近睡着的%ANIMALNAME()%、发出了很大的鼾声
	ELSE
		PRINTFORM 在附近的%ANIMALNAME()%、
		SELECTCASE TIME:1 % 25
			;小型哺乳類
			CASE 0 TO 7
				SELECTCASE RAND:4
					CASE 3
						PRINTFORML 把身体靠了過来不停蹭着
					CASE 2
						PRINTFORML 一脸不可思議的看了过来
					CASE 1
						PRINTFORML 伸了伸懒腰
					CASE 0
						PRINTFORML 砰砰地叩着地板
				ENDSELECT
			;大型哺乳類
			CASE 8 TO 13
				SELECTCASE RAND:4
					CASE 3
						PRINTFORML 抱着好奇心把脸凑了过来
					CASE 2
						PRINTFORML 好像正一動不動地看着這邊
					CASE 1
						PRINTFORML 轻轻地摇晃着身体
					CASE 0
						PRINTFORML 咚咚地叩着地板
				ENDSELECT
			;超大型哺乳類（屋内にはいってこれないんじゃないのか？
			CASE 14 TO 15
				SELECTCASE RAND:2
					CASE 1
						PRINTFORML 一動不動地俯視着這邊
					CASE 0
						PRINTFORML 不時踏着腿
				ENDSELECT
			;鳥類
			CASE 16 TO 20
				SELECTCASE RAND:5
					CASE 4
						IF TIME:1 % 25 == 18
							PRINTFORML 一動不動地
						ELSE
							PRINTFORML \@ TIME:1 % 25 > 18 ? 呼啦呼啦 # 啪塔啪塔 \@的扇着翅膀
						ENDIF
					CASE 3
						IF TIME:1 % 25 == 18
							PRINTFORML 一動不動地
						ELSE
							PRINTFORML \@ TIME:1 % 25 > 18 ? 在鳴叫著 # 頻頻啼鳴 \@
						ENDIF
					CASE 2
						IF TIME:1 % 25 == 18
							PRINTFORML 一動不動地
						ELSE
							PRINTFORML 靠到身邊、想要被撫摸而伸出了頭
						ENDIF
					CASE 1
						IF TIME:1 % 25 == 18
							PRINTFORML 一動不動地
						ELSE
							PRINTFORML 在四周到処活動
						ENDIF
					CASE 0
						IF TIME:1 % 25 == 18
							PRINTFORML 好像対%CALLNAME:MASTER%打着招呼、深深地低下了頭
						ELSE
							PRINTFORML 発出威嚇
						ENDIF
				ENDSELECT
			;爬虫類
			CASE 21 TO 24
				SELECTCASE RAND:3
					CASE 2
						PRINTFORML 迅速爬到附近凝視着這邊
					CASE 1
						PRINTFORML 張開嘴発聲威嚇
					CASE 0
						PRINTFORML 感興趣的搖晃着身体
				ENDSELECT
		ENDSELECT
	ENDIF
ELSEIF IN_HOME(ARG) && CHK_FOCUS(927,CFLAG:MASTER:現在位置,930) && (ROOMDATA:(CFLAG:MASTER:現在位置 % 100) & 場所_モブ)
	PRINTFORM 附近的怨霊、
	SELECTCASE RAND:5
		CASE 4
			PRINTFORMDL 想要靠近%CALLNAME:TARGET%、被%CALLNAME:MASTER%趕走了
		CASE 3
			PRINTFORMDL 看到%CALLNAME:TARGET%靠近之後匆忙逃走了
		CASE 2
			PRINTFORMDL 在%CALLNAME:TARGET%四周飛來飛去、然後直接逃走了
		CASE 1
			PRINTFORMDL 看到%CALLNAME:TARGET%後加快了動作、但是無法靠近%CALLNAME:MASTER%那側
		CASE 0
			PRINTFORMDL 完全不在意%CALLNAME:TARGET%的目光的樣子、毫無反應的在空中飄蕩
	ENDSELECT
ELSE
	PRINTFORM 一旁的%DATE_HITOGOMI_SYUBETU(CFLAG:PLAYER:現在位置)%、
	SELECTCASE RAND:7
		CASE 6
			PRINTFORML 露出了呆愣的表情
		CASE 5
			PRINTFORML 慌忙背过脸去
		CASE 4
			PRINTFORML 嘟哝着别秀给人看啊
		CASE 3
			PRINTFORML 露出惊讶的表情
		CASE 2
			PRINTFORML 不断地偷瞄着
		CASE 1
			PRINTFORML 红着脸移开了視線
		CASE 0
			PRINTFORML 怀疑自己的眼睛使劲揉了揉眼皮
	ENDSELECT
ENDIF
