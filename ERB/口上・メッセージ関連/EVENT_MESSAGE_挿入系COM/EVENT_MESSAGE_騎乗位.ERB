;65,騎乗位
;-------------------------------------------------
@MESSAGE_COM65
;抜かず
IF FLAG:70 == 1
	IF EXP:MASTER:時姦経験 >= EXP:TARGET:愛情経験 + 50
		IF TCVAR:100 == PLAYER && TARGET == TFLAG:103
			IF TALENT:体型 == -1
				IF NOWEX:MASTER:射精
					PRINTFORML 快要高潮了的%CALLNAME:PLAYER%、抓住被%TINKO(PLAYER)%插入的静止在空中的%CALLNAME:TARGET%的腰
					PRINTFORML 用像要把那窄小膣穴捅破的气势、开始向上耸动起了腰…
				ELSEIF TCVAR:102
					PRINTFORML 对時停中的%CALLNAME:TARGET%射出精液的%CALLNAME:MASTER%
					PRINTFORML 再次为了索求快楽开始侵犯起了%CALLNAME:TARGET%的稚嫩膣穴…
				ELSEIF TEQUIP:体位２ == 3
					PRINTFORML 让時停中的%CALLNAME:TARGET%保持骑在自己身上的姿势、再次开始向上耸动起了腰…
				ELSE
					PRINTFORML 保持%TINKO(PLAYER)%插入%CALLNAME:TARGET%的稚嫩膣穴并使其仰面躺下
					PRINTFORML 在不拔出%TINKO(PLAYER)%的前提下最大限度的高高托起%CALLNAME:TARGET%的身体
					PRINTFORML 为侵犯静止在空中的%CALLNAME:TARGET%的膣穴开始把腰向上耸动起来…

				ENDIF
			ELSEIF NOWEX:MASTER:射精
				PRINTFORML %CALLNAME:PLAYER%从下面向上耸動着腰侵犯起時停中静止在空中的%CALLNAME:TARGET%的膣穴
				PRINTFORML 快要高潮的%CALLNAME:PLAYER%抓住%CALLNAME:TARGET%的腰、更加激烈地开始向上动着腰…
			ELSEIF TCVAR:102
				PRINTFORML 对時停中的%CALLNAME:TARGET%射出精液的%CALLNAME:MASTER%
				PRINTFORML 再次为了索求快楽开始侵犯起了身体无法动弹的%CALLNAME:TARGET%…
			ELSEIF TEQUIP:体位２ == 3
				PRINTFORML 让時停中的%CALLNAME:TARGET%保持骑在自己身上的姿势、再次开始向上耸动起了腰…
			ELSE
				PRINTFORML 没从被当做人肉飞机杯用的%CALLNAME:TARGET%的膣穴中拔出并使其跨坐到自己身上、再次开始抽插起了膣穴…
			ENDIF
		ELSEIF TALENT:体型 == -1
			PRINTFORML %CALLNAME:PLAYER%钻到時停中静止在空中的%CALLNAME:TARGET%的身体下方対
			SIF PALAM:潤滑 >= PALAMLV:4
			PRINTFORM 被粘液弄湿的
			SIF TALENT:処女 == 1
			PRINTFORM 接触男人还为时尚早的
			PRINTFORML 稚嫩竖缝把%TINKO(PLAYER)%刺了进去、用力的向上聳動著腰
			PRINTFORML %CALLNAME:TARGET%保持着两腿张开静止在空中的姿势、像固定住的飞机杯般被%CALLNAME:PLAYER%使用着稚嫩的膣穴…
		ELSE
			PRINTFORML %CALLNAME:PLAYER%钻到時停中静止在空中的%CALLNAME:TARGET%的身体下方対
			SIF PALAM:潤滑 >= PALAMLV:4
			PRINTFORM 滴着愛液的
			SIF TALENT:処女 == 1
			PRINTFORM 不识男人滋味的
			;小柄体型
			IF TALENT:体型 < 0
				PRINT 幼小裂缝
			ELSE
				PRINT 秘裂
			ENDIF
			PRINTFORML 把%TINKO(PLAYER)%刺了进去、用力把腰向上送着
			PRINTFORML %CALLNAME:TARGET%保持着两腿张开静止在空中的姿势、像固定住的飞机杯般被%CALLNAME:PLAYER%使用着膣穴…
		ENDIF
	ELSE
		IF TCVAR:100 == PLAYER && TARGET == TFLAG:103
			IF NOWEX:MASTER:射精
				PRINTFORML %CALLNAME:PLAYER%让時停中的%CALLNAME:TARGET%保持跨在自己身上的姿势、
				PRINTFORML 顺从着上涌的衝動向上耸动起了腰部…
			ELSEIF TCVAR:102
				PRINTFORML 在時停中的%CALLNAME:TARGET%的膣内射入精液之后、
				PRINTFORML 趁着%TINKO(PLAYER)%尚未失去硬度再次开始向上动起了腰…
			ELSEIF TEQUIP:体位２ == 3
				PRINTFORML 让時停中的%CALLNAME:TARGET%保持跨坐在自己身上的姿势、再次开始向上动起了腰…
			ELSE
				PRINTFORML 不从膣内拔出并让時停中的%CALLNAME:TARGET%跨坐在自己身上、再次开始向上动起了腰…
			ENDIF
		ELSE
			PRINTFORML %CALLNAME:PLAYER%打开時停中的%CALLNAME:TARGET%的大腿、让%CALLNAME:TARGET%跨坐在自己身上対著
			SIF PALAM:潤滑 >= PALAMLV:4
			PRINTFORM 滴着愛液的
			SIF TALENT:処女 == 1
			PRINTFORM 不识男人滋味的
			;小柄体型
			IF TALENT:体型 < 0
				PRINT 幼小裂缝
			ELSE
				PRINT 秘裂
			ENDIF
			PRINTFORML 将%TINKO(PLAYER)%捅了进去…
		ENDIF
	ENDIF
ELSE
	IF TCVAR:100 == PLAYER && TARGET == TFLAG:103
		IF NOWEX:MASTER:射精
			PRINTFORML %CALLNAME:PLAYER%保持让%CALLNAME:TARGET%跨坐着的状态、
			PRINTFORML 顺从着上涌的衝動向上耸动起了腰部…
		ELSEIF TCVAR:102
			PRINTFORML 在%CALLNAME:TARGET%的膣内射入精液之后、
			PRINTFORML 趁着%TINKO(PLAYER)%尚未失去硬度再次开始动起了腰…
		ELSEIF TEQUIP:体位２ == 3
			PRINTFORML 让%CALLNAME:TARGET%夹紧挿入着的%TINKO(PLAYER)%、再次开始动起了腰…
		ELSE
			PRINTFORML 不从膣内拔出并抱起%CALLNAME:TARGET%、从下往上动起了腰…
		ENDIF
	ELSE
		IF TALENT:処女 == 1
			PRINTFORM %CALLNAME:TARGET%骑在%CALLNAME:PLAYER%身上、慢慢的沉下腰
			;処女＋小柄体型
			IF TALENT:体型 < 0
				PRINTFORML 、不识男人滋味的幼いワレメで%TINKO(PLAYER)%を飲み込んでいった…
			;処女
			ELSE
				PRINTFORML 、不识男人滋味的秘裂で%TINKO(PLAYER)%を飲み込んでいった…
			ENDIF
		ELSEIF MASTER_POSE(SET_V,SET_P,1) && MASTER_POSE(SET_V,SET_P,1) != TARGET
			PRINTFORML %CALLNAME:TARGET%用自己的膣内慢慢地呑入了%CALLNAME:(MASTER_POSE(SET_V,SET_P,1))%裸露出來的%TINKO(PLAYER)%…
		ELSE
			PRINTFORML %CALLNAME:TARGET%骑在%CALLNAME:PLAYER%身上、慢慢的把腰沉了下去…
		ENDIF
