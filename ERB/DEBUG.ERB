@DEBUG_MODE

$DEBUG_BEGIN

DRAWLINE
PRINTL 请选择要修改人物，或进入FLAG/自定义修正
DRAWLINE
LOCAL:1 = 0
FOR LOCAL,0,CHARANUM
	SIF NAME:LOCAL == ""
		CONTINUE
	PRINTFORMLC [{LOCAL,3,RIGHT}] %NAME:LOCAL%
	LOCAL:1 += 1
	SIF LOCAL:1 % 4 == 0
		PRINTL
NEXT

SIF LOCAL:1 % 4 != 0
	PRINTL

DRAWLINE
PRINT [500] - 全局参数　　　　　
PRINT [501] - 道具修改　　　　　
PRINTL [600] - 杂项修改　　　　　
;PRINTFORML [803] - 特殊模式：\@ TFLAG:301 > 0 ? [使用中] # [关闭中] \@
DRAWLINE
PRINTFORML [999] - 离开

$INPUT_LOOP

INPUT

IF RESULT == 999
	RETURN 0
ELSEIF RESULT == 600
	CALL SHOW_OTHER_DEBUG
	GOTO DEBUG_BEGIN
ELSEIF RESULT == 500
	CALL SHOW_FLAG_DEBUG
	GOTO DEBUG_BEGIN
ELSEIF RESULT == 501
	CALL ITEM_DEBUG
	GOTO DEBUG_BEGIN
ELSEIF RESULT == 803
	CALL ESTRUS_DEBUG
	GOTO DEBUG_BEGIN
ELSEIF RESULT >= CHARANUM || RESULT < 0 || NAME:RESULT == ""
	CLEARLINE 1
	GOTO INPUT_LOOP
;ELSEIF RESULT < 14
;	CALL SHOW_COUNTRY_DEBUG,RESULT
;	GOTO DEBUG_BEGIN
ELSE
	CALL SHOW_SLAVE_DEBUG,RESULT
	GOTO DEBUG_BEGIN
ENDIF

