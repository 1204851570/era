LOCALS = {JUEL:選択中キャラID:100}
SIF JUEL:選択中キャラID:100 > 1000000
	LOCALS = {JUEL:選択中キャラID:100 / 1000}k
PRINTFORM %PALAMNAME:100,6%の珠:%LOCALS,7,LEFT%

