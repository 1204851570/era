@LOOK_AT
#DIM 装備部位
#DIM 着衣状況
#DIMS 挿入状況
#DIMS 挿入状況2
#DIMS 表示内容
CALL PRINTBUTTON_EX(TFLAG:LOOK非表示 ? "▽[＋][Look]----------" # "▼[－][Look]----------", 3003, TFLAG:LOOK非表示)
PRINTBUTTON "[画像表示]", 816
SIF FLAG:画像表示設定
	SETCOLOR C_ORANGE
PRINTBUTTON "[表示設定]", 3100
RESETCOLOR
IF CFLAG:诶嘿嘿
	IF FLAG:挿絵表示
		PRINTBUTTON  "[動畫顯示-ON-]", 817
	ELSE
		SETCOLOR C_L_GRAY
		PRINTBUTTON  "[動畫顯示-OFF-]", 817
		RESETCOLOR
	ENDIF
ENDIF
PRINTL 
