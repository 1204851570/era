;裤子
;要素名→ID変換用関数の存否確認用関数
;変換可能（IDと要素名の関係が常に変化しない）なら設置
;状況によって名前が変わるようなのは不可能
@EXIST_裤子_ID
#LOCALSIZE 1
#LOCALSSIZE 1

;要素名→ID変換用関数
;必要なら各オブジェクトで同様に作成のこと
@裤子_ID(ARG:0, ARGS:0, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS V_NAME                  ;作成する変数名
#DIMS C_NAME, OBJ_ID_LAST + 1 ;データ保持
#DIM  O_ID                    ;参照用
IF ARGS:0 == "RESET"
	VARSET C_NAME
	FOR O_ID, 1, OBJ_ID_LAST + 1
		C_NAME:O_ID = %GET_STR(, "裤子", O_ID, "名前")%
	NEXT
ELSE
	O_ID = FINDELEMENT(C_NAME, ESCAPE(ARGS:0), , , 1)
	CALLF MAKE_INT(V_NAME, O_ID)
ENDIF



@EXIST_裤子1
#LOCALSIZE 1
#LOCALSSIZE 1

@裤子1(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "名前"
	CALLF MAKE_STR(V_NAME, "裤子")
CASE "装備部位"
	CALLF MAKE_STR(V_NAME, "「裤子」")
;旧装備処理との照会用、ズボンの1
ENDSELECT



