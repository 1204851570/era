#LOCALSSIZE 1

;オブジェクト本体
@衣装セット1(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "名前"
	CALLF MAKE_STR(V_NAME, "男裝套")
CASE "基本套装"
	CALLF MAKE_INT(V_NAME, 1)

;こっから衣装設定系
CASE "靴"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("靴", "靴"))
CASE "襪子"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("襪子", "襪子"))
CASE "裤子"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("裤子", "裤子"))
CASE "上半身内衣１"
;漢女はサラシをつける
	IF GETBIT(TALENT:ARG:2, 0)
		CALLF MAKE_INT(V_NAME, CLOTHES_ID("上半身内衣１", "裹胸"))
	ELSE
		CALLF MAKE_INT(V_NAME, CLOTHES_ID("上半身内衣１", "Ｔ恤"))
	ENDIF
CASE "上半身外衣１"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("上半身外衣１", "外衣"))
ENDSELECT



