@DRAW_MAP_BG_6(SLICED)
#DIMS SLICED
SELECTCASE SLICED
;色つけたいときはこんな感じ
CASE "木","林"
	SETCOLOR 0x90EE90
	PRINTS SLICED
	RESETCOLOR
CASE "Ψ"
	SETCOLOR 0xc53d43
	PRINTS SLICED
	RESETCOLOR
CASE "骨"
	SETCOLOR 0x8b474b
	PRINTS SLICED
	RESETCOLOR
;違う文字を表示させる例
CASE "#W"
	SETCOLOR 0x90EE90
	PRINT 木
	RESETCOLOR
CASE "#E"
	SIF TALENT:MASTER:居場所察知 && COUNTCHARA:(MAXROOM) > 0 && FLAG:察知模式 == 1
		SETCOLOR 255,100,200
	PRINT ↓
	RESETCOLOR
;ふつうの
CASEELSE
	PRINTS SLICED
ENDSELECT