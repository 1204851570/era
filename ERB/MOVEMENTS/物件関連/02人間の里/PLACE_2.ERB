	CASE 201 TO 207, 219, 220
		RETURNF 210
	CASE 225
		RETURNF 260
	CASE 213, 215, 216, 226, 227
		RETURNF 220
	CASE 211, 212, 217, 218
		RETURNF 230
	CASE 222, 224
		RETURNF 240
	CASE 223, 228
		RETURNF 250
	CASE 214, 221, 230 TO 238
		RETURNF 270
ENDSELECT
