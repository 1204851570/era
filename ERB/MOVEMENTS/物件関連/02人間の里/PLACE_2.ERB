@MAP_UNIQUEMESSAGE_2
IF WITH_MOB()
	RETURN 1
ELSE
	SELECTCASE CFLAG:MASTER:現在位置
		CASE 205
			IF INRANGE(TIME, 540, 1020)
				PRINTFORML 能看到充滿干勁的河童在接待索道的業務
			ELSE
				PRINTFORML 索道站关门了
			ENDIF
		CASE 213
			PRINTFORML 店內排列著很多書架
		CASE 222
			IF TIME >= 480 && TIME <= 720 || TIME >= 780 && TIME <= 840 && FLAG: 70 != 1
				PRINTDATA
					DATAFORM 寺子屋的孩子们在听%CALLNAME:TARGET%讲课…
				ENDDATA
				PRINTL
			ELSEIF TIME >= 720 && TIME <= 780 && FLAG:70 != 1
				PRINTDATA
					DATAFORM 寺子屋的孩子們和%CALLNAME:TARGET%正在吃飯…
				ENDDATA
				PRINTL
			ELSEIF TIME >= 480 && TIME <= 840 && FLAG:70 == 1 && !CFLAG:MASTER:诶嘿嘿
				PRINTDATA
					DATAFORM 寺子屋的孩子們停下了動作…
				ENDDATA
				PRINTL
			ELSEIF TIME >= 480 && TIME <= 840 && FLAG:70 == 1 && CFLAG:MASTER:诶嘿嘿
				PRINTDATA
					DATAFORM %CALLNAME:TARGET%在私塾的孩子們面前被襲撃了…
				ENDDATA
				PRINTL
			ENDIF
		CASE 203 TO 206
			SIF !RAND:50 && INRANGE(TIME, 540, 1020)
				PRINTFORML 拿着薬袋的胡渣男、正在向路人熱心地说着什么
	ENDSELECT
ENDIF
RETURN 1


