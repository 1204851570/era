;FileName_MOVEMENT.ERB ----------------------------- Rev1.00
;キャラ別_各種データ
;121～130番までのキャラ
;CALL		USER
;ARG		0:呼出データNO, 1:移動傾向取得時の部屋NO
;RETURN		キャラ別呼出データ値
;COMMENT	
;-----------------------------------------------------------
;成美
@CHARAMOVE_DATA_121(ARG,ARG:2)
LOCAL = 121
SELECTCASE ARG
	;移動対象メンバーチェック
	CASE 1
	;移動確率
	CASE 2
	;起床時間帯
	CASE 3
		RETURN 睡眠時間(LOCAL)
	;移動傾向
	CASE 4
		SELECTCASE ARG:2
			;とくになし
			CASEELSE
				RETURN 0
		ENDSELECT
	;仕事時間帯
	CASE 5
	;行く地域(0=博1=寺2=里3=湖4=竹5=森6=冥7=山8=守9=地10=月）
	CASE 6
		SELECTCASE ARG:2
			CASE 5
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
	;出現場所
	CASE 7
		SELECTCASE ARG:2
			CASE 51, 52, 53, 54, 56, 57
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
ENDSELECT

;舞
@CHARAMOVE_DATA_122(ARG,ARG:2)
LOCAL = 122
SELECTCASE ARG
	;移動対象メンバーチェック
	CASE 1
		;連動処理（里乃の方が高いときは里乃を参照）
		IF TALENT:123:恋慕 || (TALENT:123:思慕 && !TALENT:123:思慕)
			CFLAG:LOCAL:現在位置 = CFLAG:123:遭遇位置 ? CFLAG:123:遭遇位置 # SUKIMA()
		ELSE
			CFLAG:LOCAL:現在位置 = CFLAG:122:遭遇位置 ? CFLAG:122:遭遇位置 # SUKIMA()
		ENDIF
	;移動確率
	CASE 2
	;起床時間帯
	CASE 3
		RETURN 睡眠時間(LOCAL)
	;移動傾向
	CASE 4
		SELECTCASE ARG:2
			;とくになし
			CASEELSE
				RETURN 0
		ENDSELECT
	;仕事時間帯
	CASE 5
	;行く地域(0=博1=寺2=里3=湖4=竹5=森6=冥7=山8=守9=地10=月）
	CASE 6
		SELECTCASE ARG:2
			CASE 2, 4, 5
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
	;出現場所
	CASE 7
		SELECTCASE ARG:2
			CASE 21, 22, 23, 41, 42, 43, 46, 47, 51, 52, 53, 56, 57
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
ENDSELECT

