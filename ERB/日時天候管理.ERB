@CHANGE_MONTH
#DIM 神簽代
;日付が月を跨いだら月変更
IF DAY:3 > 31
	DAY:3 = 1
	DAY:2 ++
	;冬の次は春
	IF DAY:2 > 4
		DAY:2 = 1
		FLAG:年始貸付金 = FLAG:地主貸付金
	ENDIF
	;夏になったらバレンタインフラグリセット
	SIF DAY:2 == 2
		CVARSET CFLAG, 383
	;徴収発生
	SIF FLAG:地主
		FLAG:年貢 = 1
	IF FLAG:澆水代行
		FLAG:澆水代行 = 0
		PRINTFORMW 澆水代理的期限到期了。
	ENDIF
	IF FLAG:新聞購読
		IF MONEY < 3000 && MONEY:2 < 10
			PRINTFORML 因爲缺錢所以文文。新聞的訂閱停止了。
		ELSE
			PRINTL
			PRINTFORML ★繼續訂閱文文。新聞嗎？（月額￥3000 or 10籌碼）
			CALL ASK_M("解約",1,"更新（￥3000）",MONEY >= 3000,"更新（10筹码）",MONEY:2 >= 10)
			SELECTCASE RESULT
				CASE 0
					PRINTFORML 停止了訂閱。
					FLAG:新聞購読 = 0
				CASE 1
					MONEY -= 3000
					PRINTFORML 本月繼續訂閱。
				CASE 2
					MONEY:2 -= 10
					PRINTFORML 本月繼續訂閱。
			ENDSELECT
		ENDIF
	ENDIF
	IF FLAG:神簽契約
		神簽代 = 50
		SIF !MAIN_MAP
			神簽代 = 30
		IF MONEY:2 < 神簽代
			PRINTFORML 因為籌碼不足、所以神签契約被中止了
			FLAG:神簽契約 = 0
		ELSE
			PRINTFORML 要繼續神签契約麼？
			CALL ASK_YN
			IF !RESULT
				PRINTFORML 更新了契約
				MONEY:2 -= 神簽代
			ELSE
				FLAG:神簽契約 = 0
				PRINTFORML 結束了契約
			ENDIF
		ENDIF
	ENDIF
	TRYCALLFORM IS_ENCOUNTDLC
	IF RESULT == 1
		IF FLAG:1300
			IF MONEY:2 < 10
				PRINTFORML 因為籌碼不足、所以超能力的加护被中止了
				FLAG:1300 = 0
			ELSE
				PRINTFORML 要繼續请求堇子的加护麼？
				CALL ASK_YN
				IF !RESULT
					PRINTFORML 更新了契約
					MONEY:2 -= 10
				ELSE
					FLAG:1300 = 0
					PRINTFORML 堇子收回了加护魔法
				ENDIF
			ENDIF
		ENDIF
	ENDIF
ENDIF
;異常値を固定修正
SIF DAY:3 < 1
	DAY:3 = 1
SIF DAY:2 < 1 || DAY:2 > 4
	DAY:2 = 1

