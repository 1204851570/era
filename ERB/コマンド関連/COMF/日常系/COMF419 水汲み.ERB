@COM419
SELECTCASE CFLAG:MASTER:現在位置
	CASE 霧之湖
		TFLAG:打水 = 2
	CASE 347
		TFLAG:打水 = 2
	CASE 旧地獄温泉
		TFLAG:打水 = 3
	CASE 934
		TFLAG:打水 = 3
	CASE 山之湖
		TFLAG:打水 = 5
	CASE 814
		TFLAG:打水 = 5
	;アイテム設定有れども入手経路が思いつかないのでコメントアウト
	;CASE 
	;	TFLAG:水汲み = 5
	;CASE 
	;	TFLAG:水汲み = 5
	;CASE 台所
	;	TFLAG:水汲み = 6
	;CASE 11
	;	TFLAG:水汲み = 6
	;CASE 長屋街
	;	TFLAG:水汲み = 7
	;CASE 270
	;	TFLAG:水汲み = 7
	CASEELSE
		TFLAG:打水 = 1
ENDSELECT

STAIN:MASTER:0 = 0
STAIN:MASTER:1 = 0
CFLAG:MASTER:口内精液 = 0
CFLAG:MASTER:脸上精液 = 0
CFLAG:MASTER:手上精液 = 0
IF TARGET
	STAIN:0 = 0
	STAIN:1 = 0
	CFLAG:口内精液 = 0
	CFLAG:脸上精液 = 0
	CFLAG:手上精液 = 0
ENDIF
PRINTFORML 打上来了%STR:(7000 + TFLAG:打水)%
IF GET_MAPID(CFLAG:MASTER:現在位置) != MAIN_MAP
	PRINTL 因为提着水这様走很吃力就先回去了
	CALL 从外面回家
ENDIF
TIME += 3

;-------------------------------------------------
;実行判定
;-------------------------------------------------
@COM_ABLE419
;停止中は不可
SIF FLAG:70 == 1
	RETURN 0
;実行判定
SIF !TFLAG:100
	RETURN 0
;一括管理
SIF GLOBAL_COMABLE(419)
	RETURN RESULT
SIF !打水可(CFLAG:MASTER:現在位置)
	RETURN 0
SIF CFLAG:诶嘿嘿 == 2
	RETURN 0
RETURN 1

