;FileName_EVENTCOMEND.ERB -------------------------- Rev1.00
;ターン終了の処理
;CALL		SYSTEM
;ARG		VOID
;RETURN		VOID
;COMMENT	
;-----------------------------------------------------------
@EVENTCOMEND
#DIM 装備部位
#DIMS 兒童の名前
;仕事動作のエラーチェック
;FOR LOCAL,0,CHARANUM
;	SIF CFLAG:LOCAL:326 == 0 && CFLAG:LOCAL:行動 == 5
;		PRINTFORMW ### COM_END 326ERROR %CALLNAME:LOCAL% ### CHARA_NO:{LOCAL}
;	SIF CFLAG:LOCAL:現在位置 == 0
;		PRINTFORMW ### COM_END LOCATION ERROR %CALLNAME:LOCAL% の現在位置が異常です ### CHARA_NO:{LOCAL}
;NEXT
;ターン終了共通処理
CALL TURN_RESET

SIF FLAG:宴会開催FLAG > 0
	CALL ENKAI_SINKOU

SIF TFLAG:102 < 2 && TARGET && SHIRAHU(TARGET) && !FLAG:70
	CALL ボッシュート
IF BASE:MASTER:酒気 >= MAXBASE:MASTER:酒気
	;AT_HOMEみるだけでも良いかもしれない
	IF !AT_HOME(MASTER) && GET_MAPID(CFLAG:MASTER:現在位置) != MAIN_MAP
		CALL 约会終了タイムアップ処理
	ELSE
		PRINTFORMW 持续痛饮直到极限的%CALLNAME:MASTER%、就这様突然倒下了…
	ENDIF
	GOTO OYASUMI
ENDIF
IF (BASE:MASTER:体力 <= 500 && !TCVAR:TARGET:随便) || BASE:MASTER:体力 <= 0
	PRINT （体力濒临极限）
	;発情なし崩しお部屋上がり込みの場合は選択肢を出さない
	IF CFLAG:MASTER:現在位置 == OMANEKIBEYA() && !TALENT:TARGET:恋慕

	ELSE
		;押し倒され中なら分岐
		IF TFLAG:102 == 3 && BASE:MASTER:体力 > 0 && !TCVAR:TARGET:随便
			CALL ASK_YN("随便もらう", "解放してもらう")
			IF !RESULT
				TCVAR:TARGET:随便 = 1
				BASE:MASTER:体力 -= 250
				GOTO SUKINISITE
			ENDIF
		ENDIF
	ENDIF
	PRINTW 　一天过去了
	IF BASE:MASTER:体力 <= 0 && !CHK_DATENOW(CFLAG:MASTER:约会中)
		PRINTFORMW %CALLNAME:MASTER%体力不支晕过去了…
		FLAG:気絶中断 = 100 + CFLAG:MASTER:現在位置 / 100
	ENDIF
	;押し倒され中ならTARGETに嗜虐経験
	IF TFLAG:102 == 3
		IF BASE:MASTER:体力 <= 0
			EXP:嗜虐快楽経験 += 10
			PRINTL 
			PRINTFORMW 嗜虐快楽経験 +10(%CALLNAME%)
			PRINTL 
		ELSE
			EXP:嗜虐快楽経験 += 2
			PRINTL 
			PRINTFORMW 嗜虐快楽経験 +2(%CALLNAME%)
			PRINTL 
		ENDIF
	ENDIF
	$OYASUMI
	CALL SET_SLEEP(1,MASTER,0)
	CFLAG:MASTER:就寝時間 = DAY * 1440 + TIME
	;外泊できる状態ならそのまま寝る（恋慕前提）
	IF CFLAG:MASTER:現在位置 == OMANEKIBEYA() && !TCVAR:(CFLAG:MASTER:招待):连住禁止
		IF CFLAG:(CFLAG:MASTER:招待):约会中 && TALENT:TARGET:恋慕
			PRINTFORMW 今天%CALLNAME:(CFLAG:MASTER:招待)%提供了住宿的様子
			FLAG:気絶中断 = 0
		;発情なし崩しお部屋上がり込みの場合
		ELSE
			PRINTFORML そろそろ%CALLNAME:(CFLAG:MASTER:招待)%も正気に返る頃合いだろう、潮時のようだ…
			IF TFLAG:102 >= 2
				PRINTFORMW すがりついてくる%CALLNAME:(CFLAG:MASTER:招待)%を何とか振り解き、%GET_MAPNAME(MAIN_MAP)%へ戻った…
			ELSE
				PRINTFORMW じっとり熱っぽい%CALLNAME:(CFLAG:MASTER:招待)%の視線から逃れるように、%GET_MAPNAME(MAIN_MAP)%へ戻った…
			ENDIF
			;部屋の汚れを保存しておく
			IF YOGORE:(OMANEKIBEYA()) > 0
				TCVAR:(CFLAG:MASTER:招待):待客室的污垢 = YOGORE:(OMANEKIBEYA())
			ELSE
				;０以下にした場合は識別するために１残す
				TCVAR:(CFLAG:MASTER:招待):待客室的污垢 = 1
			ENDIF
			;お招きフラグに0を代入
			CFLAG:TARGET:招待 = 0
			CFLAG:MASTER:招待 = 0
			FLAG:気絶中断 = 0
			CFLAG:MASTER:現在位置 = ENTRANCE
			PRINTW 
		ENDIF
	ELSEIF TARGET > 0 && TALENT:TARGET:恋慕 && BASE:TARGET:体力 >= 500 && !CFLAG:TARGET:睡眠 && !CFLAG:MASTER:陪睡中 && ((CFLAG:TARGET:同行 && CHK_DATENOW(CFLAG:MASTER:约会中)) || !CHK_DATENOW(CFLAG:MASTER:约会中))
		IF CHK_DATENOW(CFLAG:MASTER:约会中)
			PRINTFORMW %CALLNAME:TARGET%支撐著%CALLNAME:MASTER%回到了%GET_MAPNAME(MAIN_MAP)%
		ENDIF
		IF CFLAG:MASTER:現在位置 != CFLAG:MASTER:初始位置
			PRINTFORMW %CALLNAME:TARGET%把%CALLNAME:MASTER%送回了私室…
			CFLAG:MASTER:現在位置 = CFLAG:MASTER:311
			CALL KOJO_MESSAGE_SEND("EVENT",3,TARGET,6,0)
		ELSE
			PRINTFORMW %CALLNAME:TARGET%把%CALLNAME:MASTER%送到了\@ INROOM(CFLAG:MASTER:現在位置) == 2 ? 床上 # 被窝 \@…
		ENDIF
		CFLAG:MASTER:约会中 = MAIN_MAP
		FLAG:気絶中断 = 0
	ELSEIF CFLAG:MASTER:現在位置 == 小人的虫籠
		CFLAG:MASTER:現在位置 = 走廊
		PRINTFORMW 不知什么时候%CALLNAME:MASTER%滚到了走廊上…
	ELSEIF CHK_DATENOW(CFLAG:MASTER:约会中)
		PRINTFORMW %CALLNAME:MASTER%竭盡最後的力氣、回到了%GET_MAPNAME(MAIN_MAP)%
		FLAG:気絶中断 = 100 + CFLAG:MASTER:現在位置 / 100
		CFLAG:MASTER:現在位置 = ENTRANCE
		PRINTFORMW 
	ELSE
		PRINTFORMW %CALLNAME:MASTER%步履蹣跚、回到了臥室
		CFLAG:MASTER:現在位置 = CFLAG:MASTER:311
		FLAG:気絶中断 = 0
	ENDIF
	FOR LOCAL,0,CHARANUM
		SIF CFLAG:LOCAL:出禁
			CONTINUE
		CALL ENDUFUFU(LOCAL)
		CALL SET_PRANK(0,LOCAL)
		CFLAG:LOCAL:约会中 = MAIN_MAP
		CFLAG:LOCAL:同行 = 0
		FLAG:约会的对象 = 0
		TFLAG:约会前好感度 = 0
		SIF !BATHCHECK(CFLAG:LOCAL:現在位置) && TEQUIP:LOCAL:浴室PLAY
			TEQUIP:LOCAL:浴室PLAY = 0
	NEXT
;	数値変数が小数点以下を切り捨てる特性を悪用したものがこちら、他の変数を使わない分こちらのほうがいいのかも？
	CFLAG:MASTER:约会中 = CFLAG:MASTER:現在位置 / 100
	TFLAG:106 = 0
	BEGIN AFTERTRAIN
ENDIF
IF TARGET && TFLAG:接吻マーク == TARGET && TFLAG:绷帯 && !RAND:2
	PRINTFORMW 察覺到%CALLNAME:MASTER%的脖子上被包住的東西的%CALLNAME:TARGET%、拉開了繃帯
	TFLAG:绷帯 = 0
	BASE:憤怒 += 500
ENDIF
$SUKINISITE
IF TARGET && CFLAG:心情不快 && !FLAG:70 && !TCVAR:强行 && !CFLAG:負け犬
	IF TALENT:TARGET:胆量 < 0
		PRINTFORMW 把%CALLNAME:TARGET%弄哭了…
		CALL KOJO_MESSAGE_SEND("EVENT",29,TARGET,2,0)
	ELSE
		PRINTFORMW 把%CALLNAME:TARGET%惹火了…
		CALL KOJO_MESSAGE_SEND("EVENT",29,TARGET,1,0)
	ENDIF
	
	IF FLAG:守矢神簽约会的对象 != TARGET
		FOR LOCAL,0,CHARANUM
			CALL ENDUFUFU(LOCAL)
		NEXT
		TFLAG:106 = 0
	ENDIF
ENDIF

IF FLAG:70 == 1
	IF BASE:MASTER:TSP <= 0
		PRINTFORML (TSP用完了)
		[SKIPSTART]
		FOR LOCAL,1,CHARANUM
			IF CFLAG:LOCAL:現在位置 == CFLAG:PLAYER:現在位置
				FOR 装備部位,0,23
					SIF (!睡眠時間(LOCAL) && EQUIP:LOCAL:装備部位 != CFLAG:LOCAL:(200 + 装備部位)) || (睡眠時間(LOCAL) && EQUIP:LOCAL:装備部位 != CFLAG:LOCAL:(240 + 装備部位))
						LOCAL:1 ++
					SIF 装備部位 == 6 && CFLAG:LOCAL:没穿内裤 && ((!睡眠時間(LOCAL) && EQUIP:LOCAL:装備部位 != CFLAG:LOCAL:(200 + 装備部位)) || (睡眠時間(LOCAL) && EQUIP:LOCAL:装備部位 != CFLAG:LOCAL:(240 + 装備部位)))
						LOCAL:1 --
				NEXT
				IF LOCAL:1
					EQUIP:LOCAL:0 = 1
					IF CFLAG:LOCAL:時間停止換装 == 1
						PRINTFORMW %CALLNAME:MASTER%把%CALLNAME:LOCAL%\@ CFLAG:LOCAL:没穿内裤 ? 内裤以外的 #  \@地方弄回了之前的樣子！
						CFLAG:LOCAL:時間停止換装 = 0
					ELSE
						PRINTFORMW %CALLNAME:MASTER%給%CALLNAME:LOCAL%穿上了\@ CFLAG:LOCAL:没穿内裤 ? 内裤以外的 #  \@衣服
					ENDIF
				ENDIF
			ENDIF
		NEXT
		[SKIPEND]
		IF CFLAG:MASTER:現在位置 != OMANEKIBEYA()
			PRINTFORMW %CALLNAME:MASTER%慌忙离开了现场
			TFLAG:運搬 = 0
			CALL FISHER_YATES_SHAFFLE(CHARANUM - 1)
			FOR LOCAL:10, 0, CHARANUM - 1
				LOCAL = SHAFFLE_ARRAY:(LOCAL:10) + 1
				SIF CFLAG:LOCAL:現在位置 != CFLAG:MASTER:現在位置
					CONTINUE
				IF EX:LOCAL:Ｃ絶頂 + EX:LOCAL:Ｖ絶頂 + EX:LOCAL:Ａ絶頂 + EX:LOCAL:Ｂ絶頂 + EX:LOCAL:Ｍ絶頂 > 30
					PRINTFORMW 突然袭来的激烈絶頂令%CALLNAME:LOCAL%发出甜美的娇喘声、当场瘫坐在地
					CFLAG:LOCAL:333 += 20
				ELSEIF EX:LOCAL:Ｃ絶頂 + EX:LOCAL:Ｖ絶頂 + EX:LOCAL:Ａ絶頂 + EX:LOCAL:Ｂ絶頂 + EX:LOCAL:Ｍ絶頂 > 0
					PRINTFORMW %CALLNAME:LOCAL%因为謎之快感身体猛地僵硬起来
				ELSEIF CFLAG:LOCAL:291 == 1
					PRINTFORMW 不知什么时候内裤被脱掉的%CALLNAME:LOCAL%一幅坐立不安的様子
				ENDIF
				SIF EX:LOCAL:処女喪失记号 == 1
					PRINTFORMW %CALLNAME:LOCAL%因为股間的鈍痛而紧皱着眉头…
			NEXT
			IF !AT_HOME(MASTER)
				PRINTFORML 往%GET_MAPNAME(MAIN_MAP)%全速疾奔而去…
				BASE:MASTER:体力 -= TIME_REQUIRED(MAIN_MAP,CFLAG:MASTER:约会中) * 5
				BASE:MASTER:気力 -= TIME_REQUIRED(MAIN_MAP,CFLAG:MASTER:约会中) * 5
				BASE:MASTER:体力 = MAX(BASE:MASTER:体力,0)
				BASE:MASTER:気力 = MAX(BASE:MASTER:気力,0)
				TIME += TIME_REQUIRED(MAIN_MAP,CFLAG:MASTER:约会中) / 5
			ENDIF
			IF CFLAG:MASTER:現在位置 != CFLAG:MASTER:初始位置
				CFLAG:MASTER:現在位置 = CFLAG:MASTER:初始位置
			ELSE
				CFLAG:MASTER:現在位置 = 避難处
			ENDIF
			BASE:MASTER:TSP = 0
			CALL AFTER_AFFAIR
			FOR LOCAL,0,CHARANUM
				IF CFLAG:LOCAL:诶嘿嘿 == 1 && LOCAL != 0
					;うふふ終了口上呼び出し
					CALL KOJO_MESSAGE_SEND("EVENT",15,LOCAL,2,0)
				ENDIF
				CALL ENDUFUFU(LOCAL)
				CFLAG:LOCAL:约会中 = MAIN_MAP
			NEXT
			FLAG:约会的对象 = 0
			TFLAG:约会前好感度 = 0
			TFLAG:106 = 0
		ELSE
			CALL FISHER_YATES_SHAFFLE(CHARANUM - 1)
			FOR LOCAL:10, 0, CHARANUM - 1
				LOCAL = SHAFFLE_ARRAY:(LOCAL:10) + 1
				IF CFLAG:LOCAL:現在位置 == CFLAG:MASTER:現在位置
					IF EX:LOCAL:Ｃ絶頂 + EX:LOCAL:Ｖ絶頂 + EX:LOCAL:Ａ絶頂 + EX:LOCAL:Ｂ絶頂 + EX:LOCAL:Ｍ絶頂 > 30
						PRINTFORMW 突然袭来的激烈絶頂令%CALLNAME:LOCAL%发出甜美的娇喘声、当场瘫坐在地
						CFLAG:LOCAL:333 += 20
					ELSEIF EX:LOCAL:Ｃ絶頂 + EX:LOCAL:Ｖ絶頂 + EX:LOCAL:Ａ絶頂 + EX:LOCAL:Ｂ絶頂 + EX:LOCAL:Ｍ絶頂 > 0
						PRINTFORMW %CALLNAME:LOCAL%因为謎之快感身体猛地僵硬起来
					ELSEIF CFLAG:LOCAL:291 == 1
						PRINTFORMW 不知什么时候内裤被脱掉的%CALLNAME:LOCAL%一幅坐立不安的様子
					ENDIF
					SIF EX:LOCAL:処女喪失记号 == 1
						PRINTFORMW %CALLNAME:LOCAL%因为股間的鈍痛而紧皱着眉头…
				ENDIF
				CALL ENDUFUFU(LOCAL)
			NEXT
			CFLAG:MASTER:诶嘿嘿 = 0
		ENDIF
		FLAG:70 = 0
		RESETBGCOLOR
		BASE:MASTER:TSP = 0
	ENDIF
	SIF CFLAG:MASTER:诶嘿嘿 && BASE:MASTER:勃起 <= 900 && BASE:MASTER:精力 >= 500
		BASE:MASTER:勃起 += 10
ENDIF
IF TARGET && BASE:体力 < 500
	IF FLAG:70 == 1 && CFLAG:MASTER:現在位置 != OMANEKIBEYA()
		IF !CFLAG:衰弱
			PRINTFORML （%CALLNAME:TARGET%的体力濒临了极限）
			PRINTFORMW %CALLNAME:MASTER%离开了这个地方、解除了時間停止
			CFLAG:衰弱 = 360
			FLAG:70 = 0
			FLAG:约会的对象 = 0
			TFLAG:约会前好感度 = 0
			RESETBGCOLOR
			IF AT_HOME(MASTER)
				CFLAG:MASTER:约会中 = MAIN_MAP
				IF CFLAG:MASTER:現在位置 != CFLAG:MASTER:初始位置
					CFLAG:MASTER:現在位置 = CFLAG:MASTER:初始位置
				ELSE
					CFLAG:MASTER:現在位置 = 避難处
				ENDIF
			ELSE
				IF STR:(6000 + CFLAG:MASTER:現在位置 + 10) == ""
					CFLAG:MASTER:現在位置 -= 10
				ELSE
					CFLAG:MASTER:現在位置 += 10
				ENDIF
			ENDIF
		ENDIF
		CALL CHARA_SLEEP, TARGET, 0
		FOR LOCAL,0,CHARANUM
			CALL ENDUFUFU(LOCAL)
			CALL SET_PRANK(0,LOCAL,1)
			SIF LOCAL
				CFLAG:LOCAL:约会中 = MAIN_MAP
			CFLAG:LOCAL:同行 = 0
		NEXT
		FLAG:约会的对象 = 0
		TFLAG:约会前好感度 = 0
		TFLAG:106 = 0
	ELSE
		IF !CFLAG:衰弱
			PRINTFORMW （%CALLNAME:TARGET%的体力濒临了极限）
			CFLAG:衰弱 = 360
			FLAG:70 = 0
			RESETBGCOLOR
		ENDIF
		CALL CHARA_SLEEP, TARGET, 0
		FOR LOCAL,0,CHARANUM
			CALL ENDUFUFU(LOCAL)
			CALL SET_PRANK(0,LOCAL,1)
			SIF LOCAL
				CFLAG:LOCAL:约会中 = MAIN_MAP
			CFLAG:LOCAL:同行 = 0
		NEXT
		FLAG:约会的对象 = 0
		TFLAG:约会前好感度 = 0
		TFLAG:106 = 0
	ENDIF
ENDIF

IF FLAG:兒童描写
	FOR LOCAL,1,CHARANUM
		SIF !TALENT:LOCAL:育児中
			CONTINUE
		SIF CFLAG:LOCAL:現在位置 != CFLAG:MASTER:現在位置
			CONTINUE
		兒童の名前 = %CHILDNAME:LOCAL:(TALENT:LOCAL:育児中)%
		;欲求不満度抑制と幼児期の空腹判定と起床判定
		IF TCVAR:LOCAL:兒童空腹度 != -1
			SIF TCVAR:LOCAL:兒童空腹カウント
				TCVAR:LOCAL:兒童空腹度 += (TIME:0 - TCVAR:LOCAL:兒童空腹カウント)
			TCVAR:LOCAL:兒童空腹カウント = TIME:0
			SIF CFLAG:LOCAL:积攒度 > 300
			CFLAG:LOCAL:积攒度 = 300
		ELSEIF TCVAR:LOCAL:兒童空腹度 == -1 && TIME:0 - TCVAR:LOCAL:兒童空腹カウント > 120 && !RAND:2 && !CFLAG:MASTER:诶嘿嘿 && CFLAG:LOCAL:出産経過日 < CHILD_語彙
			PRINTFORMDL %兒童の名前%は目を覚ました
			TCVAR:LOCAL:兒童空腹度 = 120
			TCVAR:LOCAL:兒童空腹カウント = TIME:0
			SIF CFLAG:LOCAL:积攒度 > 300
			CFLAG:LOCAL:积攒度 = 300
		ENDIF
		;通学
		IF GO_SCHOOL(LOCAL)
			IF !CHK_DATENOW(CFLAG:LOCAL:约会中) && INRANGE(TIME, 420, 540) && TCVAR:LOCAL:兒童空腹度 != -1 && !CFLAG:MASTER:诶嘿嘿
				PRINTFORMDL %兒童の名前%出発去寺子屋了
				CALL KOJO_MESSAGE_SEND("EVENT", 0, LOCAL, 0, 0, "CHILD_RAISING_登下校")
				TCVAR:LOCAL:兒童空腹度 = -1
			;ウフフ中帰ってこない空気の読める子供
			ELSEIF !CHK_DATENOW(CFLAG:LOCAL:约会中) && INRANGE(TIME, 840, 1020) && TCVAR:LOCAL:兒童空腹度 == -1 && !CFLAG:MASTER:诶嘿嘿
				PRINTFORMDL %兒童の名前%從寺子屋回來了
				CALL KOJO_MESSAGE_SEND("EVENT", 0, LOCAL, 1, 0, "CHILD_RAISING_登下校")
				TCVAR:LOCAL:兒童空腹度 = 0
			ELSEIF INRANGE(TIME, 540, 840)
				TCVAR:LOCAL:兒童空腹度 = -1
			ELSEIF INRANGE(TIME, 1020, 1260)
				TCVAR:LOCAL:兒童空腹度 = 0
			ENDIF
		ENDIF
		;これ以降は通学/睡眠中には発生しない
		SIF TCVAR:LOCAL:兒童空腹度 == -1
			CONTINUE
		;うふふが中断するようなイベントも作るべきか？
		SIF CFLAG:MASTER:诶嘿嘿
			CONTINUE
		;嫁が寝てたりしてたら子供も無反応ってのもなあ
		SIF !SHIRAHU(LOCAL)
			CONTINUE
		;幼児期
		IF CFLAG:LOCAL:出産経過日 < CHILD_語彙
			IF BATHROOM(CFLAG:現在位置) && !RAND:2
				CALL KOJO_MESSAGE_SEND("EVENT", 0, LOCAL, 0, 0, "CHILD_RAISING_BATH", "幼児期")
			ELSEIF TIME:0 <= CFLAG:LOCAL:就寝時間 && TCVAR:LOCAL:兒童空腹度 >= 120 && CFLAG:LOCAL:出産経過日 < CHILD_離乳
					TCVAR:LOCAL:兒童空腹度 = 0
					EXP:LOCAL:噴乳経験 ++
					PRINTFORMDL %CALLNAME:LOCAL%在給%兒童の名前%餵奶
					CALL KOJO_MESSAGE_SEND("EVENT", 0, LOCAL, 0, 0, "CHILD_RAISING_吃飯", "授乳")
			ELSEIF TIME:0 <= CFLAG:LOCAL:就寝時間 && TCVAR:LOCAL:兒童空腹度 >= 120 && GROUPMATCH(SELECTCOM, 414, 415, 610, 615, 617)
					TCVAR:LOCAL:兒童空腹度 = 0
					PRINTFORMDL %CALLNAME:LOCAL%在給%兒童の名前%餵著嬰兒輔食
					CALL KOJO_MESSAGE_SEND("EVENT", 0, LOCAL, 0, 0, "CHILD_RAISING_吃飯", "離乳食")
			ELSEIF TCVAR:LOCAL:兒童空腹度 < 120 && !RAND:6
				PRINTFORMDL %兒童の名前%香甜的睡著了……
				TCVAR:LOCAL:兒童空腹度 = -1
					CALL KOJO_MESSAGE_SEND("EVENT", 0, LOCAL, 0, 0, "CHILD_RAISING_SLEEPING", "幼児期")
			ELSEIF CFLAG:LOCAL:出産経過日 >= CHILD_玩具 && !RAND:5
				PRINTFORMDL %兒童の名前%玩著%CALLNAME:MASTER%所製作的玩具
				CALL KOJO_MESSAGE_SEND("EVENT", 0, LOCAL, 0, 0, "CHILD_RAISING_TOY", "幼児期")
			ELSEIF !RAND:3
				CALL KOJO_MESSAGE_SEND("EVENT", 0, LOCAL, 0, 0, "CHILD_RAISING_OTHER", "幼児期")
			ENDIF
		;幼少期
		ELSEIF INRANGE(CFLAG:LOCAL:出産経過日, CHILD_語彙, CHILD_自立前 -1)
			IF 兒童の睡眠時間(LOCAL)
				PRINTFORMDW %兒童の名前%向寝室走去……
				TCVAR:LOCAL:兒童空腹度 = -1
				CALL KOJO_MESSAGE_SEND("EVENT", 0, LOCAL, 0, 0, "CHILD_RAISING_SLEEPING", "幼少期")
			ELSEIF BATHROOM(CFLAG:現在位置) && !RAND:2
				CALL KOJO_MESSAGE_SEND("EVENT", 0, LOCAL, 0, 0, "CHILD_RAISING_BATH", "幼少期")
			ELSEIF GROUPMATCH(SELECTCOM, 414, 415, 610, 615, 617)
				CALL KOJO_MESSAGE_SEND("EVENT", 0, LOCAL, 0, 0, "CHILD_RAISING_吃飯", "幼少期")
			ELSEIF !RAND:6
				PRINTFORMDL %兒童の名前%玩著%CALLNAME:MASTER%所製作的玩具
				CALL KOJO_MESSAGE_SEND("EVENT", 0, LOCAL, 0, 0, "CHILD_RAISING_TOY", "幼少期")
			ELSEIF !RAND:3
				TCVAR:LOCAL:兒童空腹度 = 0
				CALL KOJO_MESSAGE_SEND("EVENT", 0, LOCAL, 0, 0, "CHILD_RAISING_OTHER", "幼少期")
			ENDIF
		;自立前
		ELSE
			;子供の自立日でなければ寝る
			IF 兒童の睡眠時間(LOCAL) && CFLAG:LOCAL:出産経過日 != CHILD_自立
				PRINTFORMDW %兒童の名前%向寝室走去……
				TCVAR:LOCAL:兒童空腹度 = -1
				CALL KOJO_MESSAGE_SEND("EVENT", 0, LOCAL, 0, 0, "CHILD_RAISING_SLEEPING", "自立前")
			ELSEIF BATHROOM(CFLAG:現在位置) && !RAND:2
				CALL KOJO_MESSAGE_SEND("EVENT", 0, LOCAL, 0, 0, "CHILD_RAISING_BATH", "自立前")
			ELSEIF GROUPMATCH(SELECTCOM, 414, 415, 610, 615, 617)
				CALL KOJO_MESSAGE_SEND("EVENT", 0, LOCAL, 0, 0, "CHILD_RAISING_吃飯", "自立前")
			ELSEIF CFLAG:LOCAL:現在位置 == CFLAG:LOCAL:初始位置 && !RAND:8
				PRINTFORMDL %兒童の名前%眺望着%CALLNAME:MASTER%為自己製作的木頭玩具湧起了深深的回憶……
				CALL KOJO_MESSAGE_SEND("EVENT", 0, LOCAL, 0, 0, "CHILD_RAISING_TOY", "自立前")
			ELSEIF !RAND:5
				TCVAR:LOCAL:兒童空腹度 = 0
				CALL KOJO_MESSAGE_SEND("EVENT", 0, LOCAL, 0, 0, "CHILD_RAISING_OTHER", "自立前")
			ENDIF
		ENDIF
	NEXT
	RESETCOLOR
ENDIF

IF GET_EXISTMOB(MAIN_MAP)
	FOR LOCAL,MINROOM(),MAXROOM
		IF ROOMDATA:(LOCAL % 100) & 場所_モブ
			YOGORE:LOCAL += TIME_PROGRESS(TIME:1) * MOB_YOGORE(MAIN_MAP)
			SIF CFLAG:MASTER:現在位置 == LOCAL && AT_HOME(MASTER)
				TRYCALLFORM MOB_TURNENDEVENT_{MAIN_MAP}
		ENDIF
	NEXT
ENDIF
IF !FLAG:70 && TARGET
	IF TALENT:TARGET:動物耳 && CFLAG:TARGET:900 == 7 && !TCVAR:TARGET:発情 && PALAM:TARGET:欲情 >= PALAMLV:5
		PRINTFORML %CALLNAME:TARGET%脸颊泛红、气息荒乱起来…
		PRINTFORML 在危険日極度興奮起来的%CALLNAME:TARGET%好像[発情]了
		TCVAR:TARGET:発情 = 1
	ENDIF
ENDIF

FOR LOCAL,0, CHARANUM
	SIF TIME > TCVAR:LOCAL:追い出され時刻 + 30
		TCVAR:LOCAL:追い出され地点 = 0
	;IF !CFLAG:LOCAL:うふふ
		FOR LOCAL:2,101,109
			SIF TEQUIP:LOCAL:(LOCAL:2) != TEQUIP:LOCAL:(LOCAL:2 + 10)
				TEQUIP:LOCAL:(LOCAL:2) = 0
		NEXT
	;ENDIF
	SIF CFLAG:LOCAL:現在位置 == SUKIMA()
		CONTINUE
	;今日の挨拶をしてなかった場合遭遇時口上を出す
	IF LOCAL > 0 && !TCVAR:LOCAL:今天有否见面 && CFLAG:MASTER:現在位置 == CFLAG:LOCAL:現在位置 && !FLAG:70
		IF CFLAG:LOCAL:面識 == 0
			CALL SYOTAIMEN(LOCAL)
		ELSEIF CHK_DATENOW(CFLAG:MASTER:约会中)
			CALL KOJO_MESSAGE_SEND("EVENT",1,LOCAL,6,0)
		ELSE
			CALL KOJO_MESSAGE_SEND("EVENT",1,LOCAL,1,0)
		ENDIF
		CALL FirstMeetToday(LOCAL)
	ENDIF
	;衣装の着替え直しは@SHOW_STATUSに移動
	[SKIPSTART]
	LOCAL:1 = 0
	FOR 装備部位,0, 23
		SIF (!睡眠時間(LOCAL) && EQUIP:LOCAL:装備部位 != CFLAG:LOCAL:(200 + 装備部位)) || (睡眠時間(LOCAL) && EQUIP:LOCAL:装備部位 != CFLAG:LOCAL:(240 + 装備部位))
			LOCAL:1 ++
		SIF 装備部位 == 6 && CFLAG:LOCAL:没穿内裤 && ((!睡眠時間(LOCAL) && EQUIP:LOCAL:装備部位 != CFLAG:LOCAL:(200 + 装備部位)) || (睡眠時間(LOCAL) && EQUIP:LOCAL:装備部位 != CFLAG:LOCAL:(240 + 装備部位)))
			LOCAL:1 --
	NEXT
	IF !CFLAG:LOCAL:诶嘿嘿 && CFLAG:LOCAL:現在位置 != BATHROOM(CFLAG:LOCAL:現在位置) && LOCAL:1
		IF 睡眠時間(LOCAL)
			;パジャマセット
			CALL CTRL_CLOTHES_SET(LOCAL, "現在衣装の変更_睡衣")
			CFLAG:LOCAL:睡衣 = 1
		ELSE
			;普段着セット
			CALL CTRL_CLOTHES_SET(LOCAL, "現在衣装の変更_便装")
			CFLAG:LOCAL:睡衣 = 0
		ENDIF
		SIF !睡眠時間(LOCAL) && CFLAG:LOCAL:眠姦発覚
			CFLAG:LOCAL:眠姦発覚 = 0
		CALL CLOTHES_SETTING_TRAIN(LOCAL)
	ENDIF
	[SKIPEND]
	;いちおうCLOTHES_SETTING_TRAINはやって損はなかろう
		SIF !睡眠時間(LOCAL) && CFLAG:LOCAL:眠姦発覚
			CFLAG:LOCAL:眠姦発覚 = 0
		CALL CLOTHES_SETTING_TRAIN(LOCAL)
NEXT
;強制的にWAITが追加されるのを回避するためにTWAITを仕込む
;これで入力回数が１回減る
IF CFLAG:MASTER:現在位置 == 99
	SETCOLOR C_RED
	PRINTFORMW ### COM_END LOCATION ERROR! %CALLNAME:MASTER%は存在しないはずの場所にいる！ ###
	PRINTW 强制地结束一天。请取得日志、提出错误报告。
	RESETCOLOR
	CFLAG:MASTER:現在位置 = CFLAG:MASTER:311
	CALL SET_SLEEP(1,MASTER,0)
	TFLAG:106 = 0
	BEGIN AFTERTRAIN
ENDIF
	;CFLAG:MASTER:就寝時間 = DAY * 1440 + TIME
SIF SELECTCOM == 400
	TWAIT 100,0

@ENKAI_SINKOU
;宴会の参加人数をセット
FLAG:35 = 0
FOR LOCAL,0,CHARANUM
	IF CFLAG:LOCAL:宴会参加 == 1 || CFLAG:LOCAL:宴会参加 == 2
		SIF CFLAG:LOCAL:現在位置 == TFLAG:宴会場
			FLAG:35 ++
	ENDIF
NEXT

SIF FLAG:宴会開催FLAG
	RETURN

;天気が悪いと中止になる宴会もある

IF TIME:5 >= 4 && FLAG:36 > 0 && GROUPMATCH(CFLAG:[[天子]]:宴会参加, 1, 2)
	PRINTFORMW 悪天候により宴会は中止になるかと思われたが、天子の力により空は晴天渡った
	TIME:5 = 0
ENDIF
IF TIME:5 >= 4 && FLAG:36 > 0 && DAY >= FLAG:開始日 && TIME >= FLAG:33
	PRINTL  
	PRINTFORMW 很遗憾、\@ CFLAG:MASTER:現在位置 == TFLAG:宴会場 ? 由于天気不好的原因宴会中止了 # 很遗憾、由于坏天気的原因宴会似乎要中止了\@
	CALL ENKAI_STOP
;宴会の途中で降り始めた場合
ELSEIF FLAG:宴会開催FLAG >= 98 && TIME:5 >= 4 && FLAG:36 > 0
	PRINTL  
	PRINTFORMW 很遗憾、\@ CFLAG:MASTER:現在位置 == TFLAG:宴会場 ? 由于天気不好的原因宴会中止了 # 很遗憾、由于坏天気的原因宴会似乎要中止了\@
	CALL ENKAI_STOP
;宴会開始
ELSEIF FLAG:宴会開催FLAG != 98 && FLAG:宴会開催FLAG != 99 && DAY >= FLAG:開始日 && TIME >= FLAG:33
	IF CFLAG:MASTER:現在位置 == TFLAG:宴会場
		PRINTFORML 
		IF FLAG:35 < 2
			PRINTW 在恬静的气氛中、宴会开始了…
		ELSEIF FLAG:35 >= 8
			PRINTW 在热闹的气氛中、宴会开始了…
		ELSE
			PRINTW 在祥和的气氛中、宴会开始了…
		ENDIF
		PRINTFORML 
		FLAG:宴会開催FLAG = 99
	ELSE
		FLAG:宴会開催FLAG = 98
	ENDIF
ELSEIF FLAG:宴会開催FLAG != 99 && DAY >= FLAG:開始日 && TIME >= FLAG:33
	IF CFLAG:MASTER:現在位置 == TFLAG:宴会場
		PRINTL  
		PRINTW 宴会好像已经开始了的様子…
		PRINTL  
		FLAG:宴会開催FLAG = 99
	ENDIF
	;主催者側はまだ宴会準備フラグのままのため、フラグを無理やり進める
	FOR LOCAL,0,CHARANUM
		SIF CFLAG:LOCAL:20 == 2
			CALL ENKAI_WORK(LOCAL)
	NEXT
ENDIF
