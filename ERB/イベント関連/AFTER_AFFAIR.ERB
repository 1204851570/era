	ぱんつ状況 = 0
;パンツを一旦脱いでいた場合
ELSEIF !EQUIP:下半身内衣１ && !EQUIP:下半身内衣２
	ぱんつ状況 = 1
ELSE
	ぱんつ状況 = 2
ENDIF


IF !ぱんつ状況
	PRINTFORMDL %CALLNAME:TARGET%的下面是真空的。
	IF ARGS == "SPERM"
		PRINTFORMDL 今天、没穿内裤%EXPR_VAR("SPERM")%好像正在垂滴下来的様子。
	ELSE
		PRINTFORMDL 今天、%EXPR_VAR("CROTCH")%充满着爱液的湿润、没有了内裤的保护、就这様暴露在空气中。
	ENDIF
;パンツがある場合
ELSE
	SIF ぱんつ状況 == 1
		PRINTFORMDL %CALLNAME:TARGET%は脱いでいた内衣を穿きなおした。
	IF ARGS == "SPERM"
		PRINTFORMD 垂流着%EXPR_VAR("SPERM")%
	ELSE
		PRINTFORMD 依稀露着爱液的闪光
	ENDIF
	PRINTFORMD が\@ ぱんつ状況 == 1? その# \@内衣を%TEXTR("湿漉漉的/泛着潮湿的气息/湿透")%
	;パンツが透けうる場合
	IF PANTIES_TRANSPARENCY(TARGET)
		PRINTFORMDL 、甚至可以看到整个阴唇的形状和阴蒂的凸起。
	ELSE
		PRINTFORMDL た。
	ENDIF
ENDIF


;### びしょ濡れ判定用式中関数 ################
;ARG=キャラ番号
@SOAKED_OR_NOT(ARGS,ARG)
#FUNCTION
#DIM 必要膣充填率
#DIM 必要肛門充填率
#DIM 必要潤滑程度

SIF ARG == 0
	ARG = TARGET

必要膣充填率 = 30
必要肛門充填率 = 30
必要潤滑程度 = PALAMLV:5

IF ARGS == "V_SEMINAL"
	IF 充填率(ARG,1) >= 必要膣充填率
		RETURNF 1
	ELSE
		RETURNF 0
	ENDIF
ELSEIF ARGS == "A_SEMINAL"
	IF 充填率(ARG,2) >= 必要肛門充填率
		RETURNF 1
	ELSE
		RETURNF 0
	ENDIF
ELSEIF ARGS == "SECRETIONAL"
	IF PALAM:ARG:潤滑 >= 必要潤滑程度
		RETURNF 1
	ELSE
		RETURNF 0
	ENDIF
ENDIF

THROW 错误的文字列引数


;### 表現バリエーション用式中関数 ################
@EXPR_VAR(ARGS)
#FUNCTIONS
VARSET LOCALS

IF ARGS == "SPERM"
	SELECTCASE RAND:4
	CASE 0
		LOCALS = 白濁液
	CASE 1
		LOCALS = 精液
	CASE 2
		LOCALS = 精液
	CASEELSE
		LOCALS = 子種汁
	ENDSELECT
ELSEIF ARGS == "VULVA"
	SELECTCASE RAND:4
	CASE 0
		LOCALS = 秘処
	CASE 1
		LOCALS = 秘裂
	CASE 2
		LOCALS = 陰門
	CASEELSE
		LOCALS = 女陰
	ENDSELECT
ELSEIF ARGS == "ANUS"
	SELECTCASE RAND:3
	CASE 0
		LOCALS = 菊门
	CASE 1
		LOCALS = 肛門
	CASEELSE
		LOCALS = 菊花
	ENDSELECT
ELSEIF ARGS == "CROTCH"
	SELECTCASE RAND:3
	CASE 0
		LOCALS = 股間
	CASE 1
		LOCALS = 局部
	CASEELSE
		LOCALS = 股
