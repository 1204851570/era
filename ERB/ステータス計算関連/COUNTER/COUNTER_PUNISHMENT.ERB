@EVENT_COUNTER_PUNISHMENT(ARG,ARG:1)
VARSET LOCAL
LOCAL = 1
WHILE LOCAL
LOCAL:1 = RAND:8
RESULT = 1
TRYCALLFORM PUNISHMENT_{LOCAL:1}(ARG,ARG:1)
SIF CFLAG:ARG:掌握把柄 <= 0
	CFLAG:ARG:掌握把柄 = 0
LOCAL = RESULT
WEND
SIF !CFLAG:ARG:掌握把柄
	PRINTFORML  这之后被%CALLNAME:ARG%饶过了的様子…
DRAWLINE
WAIT
;パンツを脱がせる
@PUNISHMENT_0(ARG,ARG:1)
SIF !CFLAG:PLAYER:206
	RETURN 1
CFLAG:PLAYER:206 = 0

PRINTFORML %CALLNAME:ARG%奪走了%CALLNAME:PLAYER%的内裤
PRINTFORML 下令今天一天都得不穿内裤渡过并将凌乱的衣服整理好…
CFLAG:ARG:掌握把柄 -= 1
EXP:ARG:嗜虐快楽経験 += 1
RETURN 0

;ローター挿入
@PUNISHMENT_1(ARG,ARG:1)
SIF !(TALENT:PLAYER:性別 & 1)
	RETURN 1
CFLAG:PLAYER:跳蛋挿入 = 240
PRINTFORML %CALLNAME:ARG%把跳蛋塞进%CALLNAME:PLAYER%的私处并固定好
PRINTFORML 下令就这様呆着一段时间并将凌乱的衣服整理好…
CFLAG:ARG:掌握把柄 -= 2
EXP:ARG:嗜虐快楽経験 += 10

RETURN 0

;ローターA挿入
@PUNISHMENT_2(ARG,ARG:1)
;応急処置としてコメントアウトしておきます
;SIF CFLAG:PLAYER:ローターA挿入
;	RETURN 1
SIF TFLAG:パール期限
	RETURN 1
CFLAG:PLAYER:跳蛋A挿入 = 240
PRINTFORML %CALLNAME:ARG%把跳蛋塞进%CALLNAME:PLAYER%的肛門并固定好
PRINTFORML 下令就这様呆着一段时间并将凌乱的衣服整理好…
CFLAG:ARG:掌握把柄 -= 2
EXP:ARG:嗜虐快楽経験 += 10

RETURN 0

;キスマーク
@PUNISHMENT_3(ARG,ARG:1)
SIF TFLAG:接吻マーク
	RETURN 1
SIF !TALENT:ARG:恋慕
	RETURN 1
PRINTFORML %CALLNAME:ARG%抱住%CALLNAME:PLAYER%、用力吸着后颈留下了吻痕…
PRINTFORML 在它消失之前似乎别让其他的女孩子看到比较好…
TFLAG:接吻マーク = ARG
CFLAG:ARG:掌握把柄 -= 1
EXP:ARG:嗜虐快楽経験 += 2
RETURN 0

;ほほを抓る
@PUNISHMENT_4(ARG,ARG:1)
PRINTFORML %CALLNAME:ARG%は%CALLNAME:PLAYER%の頬をちからいっぱい抓ってきた
PRINTFORML いたひ…
CFLAG:ARG:掌握把柄 -= 1
EXP:ARG:嗜虐快楽経験 += 2
BASE:MASTER:気力 -= 50
RETURN 0

;足を踏む
@PUNISHMENT_5(ARG,ARG:1)
PRINTFORML %CALLNAME:ARG%は%CALLNAME:PLAYER%の足をぐりっと踏みつけた
PRINTFORML アイタタタ
CFLAG:ARG:掌握把柄 -= 1
EXP:ARG:嗜虐快楽経験 += 2
BASE:MASTER:体力 -= 50
RETURN 0

;3回回ってワンと鳴け
@PUNISHMENT_6(ARG,ARG:1)
PRINTFORML %CALLNAME:ARG%は%CALLNAME:PLAYER%に犬のまねを求めてきた
PRINTFORML ぐる
PRINTFORML ぐる
PRINTFORML ぐる
PRINTFORML ワン！
PRINTFORML %CALLNAME:ARG%は口元だけ笑った
CFLAG:ARG:掌握把柄 -= 1
EXP:ARG:嗜虐快楽経験 += 5
BASE:MASTER:気力 -= 200
RETURN 0

;アナルパール装着
@PUNISHMENT_7(ARG,ARG:1)
SIF EXP:PLAYER:Ａ経験 < 10
	RETURN 1
SIF ABL:ARG:施虐属性 < 2
	RETURN 1
SIF TFLAG:パール期限
	RETURN 1
LOCAL = 4 + RAND:4
CALL PEARL_INSERT(ARG, LOCAL)

PRINTFORML %CALLNAME:ARG%は%CALLNAME:PLAYER%の菊花に肛門パールを挿入すると
PRINTFORML あと{TFLAG:パール期限 - TIME}分そのままで居るように命令して着衣の乱れを整えさせた…
CFLAG:ARG:掌握把柄 -= 3
EXP:ARG:嗜虐快楽経験 += 15

[SKIPSTART]
@PUNISHMENT_7(ARG,ARG:1)
PRINTFORMW %CALLNAME:ARG%対%CALLNAME:PLAYER%、下達了現在當場自慰的命令……
$LOOP
CALL ASK_M("勉強地做",1,"展示自慰的樣子",ABL:MASTER:露出癖 >= 3,"邊說喜歡邊做",ABL:MASTER:露出癖 >= 5)
SELECTCASE RESULT
	CASE 1
		PRINTFORMW %CALLNAME:MASTER%不情不願的開始了自慰、但是由於是在注視之下所以\@ TALENT:MASTER:性別 == 1? 完全沒有濕# 萎靡不振完全沒有勃起的樣子\@……
		IF ABL:ARG:欲望 < 5
			IF ABL:ARG:施虐属性 >= 3 && (TALENT:MASTER:性別 & 2)
				PRINTFORML %CALLNAME:ARG%用難聽的話罵著%CALLNAME:MASTER%
				PRINTFORMW %CALLNAME:MASTER%的心靈受到了深深地傷害
				BASE:MASTER:気力 -= 1000
				BASE:MASTER:勃起 = 0
				SOURCE:ARG:強要 += 300
				SOURCE:ARG:侮辱 += 300
				SOURCE:ARG:加虐 += 500
				CFLAG:ARG:掌握把柄 -= 2
				TIME += 3
			ELSEIF (TALENT:ARG:思慕 || TALENT:ARG:恋慕)
				PRINTFORML %CALLNAME:ARG%説道會有這種時候的、進行安慰
				PRINTFORMW %CALLNAME:MASTER%的心靈受到了傷害
				BASE:MASTER:気力 -= 300
				BASE:MASTER:勃起 = 0
				SOURCE:ARG:強要 += 200
				SOURCE:ARG:侮辱 += 200
				SOURCE:ARG:加虐 += 100
				SOURCE:ARG:情愛 += 50
				CFLAG:ARG:掌握把柄 -= 2
				TIME += 3
			ELSE
				PRINTFORML %CALLNAME:ARG%非常無聊的樣子、說道已經可以了
				PRINTFORMW %CALLNAME:MASTER%的心靈受到了傷害
				BASE:MASTER:気力 -= 500
				BASE:MASTER:勃起 = 0
				SOURCE:ARG:強要 += 200
				SOURCE:ARG:侮辱 += 200
				SOURCE:ARG:加虐 += 300
				CFLAG:ARG:掌握把柄 -= 1
				TIME += 3
			ENDIF
		ELSEIF ABL:ARG:施虐属性 >= 3
			PRINTFORML %CALLNAME:ARG%說著我來幫你吧、開始用腳摩擦著%CALLNAME:MASTER%的股間……
			PRINTFORML 於是、%CALLNAME:MASTER%的\@ TALENT:MASTER:性別 == 1? 私処立刻就変得濕潤了# 肉棒很快就恢復了硬度\@
			PRINTFORML 被這種樣子煽動起嗜虐心的%CALLNAME:ARG%、言辭和腳下的動作更加激烈了
			PRINTFORML 不久%CALLNAME:MASTER%発出了沒出息的呻吟達到了絕頂
			IF TALENT:MASTER:性別 & 2
				BASE:MASTER:勃起 = 500
				BASE:MASTER:精力 = MAX(BASE:MASTER:精力 - 100,0)
				EXP:MASTER:射精経験 ++
			ENDIF
			BASE:MASTER:気力 = MAX(BASE:MASTER:気力 - 300,0)
			TIME += 10
			EXP:MASTER:絶頂経験 ++
			EXP:MASTER:自慰経験 ++
			SOURCE:ARG:強要 += 500
			SOURCE:ARG:侮辱 += 500
			SOURCE:ARG:加虐 += 700
			SOURCE:ARG:征服 += 700
			SOURCE:ARG:達成 += 300
			CFLAG:ARG:掌握把柄 -= 5
		ELSE
			PRINTFORML %CALLNAME:ARG%掀起了%SHOW_BOTTOM(ARG,1)%、讓%UNDER_SKIRT_DESCRIPTION(ARG)%暴露出來…
			PRINTFORML 於是、%CALLNAME:MASTER%的\@ TALENT:MASTER:性別 == 1? 私処立刻就変得濕潤了# 肉棒很快就恢復了硬度\@
			PRINTFORML 被命令重新開始的%CALLNAME:MASTER%、在高潮一次之後還在持續著自慰
			IF TALENT:MASTER:性別 & 2
				BASE:MASTER:勃起 = 500
				EXP:MASTER:射精経験 ++
				BASE:MASTER:精力 = MAX(BASE:MASTER:精力 - 50,0)
			ENDIF
			BASE:MASTER:気力 = MAX(BASE:MASTER:気力 - 300,0)
			TIME += 10
			EXP:MASTER:絶頂経験 ++
			EXP:MASTER:自慰経験 ++
			SOURCE:ARG:強要 += 300
			SOURCE:ARG:侮辱 += 300
			SOURCE:ARG:挑発 += 300
			SOURCE:ARG:征服 += 200
			SOURCE:ARG:誘惑 += 100
			CFLAG:ARG:掌握把柄 -= 3
			TIME += 10 
		ENDIF
	CASE 2
		PRINTFORMW まだ未実装なんだぜ
		GOTO LOOP
	CASE 3
		PRINTFORMW まだ未実装なんだぜ
		GOTO LOOP
ENDSELECT
RETURN 0
PRINTFORML %CALLNAME:ARG%用充滿憐憫的視線看著%CALLNAME:MASTER%
PRINTFORML %CALLNAME:MASTER%的心靈受到了傷害


;[SKIPSTART]
PRINTFORML %CALLNAME:MASTER%非常喜歡%CALLNAME:ARG%、一邊高喊著最喜歡%CALLNAME:ARG%一邊開始了自慰
IF ABL:ARG:親密 < 5
	PRINTFORML %CALLNAME:ARG%慌張的命令停止
	BASE:MASTER:気力 -= 100
	SOURCE:ARG:強要 += 100
	SOURCE:ARG:侮辱 += 150
	SOURCE:ARG:情愛 += 50
	CFLAG:ARG:掌握把柄 -= 2
ELSE
	PRINTFORML %CALLNAME:ARG%有一點吃驚、但是並沒有感到討厭
	SIF ABL:ARG:施虐属性 >= 3
		PRINTFORML %CALLNAME:ARG%罵起了恬不知恥開始沉溺于快感中的%CALLNAME:MASTER%……
	IF ABL:MASTER:露出癖 >= 3 || ABL:MASTER:受虐属性 >= 3
		PRINTFORML 在%CALLNAME:ARG%的注視下感到興奮的%CALLNAME:MASTER%的自慰動作愈発激烈了……
		CFLAG:ARG:积攒度 += 150
	ENDIF
	IF ABL:ARG:欲望 > 5 && CFLAG:ARG:积攒度 > 500
		PRINTFORML 看著%CALLNAME:MASTER%自慰的樣子、%CALLNAME:ARG%也把手伸向了自己的私処……
		
ENDIF
[SKIPEND]
