	LOCAL = EXP:ARG:手淫経験
	LOCAL:1 = 10 + ABL:ARG:技巧 / 2 + TALENT:ARG:靈活手指 * 2 + TALENT:ARG:習得速度
	IF LOCAL * LOCAL:1 / 10 > EXPLV:(ABL:ARG:指 + 3) / 2; && EXP_UP(24,ARG)
		ABL:ARG:指 ++
		PRINTFORMW %CALLNAME:ARG%的手指变得灵活了
		IF ABL:ARG:指 >= 5 && !TALENT:ARG:靈活手指
			PRINTFORMW %CALLNAME:ARG%获得了[靈活手指]
			TALENT:ARG:靈活手指 = 1
		ENDIF
