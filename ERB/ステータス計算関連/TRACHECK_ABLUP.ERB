IF ABL:ARG:音楽技能 < 6
	IF (MAX(EXP:ARG:演奏経験, EXP:ARG:歌唱経験, EXP:ARG:舞踏経験) > EXPLV:(ABL:ARG:音楽技能 + 3) * 2) || (MAX(EXP:ARG:演奏経験, EXP:ARG:歌唱経験, EXP:ARG:舞踏経験) > 20 && ABL:ARG:音楽技能 == 0)
		ABL:ARG:音楽技能 ++
		PRINTFORMW %CALLNAME:ARG%的音楽技能变成了LV{ABL:ARG:音楽技能}
		SIF ARG == MASTER && ABL:ARG:音楽技能 == 2
			PRINTW 可以使用小提琴了

		[SKIPSTART]
		IF ABL:ARG:音楽技能 != 2
		ELSEIF ARG == MASTER
			PRINTW 可以使用小提琴了
		ELSEIF TALENT:ARG:音楽知識
		ELSE
			;おそらく、現状で経験を積む手段がない
			SELECTCASE MAX(EXP:ARG:演奏経験, EXP:ARG:歌唱経験, EXP:ARG:舞踏経験)
			CASE EXP:ARG:演奏経験
				PRINTFORMW 在経験的积累下%CALLNAME:ARG%可以在他人面前演奏楽器了
				TALENT:ARG:音楽知識 = 1
			CASE EXP:ARG:歌唱経験
				PRINTFORMW 在経験的积累下%CALLNAME:ARG%可以在他人面前展示歌喉了
				TALENT:ARG:音楽知識 = 2
			CASE EXP:ARG:舞踏経験
				PRINTFORMW 在経験的积累下%CALLNAME:ARG%可以在他人面前表演舞蹈了
				TALENT:ARG:音楽知識 = 3
			CASEELSE
		ENDIF
		[SKIPEND]
	ENDIF
ENDIF
