SIF TEQUIP:ARG:淋浴
	CALLFORM EQUIP_COM{COM淋浴}(ARG)

;新妻プレイ中
SIF TEQUIP:ARG:新妻PLAY
	CALLFORM EQUIP_COM{COM新妻PLAY}(ARG)

;Ｖ挿入中
SIF TEQUIP:ARG:50 >= 0 && !SOURCE:ARG:快Ｖ
	CALL EQUIP_INCERT_V(ARG)

;Ａ挿入中
SIF TEQUIP:ARG:51 >= 0 && !SOURCE:ARG:快Ａ
	CALL EQUIP_INCERT_A(ARG)

;相合い傘中
SIF TEQUIP:ARG:共用雨伞 && OUTROOF(CFLAG:ARG:現在位置) && ARG == TARGET
	CALLFORM EQUIP_COM{COM共用雨伞}(ARG)
RETURN 1
