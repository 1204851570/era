	CALLFORM EQUIP_COM{COM縄}(ARG)

;ボールギャグ装着中
SIF TEQUIP:ARG:口球
	CALLFORM EQUIP_COM{COM口球}(ARG)

;浣腸＋アナルプラグ装着中
SIF TEQUIP:ARG:浣腸
	CALLFORM EQUIP_COM{COM浣腸}(ARG)

;拡張バルーン装着中
SIF TEQUIP:ARG:拡張氣球
	CALLFORM EQUIP_COM{COM拡張氣球}(ARG)

;アナル電極装着中
SIF TEQUIP:ARG:肛門電極
	CALLFORM EQUIP_COM{COM肛門電極}(ARG)

;ビデオ撮影中
SIF ARG > 0 && TEQUIP:MASTER:视频拍摄
	CALLFORM EQUIP_COM{COM视频拍摄}(ARG)

;羞恥プレイ中
;SIF TEQUIP:ARG:30
;	CALL EQUIP_COM186(ARG)

;お風呂場プレイ中
SIF TEQUIP:ARG:浴室PLAY
	CALLFORM EQUIP_COM{COM浴室PLAY}(ARG)

