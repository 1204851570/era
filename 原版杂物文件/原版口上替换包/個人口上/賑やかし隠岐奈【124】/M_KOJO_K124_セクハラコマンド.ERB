			;グラインド
			CASE 3
				SELECTCASE RAND:2
					CASE 0
						PRINTFORMW 「ひゃ、ひぃんっ%UNICODE(0x2764)% こ、声が、でちゃうぅっ%UNICODE(0x2764)%」
					CASEELSE
						PRINTFORML 「ひっっっ、ふあぁっ%UNICODE(0x2764)% だめ、あぁぁ…っ%UNICODE(0x2764)%」
						PRINTFORMW 隠岐奈は蕩け切った表情で熱い息を漏らしている。
				ENDSELECT
		ENDSELECT
	ELSE
	ENDIF
ENDIF

RETURN 1
