RETURN RESULT

@M_KOJO_MESSAGE_COM_K124_310_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	IF FLAG:70
	ELSEIF CFLAG:睡眠
		PRINTFORML すやすやと沉眠著隠岐奈の尻を、思う存分撫で回した。
		PRINTFORML 「ん、ぅ……」
		PRINTFORMW 柔らかだが弾力のある、素敵な触り心地だ……。
	ELSEIF CFLAG:诶嘿嘿 == 2
		SELECTCASE TCVAR:20
			;アナル奉仕
			CASE 72
				SELECTCASE RAND:2
					CASE 0
						PRINTFORMW 「そ、そんなに熱心に……ひぅっ！」
					CASEELSE
						PRINTFORMW 「ほ、ほら、もっと精心……ひぃんっ」
				ENDSELECT
			;スカートの中を見せる
			CASE 35
				SELECTCASE RAND:2
					CASE 0
						PRINTFORMW 「はぁ…はぁ…さ、触るだけでいい、のか？ふふっ%UNICODE(0x2764)%」
					CASEELSE
						PRINTFORMW 「そんな、いやらしい手つきで…ううっ」
				ENDSELECT
			;素股
			CASE 52
				SELECTCASE RAND:2
					CASE 0
						PRINTFORMW 「あっ、やっ！う、動きづらいだろうっ！？」
					CASEELSE
						PRINTFORMW 「す、少しは我慢できないのか…っ！」
				ENDSELECT
			CASEELSE
				SELECTCASE RAND:2
					CASE 0
						PRINTFORMW 「ひゃっ！？や、やめろ…っ」
					CASEELSE
						PRINTFORMW 「お、お尻なんて触っても……うぅんっ%UNICODE(0x2764)%」
				ENDSELECT
		ENDSELECT
	ELSE
		IF TALENT:124:恋人 || TALENT:124:恋慕
			SELECTCASE TFLAG:193
				;不快
				CASE 1
					PRINTFORMW 「ひゃっ！？こ、こら！やめんか！」
				;恥ずかしがる
				CASE 2,3
					SELECTCASE RAND:2
						CASE 0
							PRINTFORMW 「そ…そんな、いやらしい手つきで…へ、変態め……！」
						CASEELSE
							PRINTFORMW 「う、うう……やめろと言うに……」
					ENDSELECT
				;されるがまま
				CASE 4
					SELECTCASE RAND:2
						CASE 0
							PRINTFORMW 「なんだ？そんなに興奮して……ふふっ」
						CASEELSE
							PRINTFORMW 「…私のお尻なんて触って、楽しいか？」
					ENDSELECT
			ENDSELECT
		ELSE
			SELECTCASE TFLAG:193
				;不快
				CASE 1
					PRINTFORML 「うわあっ！？ お、お前、いきなり何をする！！」
					PRINTFORMW 隠岐奈は%CALLNAME:MASTER%の手をぱしんと叩いた。
				;恥ずかしがる
				CASE 2,3
					SELECTCASE RAND:2
						CASE 0
							PRINTFORMW 「ひっ！？や、やめろ！変態！！」
						CASEELSE
							PRINTFORMW 「馴れ馴れしく触るな…っ」
					ENDSELECT
				;されるがまま
				CASE 4
					SELECTCASE RAND:2
						CASE 0
							PRINTFORML 「う、うぅ……」
							PRINTFORMW 隠岐奈は羞恥で顔を真っ赤にしている。
						CASEELSE
							PRINTFORMW 「こ、今回だけは許してやる……」
					ENDSELECT
			ENDSELECT
		ENDIF
	ENDIF
ENDIF

RETURN 1
