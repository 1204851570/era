;140,イラマチオ
;==================================================
@M_KOJO_MESSAGE_COM_K124_140
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K124_140_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K124_140_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;時姦中
	IF FLAG:70
		RETURN 1
	;睡姦中
	ELSEIF CFLAG:MASTER:恶作剧 == 2
		RETURN 1
	ELSEIF TCVAR:124:烂醉 == 1
		
	;それ以外
	ELSE
		SELECTCASE RAND:2
			CASE 0
				PRINTFORML 「ん……が、ぐうっ！？ぐっ、ごほ…ッ！！？」
				SIF ABL:124:受虐属性 >= 2
					PRINTFORML （く、苦しい…けど、もっとしてほしい……っ）
					PRINTFORMW 隠岐奈は酸欠と性的興奮でえずきながら顔を真っ赤にしている。
			CASEELSE
				PRINTFORML 「っぐ！？　ん、ごほっ、ぐぅ……ッ」
				SIF ABL:124:受虐属性 >= 2
					PRINTFORML （ああ、ゾクゾクする………っ）
					PRINTFORMW 隠岐奈は酸欠と性的興奮でえずきながら顔を真っ赤にしている。
		ENDSELECT
	ENDIF
ENDIF

RETURN 1
