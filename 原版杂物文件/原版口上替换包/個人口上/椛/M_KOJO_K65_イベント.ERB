					PRINTFORMW 「\@ TIME:2 >= 5 && TIME:2 <= 7 ? こんばんは # こんにちは \@、%CALLNAME:MASTER%さん」
			ENDSELECT
		ELSE
			SELECTCASE RAND:2 
				CASE 0
					PRINTFORML 「あ、お邪魔してます」
					PRINTFORMW 椛はぺこりと頭を下げた。
				CASE 1
					PRINTFORMW 「\@ TIME:2 >= 5 && TIME:2 <= 7 ? こんばんは # こんにちは \@、%CALLNAME:MASTER%さん」
			ENDSELECT
		ENDIF
		RETURN 1
	ENDIF
