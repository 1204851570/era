;==================================================
;相手が押し倒しを解除
;==================================================
@M_KOJO_EVENT_K70_30(ARG,ARG:1)
;口上主はTARGET
;--------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------

IF LOCAL
	;-------------------------------------------------
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
	;疲れた
		PRINTFORML 「ふぅ……疲れちゃいましたぁ」
		PRINTFORMW 「%CALLNAME:MASTER%さん、ごめんなさい%UNICODE(0x2764)%」
		TCVAR:70:351 = 0
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
	;満足して解放
		PRINTFORML 「ふぅ……%UNICODE(0x2764)%」
		PRINTFORMW 「%CALLNAME:MASTER%さん、ありがとうございます%UNICODE(0x2764)%」
		TCVAR:70:351 = 0
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 3
	;満足したところを押し倒された
		PRINTFORML 「きゃっ%UNICODE(0x2764)%」
		SELECTCASE RAND:2
		CASE 1
			PRINTFORMW 「あぁんっ%UNICODE(0x2764)% ゆるしてください～っ！」
		CASEELSE
			PRINTFORMW 「あぁんっ%UNICODE(0x2764)% ごめんなさい～っ！」
		ENDSELECT
		TCVAR:70:351 = 0
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 4
	;壊れるまでしてあげる
		;ダーク小鈴ちゃん
		PRINTFORML 「ふふふ……%UNICODE(0x2764)% %CALLNAME:MASTER%さん、いいんですね……%UNICODE(0x2764)%」
		PRINTFORMW 「ついにこの『サキュバスの園』の力を使うときがきたわぁ……%UNICODE(0x2764)%」
		RETURN 1
	ENDIF
ENDIF
RETURN 1




