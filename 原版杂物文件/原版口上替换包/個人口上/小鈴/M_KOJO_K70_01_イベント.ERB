;**** FlagManagement **********************************************************
;本口上にて使用しているCFLAG・TCVAR等
;TCVAR:70:350	フキゲン
;TCVAR:70:351	押し倒されたフラグ
;TCVAR:70:352	デート会話リセット用フラグ
;;TCVAR:70:353	嫉妬フキゲン※廃止
;TCVAR:70:354	今日お誘いしたか
;TCVAR:70:355	満足フラグ（TCVAR）
;TCVAR:70:356	オナバレ分岐
;TCVAR:70:357	連打防止
;TCVAR:70:360	一日一回,bit 0=デート同行会話, 1=仕事手伝いの一回目, 2=今日の来店
;TCVAR:70:361	会話：霊夢(1)
;TCVAR:70:362	会話：魔理沙(11)
;TCVAR:70:363	会話：紫(26)
;TCVAR:70:364	会話：文(29)
;TCVAR:70:365	会話：マミゾウ(69)
;TCVAR:70:366	会話：阿求(80)
;TCVAR:70:371	お茶：霊夢(1)
;TCVAR:70:372	お茶：魔理沙(11)
;TCVAR:70:373	お茶：紫(26)
;TCVAR:70:374	お茶：文(29)
;TCVAR:70:375	お茶：マミゾウ(69)
;TCVAR:70:376	お茶：阿求(80)
;CFLAG:70:1001	満足フラグ
;CFLAG:70:1002	最後に会った日
;CFLAG:70:1003	遭遇イベント
;CFLAG:70:1004	自己紹介
;CFLAG:70:1005	来店経験
;CFLAG:70:1006	うふふ口上
;CFLAG:70:1010	無自覚出産
;CFLAG:70:1011～1020	子供の性格
;CFLAG:70:1021～1030	親が分からない
;CFLAG:70:1100	バージョン
;
;******************************************************************************
;■専用関数
;鈴奈庵判定
;1=鈴奈庵,2=自分部屋（引っ越した後も考慮）,3=部屋追い出し
@M_KOJO_K70_SUZUNAAN(ARG)
#FUNCTION
IF MAIN_MAP == 2
	IF ARG == 213
		RETURNF 1
	ELSEIF ARG == 226
		;連れ出す関数ができてました＆特定条件で連れ出せるので対応しました
		IF COM_351_CONDITION(70)
			RETURNF 2
		;不法侵入,恋慕でも思慕でもなく阿求と一緒でもなく連れ出すこともできない,要するに時止めて侵入した（一応阿求と一緒に入って阿求が出て行くパターンもある）
		ELSEIF !TALENT:70:恋慕 && !TALENT:70:思慕 && CFLAG:MASTER:現在位置 != CFLAG:80:現在位置 && COM_351_CONDITION(70) == 0
			RETURNF 3
		ELSE
			RETURNF 2
		ENDIF
	ENDIF
ELSE
	IF ARG == 220
		RETURNF 1
	ELSEIF CFLAG:70:現在位置 == OMANEKIBEYA() || CFLAG:70:現在位置 == CFLAG:70:初始位置
		RETURNF 2
	ENDIF
ENDIF

;危ない所判定
@M_KOJO_K70_ABUNAAI(ARG)
#FUNCTION
SIF AT_HOME(70) && GROUPMATCH(MAIN_MAP, 3, 4, 5, 6, 7, 8, 9, 10)
	RETURNF 1

;分岐用のゲストキャラ判定
@M_KOJO_K70_GUEST(ARG)
#DIM CHARA
;ARG	0=会話, 1=お茶
VARSET RESULT
RESULTS = 
;阿求(80)は最優先
IF CFLAG:80:現在位置 == CFLAG:70:現在位置 && !TCVAR:70:(M_KOJO_K70_GUEST_TCVAR(80, ARG)) && M_KOJO_K70_GUEST_ABLE(80)
	RESULTS = %CALLNAME:80%
	RESULT:1 = 80
	RETURN M_KOJO_K70_BALANCE(70, 80)
ENDIF
FOR LOCAL, 1, 10
	;霊夢(1)、魔理沙(11)、紫(26)、文(29)、マミゾウ(69)、阿求(80)
	CHARA = TARGET:LOCAL
	IF GROUPMATCH(CHARA, 1, 11, 26, 29, 69, 80) && !TCVAR:70:(M_KOJO_K70_GUEST_TCVAR(CHARA, ARG)) && M_KOJO_K70_GUEST_ABLE(CHARA)
		RESULTS = %CALLNAME:CHARA%
		RESULT:1 = CHARA
		;台詞を先に考えてたらよくわからん分岐になってしまった
		;恋慕の小鈴と同行中（デート含む）
		IF CFLAG:70:同行 && TALENT:70:恋慕
			RETURN 4
		;鈴奈庵でゲストと同行中
		ELSEIF CFLAG:CHARA:同行 && M_KOJO_K70_SUZUNAAN(CFLAG:70:現在位置) == 1
			RETURN 3
		;ゲストとデート中
		ELSEIF CFLAG:CHARA:同行 && FLAG:约会的对象 == CHARA
			RETURN 2
		;恋慕のゲストと同行中
		ELSEIF CFLAG:CHARA:同行 && TALENT:CHARA:恋慕
			RETURN 2
		;小鈴が恋慕
		ELSEIF TALENT:70:恋慕
			RETURN 4
		ELSE
			RETURN 1
		ENDIF
	ENDIF
NEXT

;ゲスト可否
@M_KOJO_K70_GUEST_ABLE(ARG)
#FUNCTION
;睡眠中
SIF CFLAG:ARG:睡眠
	RETURNF 0
SIF CFLAG:70:睡眠
	RETURNF 0
;添い寝中
SIF CFLAG:ARG:陪睡中
	RETURNF 0
SIF CFLAG:70:陪睡中
	RETURNF 0
;浴場
SIF BATHCHECK(CFLAG:MASTER:現在位置)
	RETURNF 0
RETURNF 1

;ゲストキャラ変換用
@M_KOJO_K70_GUEST_TCVAR(ARG, ARG:1)
#FUNCTION
VARSET LOCAL
SELECTCASE ARG
;霊夢
CASE 1
	LOCAL = 361
;魔理沙
CASE 11
	LOCAL = 362
;紫
CASE 26
	LOCAL = 363
;文
CASE 29
	LOCAL = 364
;マミゾウ
CASE 69
	LOCAL = 365
;阿求
CASE 80
	LOCAL = 366
ENDSELECT
SIF ARG:1
	LOCAL += 10
RETURNF LOCAL

;関係性比較関数,小鈴と阿求にしか使ってないけど一応汎用できるように
@M_KOJO_K70_BALANCE(ARG:1, ARG:2)
#FUNCTION
;ARG:1	主対象
;ARG:2	比較対象
;独占
IF TALENT:(ARG:1):恋人
	RETURNF 8
;悲恋
ELSEIF TALENT:(ARG:2):恋人 && TALENT:(ARG:1):恋慕
	RETURNF 7
;揶揄
ELSEIF TALENT:(ARG:2):恋人
	RETURNF 6
;信頼
ELSEIF TALENT:(ARG:1):恋慕 && (TALENT:(ARG:2):恋慕 || TALENT:(ARG:2):思慕) && CFLAG:(ARG:1):信頼度 > 1000
	RETURNF 5
;敵対
ELSEIF TALENT:(ARG:1):恋慕 && (TALENT:(ARG:2):恋慕 || TALENT:(ARG:2):思慕)
	RETURNF 4
;恋慕
ELSEIF TALENT:(ARG:1):恋慕
	RETURNF 3
;応援
ELSEIF TALENT:(ARG:2):恋慕
	RETURNF 2
;平穏
ELSE
	RETURNF 1
ENDIF

