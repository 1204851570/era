		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1
			SELECTCASE ARG:1
			CASE 0
				;-------------------------------------------------
				;交換です
				;記入チェック（=0, 非表示、1, 表示）
				LOCAL:1 = 1
				;-------------------------------------------------
				IF LOCAL:1
					PRINTFORMW 「え……\/\/\/ じゃ、じゃあ%CALLNAME:MASTER%さんのを私にください……\/\/\/」
					RETURN 1
				ENDIF
			CASE 1
				;-------------------------------------------------
				;OK（嬉々
				;記入チェック（=0, 非表示、1, 表示）
				LOCAL:1 = 1
				;-------------------------------------------------
				IF LOCAL:1
					PRINTFORMW 「え……\/\/\/ そんな……汚いですよぅ……\/\/\/」
					RETURN 1
				ENDIF
			CASE 2
				;-------------------------------------------------
				;OK（渋々
