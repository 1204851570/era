@M_KOJO_MESSAGE_COM_K103_0_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;1行入れたい
	PRINTFORML 
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORML 「あっ、稍微、やっ、くすぐったい……んっ…」
		PRINTFORML 
		PRINTFORMDW %CALLNAME:MASTER%の愛撫がこそばゆいのか、夢子は身をよじっている
		PRINTFORMDW しかし時が経つにつれ、次第に夢子の息が荒くなっていく…
		PRINTFORMDW じわりと湿り気を帯びた陰部を%M_KOJO_K103_ZURASHI()%指で悄悄的なぞると、小小的喘ぎ声を漏らした…
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;挿入中
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	;V挿入
	IF LOCAL:1 && TEQUIP:50 == PLAYER
		;射精した
		IF NOWEX:PLAYER:11
			PRINTFORML
			CALL M_KOJO_K103_CALL_ZAMEN()
			RETURN 1
		ELSE
			PRINTFORMW 「私の%M_KOJO_K103_GET("ヴァギナ")%、ぐちゅぐちゅ言ってるっ……」
			;RETURN 1
		ENDIF
	;A挿入
	ELSEIF LOCAL:1 && TEQUIP:51 == PLAYER
		;射精した
		IF NOWEX:PLAYER:11
			PRINTFORML
			CALL M_KOJO_K103_CALL_ZAMEN()
			RETURN 1
		ELSE
			PRINTFORMW 「%M_KOJO_K103_GET("菊花")%…、あ゛っっ……」
			;RETURN 1
		ENDIF
	ENDIF
	;-------------------------------------------------
	;ふたなり
	;記入チェック
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && (TALENT:2 & 3) == 3
		PRINTFORMW 
		RETURN 1
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMW 
		RETURN 1
	;恋慕
	ELSEIF TALENT:恋慕 || TALENT:愛欲
		SELECTCASE M_KOJO_K103_RANDNR(8, 911)
		CASE 1
			PRINTFORMW 「あっ、……んっ…%M_KOJO_K103_HEART()%」
			PRINTFORMW 「もっと……もっと触って……%M_KOJO_K103_HEART()%」
		CASE 2
			PRINTFORMW 「はぁ……んっ…耳弱いの…%M_KOJO_K103_HEART()%」
			PRINTFORMW 「%M_KOJO_K103_AEGI()%……%M_KOJO_K103_HEART()%」
		CASE 3
			PRINTFORMW 「あっ、そこ気持ちいい…%M_KOJO_K103_HEART()%」
			PRINTFORMW 「もっと……もっと……%M_KOJO_K103_HEART()%」
		CASE 4
			PRINTFORMW 「ああんっ……触り方がいやらしい…%M_KOJO_K103_HEART()%」
			PRINTFORMW 「あっ…でもっ………良いの、気持ちよくって…あぁっ…%M_KOJO_K103_HEART()%」
		CASE 5
			PRINTFORMW 「はぁ……ぁ…%M_KOJO_K103_HEART()%」
			PRINTFORMW 「ここ、こっち、もっと弄って……%M_KOJO_K103_HEART()%」
		CASE 6
			PRINTFORMW 「あんっ、………%M_KOJO_K103_HEART()%」
			PRINTFORMW 「あっ、あっ、だめ、クリをそんなに……%M_KOJO_K103_HEART()%」
		CASE 7
			PRINTFORMW 「んんんっ…胸、胸もっと触って…%M_KOJO_K103_HEART()%」
			PRINTFORMW 「あっ…気持ちい……%M_KOJO_K103_HEART()%」
		CASEELSE
			PRINTFORMW 「もっと色んなところ弄ってほしいの…%M_KOJO_K103_HEART()%」
			PRINTFORMW 「あっ…そう……ああんっ……%M_KOJO_K103_HEART()%」
		ENDSELECT
		RETURN 1
	;屈服3
	;ELSEIF MARK:不埒刻印 == 3
	;	PRINTFORMW 
	;	RETURN 1
	;それ以外
	ELSE
		SELECTCASE M_KOJO_K103_RANDNR(8, 911)
		CASE 1
			PRINTFORMW 「あっ、……んっ…」
			PRINTFORMW 「あ、そこ気持ちいい……%M_KOJO_K103_HEART()%」
		CASE 2
			PRINTFORMW 「はぁ……んっ…耳…」
			PRINTFORMW 「%M_KOJO_K103_AEGI()%……%M_KOJO_K103_HEART()%」
		CASE 3
			PRINTFORMW 「あっ、そこ、そこ…」
			PRINTFORMW 「あっ…あっ……%M_KOJO_K103_HEART()%」
		CASE 4
			PRINTFORMW 「ああんっ……触り方がいやらしい…」
			PRINTFORMW 「あっ…でもっ………気持ちよくって…あぁっ…%M_KOJO_K103_HEART()%」
		CASE 5
			PRINTFORMW 「はぁ……ぁ…」
			PRINTFORMW 「ここ、こっち、こっちが気持ちいいの……%M_KOJO_K103_HEART()%」
		CASE 6
			PRINTFORMW 「あんっ、………%M_KOJO_K103_HEART()%」
			PRINTFORMW 「あっ、だめ、クリをそんなに……%M_KOJO_K103_HEART()%」
		CASE 7
			PRINTFORMW 「んんっ…胸感じちゃう…」
			PRINTFORMW 「あっ…気持ちい……%M_KOJO_K103_HEART()%」
		CASEELSE
			PRINTFORMW 「体中弄られて…んんっ」
			PRINTFORMW 「あっ…だめ……ああんっ……%M_KOJO_K103_HEART()%」
		ENDSELECT
		RETURN 1
	ENDIF
ENDIF
RETURN 1
