		CASE 1
			PRINTFORMW 「あっ、……んっ…%M_KOJO_K103_HEART()%」
			PRINTFORMW 「どう…美味しい……？%M_KOJO_K103_HEART()%」
		CASE 2
			PRINTFORMW 「%M_KOJO_K103_AEGI()%…%M_KOJO_K103_HEART()%」
			PRINTFORMW 「ぺろぺろ…気持ちい…%M_KOJO_K103_HEART()%」
		CASE 3
			PRINTFORMW 「あっ、そこ……うん、クリの所…%M_KOJO_K103_HEART()%」
			PRINTFORMW 「もっと……もっと……%M_KOJO_K103_HEART()%」
		CASE 4
			PRINTFORMW 「あっ、あっクリのとこ吸うとダメっ…%M_KOJO_K103_HEART()%」
			PRINTFORMW 「あっ……あぁっ…%M_KOJO_K103_HEART()%」
		CASE 5
			PRINTFORMW 「はぁ…%M_KOJO_K103_AEGI()%…ぁ…%M_KOJO_K103_HEART()%」
			PRINTFORMW 「ここ、こっち、もっと弄って……%M_KOJO_K103_HEART()%」
		CASE 6
			PRINTFORMW 「あんっ、………%M_KOJO_K103_HEART()%」
			PRINTFORMW 「あっ、あっ、だめ、クリをそんなに……%M_KOJO_K103_HEART()%」
		CASE 7
			PRINTFORMW 「あっ……中に舌が入ってくるっ…%M_KOJO_K103_HEART()%」
			PRINTFORMW 「あっ…なかっ…なかぁ………%M_KOJO_K103_HEART()%」
		CASEELSE
			PRINTFORMW 「%M_KOJO_K103_AEGI()%…%M_KOJO_K103_HEART()%」
			PRINTFORMW 「んっ%M_KOJO_K103_AEGI()%そんな必死に…っ%M_KOJO_K103_HEART()%」
		ENDSELECT
		RETURN 1
	;屈服3
	;ELSEIF MARK:不埒刻印 == 3
	;	PRINTFORMW 
	;	RETURN 1
	;それ以外
	ELSE
		SELECTCASE M_KOJO_K103_RANDNR(8, 912);
		CASE 1
			PRINTFORMW 「あっ、……んっ…%M_KOJO_K103_HEART()%」
			PRINTFORMW 「そんなところ…美味しくないわよ……？%M_KOJO_K103_HEART()%」
		CASE 2
			PRINTFORMW 「%M_KOJO_K103_AEGI()%…%M_KOJO_K103_HEART()%」
			PRINTFORMW 「指とは…っ…違う感じで…%M_KOJO_K103_HEART()%」
		CASE 3
			PRINTFORMW 「あっ、クリばっかり…%M_KOJO_K103_HEART()%」
			PRINTFORMW 「あっ…あっ……%M_KOJO_K103_HEART()%」
		CASE 4
			PRINTFORMW 「あっ、あっクリのとこ吸うとダメっ…%M_KOJO_K103_HEART()%」
			PRINTFORMW 「あっ……あぁっ…%M_KOJO_K103_HEART()%」
		CASE 5
			PRINTFORMW 「はぁ…%M_KOJO_K103_AEGI()%…ぁ…%M_KOJO_K103_HEART()%」
			PRINTFORMW 「ここ、こっち、もっと弄って……%M_KOJO_K103_HEART()%」
		CASE 6
			PRINTFORMW 「あんっ、………%M_KOJO_K103_HEART()%」
			PRINTFORMW 「あっ、あっ、だめ、クリをそんなに……%M_KOJO_K103_HEART()%」
		CASE 7
