;80,手淫
;==================================================
@M_KOJO_MESSAGE_COM_K103_80
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K103_80_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K103_80_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORMW 「手でして欲しいのね？」
		PRINTFORMW 「ん…と、こう…？」
		PRINTFORML 
		PRINTFORMDL もう稍微強く握っても大丈夫というと、おっかなびっくり力を込めて夢子はこすり始めた
		PRINTFORMW 
		PRINTFORMW 「ん…気持ちいい…？ どう…？」
		PRINTFORML
		RETURN 1
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMW 
		RETURN 1
	;恋慕
	ELSEIF TALENT:恋慕
		SELECTCASE M_KOJO_K103_RANDNR(4,82)
		CASE 1
			PRINTFORMW 「んしょ…んしょ……どう？ 気持ちいい？%M_KOJO_K103_HEART()%」
		CASE 2
			PRINTFORMW 「ふふ…%M_KOJO_K103_GET("阴茎")%あったかい%M_KOJO_K103_HEART()%」
		CASE 3
			PRINTFORMW 「こしこし…こしこし……%M_KOJO_K103_HEART()%」
		CASEELSE
			PRINTFORMW 「先っぽの方もこうやって手のひらで弄ると気持ちいいのよね…？%M_KOJO_K103_HEART()%」
		ENDSELECT 
		SIF NOWEX:MASTER:射精
			PRINTFORMW 「あっ…膨らんできた…イキそう？ いいわよ…見せて……%M_KOJO_K103_HEART()%」
		PRINTFORML
		RETURN 1
	;屈服3
	;ELSEIF MARK:不埒刻印 == 3
	;	PRINTFORMW 
	;	RETURN 1
	;それ以外
	ELSE
		PRINTFORMW 
		RETURN 1
	ENDIF
ENDIF
RETURN 1

