		CASE 1
			PRINTFORML 「もしもーし」
			PRINTFORML 「私、今から%CALLNAME:MASTER%を食べちゃうの……なーんてね%UNICODE(0x2665) *1%」
		CASEELSE
			PRINTFORML 「むっふっふ、%CALLNAME:MASTER%……」
			PRINTFORML 「とっても気持ちいいことしてあげるね%UNICODE(0x2665) *1%」
		ENDSELECT
		IF TALENT:38:恋慕
			PRINTFORMW 「ねぇ……%CALLNAME:MASTER%、私に愛の感情をくださいな%UNICODE(0x2665) *1%」
		ELSE
			PRINTFORMW 「一緒に、深い深い無意識の淵に堕ちていこう%UNICODE(0x2665) *1%」
		ENDIF
		;妊娠中
		IF TALENT:38:妊娠 && CFLAG:38:妊娠自覚状態 == 1
			PRINTFORML 
			PRINTFORMW 「妊婦に好き勝手されちゃうの……興味ない%UNICODE(0x2665) *1%」
		;危険日
		ELSEIF CFLAG:38:生理周期 == 7 && !TALENT:38:妊娠 && CFLAG:38:允许无套 == 2
			PRINTFORML 
			PRINTFORMW 「今日はいっぱいいっぱい私に注いでね♪」
			PRINTFORMW 「%CALLNAME:MASTER%の兒童、孕みたい気分なの%UNICODE(0x2665) *1%」
		ENDIF
		TCVAR:38:351 = 1
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;なだめられた（終了時）
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
		SELECTCASE ARG:1
		CASE 0
			;抜け出した
			IF TALENT:38:恋慕
				;妊娠中
				IF TALENT:38:妊娠 && CFLAG:38:妊娠自覚状態 == 1
					PRINTFORML 「ぶーぶーいけずぅ……」
					PRINTFORMW 「うん……でも、しょうがないかぁ……」
					;我慢しません
				;危険日
				ELSEIF CFLAG:38:生理周期 == 7 && !TALENT:38:妊娠 && CFLAG:38:允许无套 == 2
					PRINTFORML 「こぉら……、逃げちゃダメぇ……」
					PRINTFORMW 「むぅ……まだ満足できてないのにー」
				ELSE
					SELECTCASE RAND:3
					CASE 0
						PRINTFORML 「えぇ……、そんなぁ……」
						PRINTFORMW 「うーん、私の体、魅力ないのかなぁ」
					CASE 1
						PRINTFORML 「あーん、そんなぁ……」
						PRINTFORMW 「もっと楽しいことしようよ」
					CASEELSE
						PRINTFORML 「ありゃりゃ……」
						PRINTFORMW 「むむ、もしかしてあんまり気持ち良くなかった？」
					ENDSELECT
				ENDIF
			ELSE
				PRINTFORML 「むむ……、逃げられちゃった……」
				PRINTFORMW 「もしもーし、私の体じゃ満足できませんかー？」
			ENDIF
		CASE 1
		;抱き着きモード満足終了
			RETURN 1
		CASE 2
		;抱き着きモード強制終了
			RETURN 1
		ENDSELECT
		TCVAR:38:351 = 0
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;抜け出す失敗
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 3
		SELECTCASE ARG:1
		CASE 1
		;好きにして状態から抜け出せなかった
			PRINTFORML 「だぁ～め、逃がさないよー」
			PRINTFORMW 「こんなに私を昂らせた責任、取ってもらうんだから♪」
			RETURN 1
		CASE 2
			SELECTCASE RAND:3
			CASE 0
				PRINTFORMW 「だーめ、もっと私を気持ちよくして……ね？」
			CASE 1
				PRINTFORML 「もう、暴れちゃって、カワイイんだから♪」
				PRINTFORMW 「大丈夫……一緒に気持ち良くなろ♪　ね%UNICODE(0x2665) *1%」
			CASEELSE
				PRINTFORMW 「ほーら捕まえた！　逃げようたってそうはいかないんだから♪」
			ENDSELECT
			RETURN 1
		ENDSELECT
	ENDIF
	;その他
	PRINTFORML  
ENDIF
RETURN 1

