	ARRAYREMOVE LOCAL, FINDELEMENT(LOCAL, 63), 1
	ARRAYREMOVE LOCAL, FINDELEMENT(LOCAL, MASTER), 1
	ARRAYREMOVE LOCAL, FINDELEMENT(LOCAL, RANDOM_CHARANUM), 1
	;FUNC_FISHER_YATES_SHAFFLE内で-1で初期化されるのでこれで全配列対象処理になる
	;場所判定がよくわからんことになってたのでとりあえず近場のキャラ？にしてみるよ
	FOR LOCAL_ID, 0, FINDELEMENT(LOCAL, -1)
		;同じマップにいるキャラ限定
		SIF GET_MAPID(CFLAG:(LOCAL:LOCAL_ID):現在位置) != GET_MAPID(CFLAG:63:現在位置)
			CONTINUE
		;同じ場所のキャラは除外
		SIF CFLAG:(LOCAL:LOCAL_ID):現在位置 == CFLAG:63:現在位置
			CONTINUE
		PRINTFORMW 「ｸﾝｸﾝ……これは%CALLNAME:(LOCAL:LOCAL_ID)%の匂いね」
		BREAK
	NEXT
ENDIF
RETURN 1
