;会話セット
;==================================================
@M_KOJO_K83_TALKSET(ARGS)
SELECTCASE ARGS
CASE "汎用"
	IF TALENT:[[琪斯美]]:恋慕
		SELECTCASE RAND:5
		CASE 1
			PRINTFORMW 「わたしも昔は……」
		CASE 2
			PRINTFORMW 「えっと……多分……」
		CASE 3
			PRINTFORMW 「うん……そうだね」
		CASE 4
			PRINTFORMW 「昨日はこんな事が……」
		CASEELSE
			PRINTFORMW 「あのね、%CALLNAME:MASTER%……」
		ENDSELECT
	ELSE
		SELECTCASE RAND:4
		CASE 1
			PRINTFORMW 「えっと……その……」
		CASE 2
			PRINTFORMW 「う…うん……」
		CASE 3
			PRINTFORMW 「あの……だから……」
		CASEELSE
			PRINTFORMW 「%CALLNAME:MASTER%は……」
		ENDSELECT
	ENDIF
CASE "キャラ"
	SELECTCASE RAND:7
	CASE 1
		PRINTFORMW 「山女魚は……わたしが生まれた時から一緒に居てくれてる……大切な人」
	CASE 2
		PRINTFORMW 「琶露西は……根は優しい人。わたしにも温柔的してくれる……」
	CASE 3
		PRINTFORML 「勇儀さんは……誰にでも同じ扱いしてくれる……」
		PRINTFORMW 「でも、わたしは弱っちいから……あんまり……」
	CASE 4
		PRINTFORMW 「覚さんは……外に出てこないから、あんまり見ない……」
	CASE 5
		PRINTFORMW 「燐ちゃんは……ときどき構ってくれる……」
		PRINTFORMW 「でも、あの猫車には入れさせてもらえない……」
	CASE 6
		PRINTFORMW 「空ちゃんは……稍微何考えるか判らない……」
	CASEELSE
		PRINTFORMW 「こ、恋ちゃんは……あ、あんまり見たことない……」
	ENDSELECT
CASE "思慕"
	SELECTCASE RAND:5
	CASE 1
		PRINTFORMW 「そ、外の世界の事……聞きたい……」
	CASE 2
		PRINTFORMW 「この桶は……わたしの一部、みたいな……」
	CASE 3
		IF MAIN_MAP == 9
			PRINTFORMW 「ち、地底の暮らしはどう……？」
		ELSE
			PRINTFORMW 「ち、地底は楽しいよ……多分……」
		ENDIF
	CASE 4
		PRINTFORMW 「%CALLNAME:MASTER%は人类なのに……変」
	CASEELSE
		PRINTFORMW 「釣瓶落は……鬼火を出したりする……」
	ENDSELECT
CASE "酔酒"
	SELECTCASE RAND:4
	CASE 1
		PRINTFORML 「へへへ……ふふ」
	CASE 2
		PRINTFORML 「えへへへへ……」
	CASE 3
		PRINTFORML 「ふへへ……ひっく！」
	CASEELSE
		PRINTFORML 「ふふっ、あははっ！」
	ENDSELECT
	PRINTFORMDW 琪斯美は酔っている……。
ENDSELECT
