						PRINTFORMW 「人类がここにいるのは危ないと思う……」
					ELSE
						PRINTFORMW 「な、なんとなく来ただけ……」
					ENDIF
				CASEELSE
					CALL M_KOJO_K83_TALKSET("汎用")
				ENDSELECT
			ENDIF
		ENDIF
	ENDIF
ENDIF
RETURN 1
