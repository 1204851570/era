			CASE 4
				PRINTFORMW 「%CALLNAME:MASTER%は人类なのに強いね……」
			CASE 5
				PRINTFORMW 「外の世界から井戸が無くなっていくから、ここには井戸がたくさんあるの……」
			CASE 6
				PRINTFORML 「最近はバケツが流行ってる……」
				PRINTFORMW 「わ、わたしには稍微……」
			CASE 7
				PRINTFORMW 「風が吹けば桶屋が儲かる……？ 桶屋……？？」
			CASE 8
				PRINTFORMW 「狭窄ところは落ち着くから好き……」
			CASE 9
				PRINTFORMW 「鬼火っていうのは……その……火……」
			CASE 10
				PRINTFORML 「たまに井戸を通じて外の世界と繋がるの……」
				PRINTFORMW 「ひ、秘密にしてね……」
			CASE 11
				PRINTFORMW 「ど、どうしたら強くなれるかな……？」
			CASE 12
				CALL M_KOJO_K83_TALKSET("汎用")
			CASE 13
				CALL M_KOJO_K83_TALKSET("思慕")
			CASEELSE
				CALL M_KOJO_K83_TALKSET("キャラ")
			ENDSELECT
		;思慕
		ELSEIF TALENT:[[琪斯美]]:思慕
			IF !RAND:6
				CALL M_KOJO_K83_TALKSET("汎用")
			ELSE
				CALL M_KOJO_K83_TALKSET("思慕")
			ENDIF
		;それ以外
		ELSE
			;地底住まい
			IF MAIN_MAP == 9
				SELECTCASE RAND:6
				CASE 1
					PRINTFORMW 「外から来たんだ……ふーん……」
				CASE 2
					PRINTFORMW 「あんまり話すのは……苦手……」
				CASE 3
					PRINTFORMW 「ち、地底は……どう？」
				CASE 4
					PRINTFORMW 「い、色々あったんだね……」
				CASE 5
					PRINTFORMW 「人类がここにいるのは危ないと思う……」
				CASEELSE
					CALL M_KOJO_K83_TALKSET("汎用")
				ENDSELECT
			;それ以外
			ELSE
				SELECTCASE RAND:6
				CASE 1
					PRINTFORMW 「外から来たんだ……ふーん……」
				CASE 2
					PRINTFORMW 「あんまり話すのは……苦手……」
				CASE 3
					PRINTFORMW 「わ、わたしは地底に住んでる……」
				CASE 4
