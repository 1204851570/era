	ELSEIF TCVAR:[[琪斯美]]:会話不能
		IF TALENT:[[琪斯美]]:恋慕
			PRINTFORMW 「そ、そうだね……」
		ELSEIF TALENT:[[琪斯美]]:思慕
			PRINTFORMW 「な、なに……？」
		ELSE
			PRINTFORMW 「…………」
		ENDIF
	;不機嫌
	ELSEIF M_KOJO_K83_FUKIGEN([[琪斯美]])
		IF TALENT:[[琪斯美]]:恋慕
			PRINTFORMW 「うん……うん……」
		ELSE
			PRINTFORMW 「うん…………」
		ENDIF
;	;デート会話,未定
;	ELSEIF TALENT:[[キスメ]]:恋慕 && FLAG:デート相手 == [[キスメ]] && DATENAME_SPOT() != "道中" && !デート会話:(CFLAG:[[キスメ]]:現在位置 / 10)
	;くしゃみ
	ELSEIF !RAND:200
		PRINTFORMW 「くちゅんっ！ ……ずず」
	;通常
	ELSE
