	LOCALS = [{番号_簡易泳池}] 簡易泳池
	PRINTBUTTONLC LOCALS, 番号_簡易泳池
ENDIF
PRINT [100] - やっぱりやめる
INPUT
SELECTCASE RESULT
CASE 100
	RETURN
CASE 番号_簡易泳池
	SIF !ITEM:簡易泳池
		RETURN
	PRINTL 「どう改造する？」
	PRINT 現在的状態：
	SIF GETBIT (FLAG:泳池, 0)
		PRINT [自走马达]
	SIF GETBIT (FLAG:泳池, 1)
		PRINT [温水機能]
	SIF GETBIT (FLAG:泳池, 2)
		PRINT [淋浴]
	SIF GETBIT (FLAG:泳池, 3)
		PRINT [不思議な泡]
	PRINTL
	
	IF !GETBIT (FLAG:泳池, 0)
		PRINTL [1] - 马达（\5000）：移動の負担がなくなる
	ELSE
		SETCOLOR 150,150,150
		PRINTL [済] - 马达：移動の負担がなくなる
		RESETCOLOR
	ENDIF
	IF !GETBIT (FLAG:泳池, 1)
		PRINTL [2] - 温水機能（\5000）：广阔場所でくつろげる
	ELSE
		SETCOLOR 150,150,150
		PRINTL [済] - 温水機能：广阔場所でくつろげる・浴室PLAYができる
		RESETCOLOR
	ENDIF
	IF !GETBIT (FLAG:泳池, 2)
		PRINTL [3] - 淋浴（\5000）：淋浴で洗える・澆水もできる
	ELSE
		SETCOLOR 150,150,150
		PRINTL [済] - 淋浴：シャワーで洗える・澆水もできる
		RESETCOLOR
	ENDIF
	SIF FLAG:泳池 < 7
		PRINTFORML [4] - ３つとも改造（１つにつき\\5000）
	IF FLAG:泳池 < 7
		PRINTL [?] - ？？？？？
	ELSEIF !GETBIT (FLAG:泳池, 3) && FLAG:泳池 < 8
		PRINTL [5] - 不思議な泡（\5000）：若鷺姫が自由に移動できるようになる
	ELSE
		SETCOLOR 150,150,150
		PRINTL [済] - 不思議な泡：若鷺姫が自由に移動できるようになる
		RESETCOLOR
		PRINTL これ以上改造できない
	ENDIF
	
	PRINTL [100]やっぱりやめる
	INPUT
	SELECTCASE RESULT
	CASE 100
		RETURN
	CASE 1
		IF !GETBIT (FLAG:泳池, 0)
			PRINTL 移動の負担がなくなった！
			SETBIT FLAG:泳池, 0
			MONEY -= 5000
		ELSE
			RETURN
		ENDIF
	CASE 2
		IF !GETBIT (FLAG:泳池, 1)
			PRINTL 广阔場所でくつろげるようになった！
			PRINTL 浴室PLAYができるようになった！
			SETBIT FLAG:泳池, 1
			MONEY -= 5000
		ELSE
			RETURN
		ENDIF
	CASE 3
		IF !GETBIT (FLAG:泳池, 2)
			PRINTL 淋浴が使えるようになった！
			PRINTL 澆水が楽になった！
			SETBIT FLAG:泳池, 2
			MONEY -= 5000
		ELSE
			RETURN
		ENDIF
	CASE 4
