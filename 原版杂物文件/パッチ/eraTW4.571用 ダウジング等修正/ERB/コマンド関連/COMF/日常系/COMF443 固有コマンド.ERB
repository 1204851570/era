相手残機 = 3
残りHP = MAXBASE:MASTER:体力
残りボム回数 = 2
ターン数 = 1

WHILE プレイヤー残機 > 0 && 相手残機 > 0
	;ダイス数計算
	プレイヤーB数 = ABL:MASTER:戦闘能力 - 1 + RAND:5
	相手B数 = ABL:TARGET:戦闘能力 + RAND:4
	SIF プレイヤーB数 <= 0
		プレイヤーB数 = 1
	SIF 相手B数 <= 0
		相手B数 = 1
	SIF 相手残機 == 1
		相手B数 ++
	;ダイス面計算
	プレイヤーB面 = (ABL:MASTER:戦闘能力 + 1) * (5 + RAND:9) + RAND:17
	相手B面 = (ABL:TARGET:戦闘能力 + 1) * (8 + RAND:4) + RAND:7 - (相手残機 - 1) * ABL:TARGET:戦闘能力
