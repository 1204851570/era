CASE "固有下着着用条件"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有下着透ける"
	CALLF MAKE_INT(V_NAME, 1)

CASE "所持楽曲"
{
	CALLF MAKE_STR(V_NAME, 
	"東方妖恋談"
	 + "/" + "Witching Dream"
	 + "/" + "少女綺想曲　～ Capriccio"
	 + "/" + "永夜の報い　～ Imperishable Night."
	 + "/" + "少女綺想曲　～ Dream Battle"
	 + "/" + "春色小径　～ Colorful Path"
	 )
}

;農民関連
;神社は土地持ってそう、神田
CASE "農民：説明"
	CALLF MAKE_STR(V_NAME, "米農家")
CASE "農民：作物"
	CALLF MAKE_STR(V_NAME, "米")
CASE "農民：田的种类"
	CALLF MAKE_STR(V_NAME, "水田")
CASE "農民：畑の所在地"
	CALLF MAKE_STR(V_NAME, "博麗神社")

CASE "農民：MapID"
	CALLF MAKE_INT(V_NAME, 0)
CASE "農民：場所ID"
	CALLF MAKE_INT(V_NAME, 22)
CASE "農民：報酬"
	CALLF MAKE_INT(V_NAME, 400)
CASE "農民：金利"
	CALLF MAKE_INT(V_NAME, 5)

;ここは依頼IDが関連しない情報
;依頼発生率スカ2/一般3/固有5
CASE "没有依頼発生率"
	CALLF MAKE_INT(V_NAME, 2)
CASE "一般依頼発生率"
	CALLF MAKE_INT(V_NAME, 3)
CASE "固有依頼発生率"
	CALLF MAKE_INT(V_NAME, 5)
CASE "固有依頼最終要素"
	CALLF MAKE_INT(V_NAME, 103)
CASEELSE
	;固有依頼関連のデータにバトンタッチ
	SIF STRCOUNT(O_DATA, "依頼")
		CALLF 固有依頼1(ARG, O_DATA, V_NAME)
