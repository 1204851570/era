;----------------------------------------------------------------------------------------------------
;MUSICROOM本体
;----------------------------------------------------------------------------------------------------
@SHOP_MUSICROOM(GENRE_ID = 0, TITLE_ID = 0)
#DIM  GENRE_ID      ;作品ジャンルの切り替え用
#DIM  TITLE_ID      ;アルバム≒作品タイトル（＜東方紅魔郷＞など）の切り替え用
#DIM  M_ID          ;楽曲ループ処理用
#DIMS MUSIC_LIST, 30;曲名リスト
#DIMS WORK_TITLE    ;作品名
SIF WORK_TITLE == ""
	WORK_TITLE '= GET_WORKTITLE_BY_ID(GENRE_ID, TITLE_ID)
CALLF FUNC_SPLIT(GET_MUSIC_LIST_BY_WORKTITLE(WORK_TITLE), MUSIC_LIST)
PRINTFORML %ALBUM_DATA(WORK_TITLE, "表示名"), 40, LEFT%　取得方法：%ALBUM_DATA(WORK_TITLE, "方法表示")%
CALL SHOW_MUSIC_LIST(MUSIC_LIST)
DRAWLINE
PRINTFORM 　　　
PRINTBUTTON "[8]↑", 8
PRINTFORML

PRINTBUTTON "[4]←", 4
PRINTFORM 　　　
PRINTBUTTON "→[6]", 6
PRINTFORML

PRINTFORM 　　　
PRINTBUTTON "[2]↓", 2
PRINTFORML
PRINTBUTTON "[0]もどる", 0
DO
	ONEINPUT
	SELECTCASE RESULT
	CASE 0
		RETURN
	CASE 8
		GENRE_ID = T_PROJECT_GENRE_LIMIT(GENRE_ID - 1)
		TITLE_ID = T_PROJECT_TITLE_LIMIT(TITLE_ID, T_PROJECT_GENRE:GENRE_ID)
	CASE 4
		TITLE_ID = T_PROJECT_TITLE_LIMIT(TITLE_ID - 1, T_PROJECT_GENRE:GENRE_ID)
	CASE 6
		TITLE_ID = T_PROJECT_TITLE_LIMIT(TITLE_ID + 1, T_PROJECT_GENRE:GENRE_ID)
	CASE 2
		GENRE_ID = T_PROJECT_GENRE_LIMIT(GENRE_ID + 1)
		TITLE_ID = T_PROJECT_TITLE_LIMIT(TITLE_ID, T_PROJECT_GENRE:GENRE_ID)
	CASEELSE
		CONTINUE
	ENDSELECT
	BREAK
LOOP 1

WORK_TITLE '= GET_WORKTITLE_BY_ID(GENRE_ID, TITLE_ID)
RESTART



;----------------------------------------------------------------------------------------------------
;ジャンルID、タイトルIDを個別の作品タイトルに変換
;----------------------------------------------------------------------------------------------------
@GET_WORKTITLE_BY_ID(GENRE_ID, TITLE_ID)
#FUNCTIONS
#DIM  GENRE_ID
#DIM  TITLE_ID
#DIMS TEMP_STR
TEMP_STR = T_PROJECT_TITLE_%T_PROJECT_GENRE:GENRE_ID%:{TITLE_ID}
RETURNF STRFORM("%" + TEMP_STR + "%")

;----------------------------------------------------------------------------------------------------
;ページ循環処理用
;----------------------------------------------------------------------------------------------------
@T_PROJECT_GENRE_LIMIT(GENRE_ID)
#FUNCTION
#DIM GENRE_ID
#DIM GENRE_END
GENRE_END = VARSIZE("T_PROJECT_GENRE") - 1
SIF GENRE_ID < 0
	RETURNF GENRE_END
SIF GENRE_ID > GENRE_END
	RETURNF 0
RETURNF GENRE_ID

;----------------------------------------------------------------------------------------------------
;ページ循環処理用
;----------------------------------------------------------------------------------------------------
@T_PROJECT_TITLE_LIMIT(TITLE_ID, GENRE_TYPE)
#FUNCTION
#DIM  TITLE_ID
#DIMS GENRE_TYPE
#DIM  TITLE_END
#DIMS TEMP_STR
TEMP_STR  = VARSIZE("T_PROJECT_TITLE_%GENRE_TYPE%")
TITLE_END = TOINT(STRFORM("{" + TEMP_STR +"}")) - 1
SIF TITLE_ID < 0
	RETURNF TITLE_END
SIF TITLE_ID > TITLE_END
	RETURNF 0
RETURNF TITLE_ID


;----------------------------------------------------------------------------------------------------
;楽曲表示処理
;現状は無造作に表示するだけだが
;本実装の段階では
;・未取得なら楽曲名の代わりに取得条件を表示
;　・未取得かつ取得条件満たしてないなら取得条件表示をグレイアウト
;　・未取得で取得条件満たしてるなら取得条件表示を白色表示
;とかそんなUIにすると思う≒各曲で表示に要する行が1行分短くなる
;----------------------------------------------------------------------------------------------------
@SHOW_MUSIC_LIST(MUSIC_LIST)
#DIMS REF MUSIC_LIST  ;楽曲名リスト
#DIM      M_ID        ;ループ用
#DIMS     T_MUSIC_NAME;参照中の楽曲名
FOR M_ID, 0, FINDELEMENT(MUSIC_LIST, "", , , 1)
	T_MUSIC_NAME '= MUSIC_LIST:M_ID
	PRINTFORML 　{M_ID + 1, 2}：%T_MUSIC_NAME%
	PRINTFORML 　　　%MUSIC_DATA(T_MUSIC_NAME, "楽曲解説")%
	PRINTFORML 　　　%MUSIC_DATA(T_MUSIC_NAME, "取得条件")%
NEXT



