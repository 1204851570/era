;----------------------------------------------------------------------------------------------------
;楽曲取得関連処理
;----------------------------------------------------------------------------------------------------
;全キャラの取得可能楽曲をサーチ（好感度が高い順に判定）
;戻り値：取得可能楽曲名/空文字列
@GET_SCORE_SEARCH_BY_ALLCHARA
#FUNCTIONS
#DIM          L_ID                         ;渡すだけ→ループ用
#DIM  DYNAMIC CHARA_LIST, 人物数量上限 ;キャラリスト
#DIM  DYNAMIC CHARA_CNT                    ;キャラ数かうんと
#DIMS         GET_MUSIC                    ;取得可能楽曲名
CALLF MAKE_CHARA_LIST("主角外的全角色列表", L_ID, CHARA_LIST, CHARA_CNT)
CALLF SORT_LIST("好感降順", CHARA_LIST, CHARA_CNT)
FOR L_ID, 0, CHARA_CNT
	GET_MUSIC '= GET_SCORE_SEARCH_BY_CHARA(CHARA_LIST:L_ID)
	SIF GET_MUSIC == ""
		CONTINUE
	RETURNF GET_MUSIC
NEXT
RETURNF ""



;特定キャラ準拠で取得可能楽曲をサーチ
;戻り値：取得可能楽曲名/空文字列
@GET_SCORE_SEARCH_BY_CHARA(C_ID)
#FUNCTIONS
#DIM  C_ID       ;判定対象キャラ
#DIMS TEMP_M_STR ;キャラの所持楽曲（展開前）
#DIMS M_LIST, 30 ;キャラの所持楽曲（展開後）
TEMP_M_STR '= GET_STR(, "キャラデータ" , C_ID, "所持楽曲")
SIF !STRLENS(TEMP_M_STR)
	RETURNF ""
CALLF FUNC_SPLIT(TEMP_M_STR, M_LIST)
RETURNF GET_SCORE_SEARCH_BY_MUSIC_LIST(M_LIST)



;ゲームタイトル準拠で取得可能楽曲をサーチ
;戻り値：取得可能楽曲名/空文字列
;"TITLE_LIST_整数ナンバー"は広域CONSTでDIMったほうがいいかも
@GET_SCORE_SEARCH_BY_ALLTITLE
#FUNCTIONS
#DIM  TITLE_ID   ;ループ用
#DIMS M_LIST, 30 ;各ゲームタイトルの収録楽曲（展開後）
#DIMS GET_MUSIC  ;取得可能楽曲名
FOR TITLE_ID, 1, VARSIZE("TITLE_LIST_整数ナンバー")
	CALLF FUNC_SPLIT(GET_MUSIC_LIST_BY_WORKTITLE(TITLE_LIST_整数ナンバー:PAGE), M_LIST)
	GET_MUSIC '= GET_SCORE_SEARCH_BY_MUSIC_LIST(M_LIST)
	SIF GET_MUSIC == ""
		CONTINUE
	RETURNF GET_MUSIC
NEXT
RETURNF ""



;曲名リストから取得可能楽曲をサーチ
;戻り値：取得可能楽曲名/空文字列
@GET_SCORE_SEARCH_BY_MUSIC_LIST(M_LIST)
#FUNCTIONS
#DIM      M_ID   ;ループ用
#DIMS REF M_LIST ;楽曲名リスト配列
FOR M_ID, 0, FINDELEMENT(M_LIST, "", , , 1)
	SIF CAN_GET_MUSIC(M_LIST:M_ID)
		RETURNF M_LIST:M_ID
NEXT
RETURNF ""


;楽曲取得判定
@CAN_GET_MUSIC(M_NAME)
#FUNCTION
#DIMS M_NAME
SIF HAS_MUSIC(M_NAME)
	RETURNF 0
SIF MUSIC_DATA(M_NAME, "取得可否") == "取得可能"
	RETURNF 1
RETURNF 0


;SAVESTR:取得楽曲はまだできてねーですだ

;楽曲所持判定
@HAS_MUSIC(M_NAME)
#FUNCTION
SIF STRCOUNT(SAVESTR:取得楽曲, ESCAPE("/" + M_NAME + "/"))
	RETURNF 1
RETURNF 0



;楽曲追加処理
@ADD_MUSIC(M_NAME)
SIF !STRLENS(SAVESTR:取得楽曲)
	SAVESTR:取得楽曲 '= "/"
SIF HAS_MUSIC(M_NAME)
	RETURN
SAVESTR:取得楽曲 += M_NAME + "/"



