				ELSEIF PALAM:潤滑 >= PALAMLV:2
					IF PALAM:潤滑 >= PALAMLV:6
						PRINTFORM  秘裂から愛液\@ EX:処女喪失记号? の混じった破瓜血# \@が滴っている
					ELSEIF EX:処女喪失记号
						PRINT  秘裂から破瓜血が流れている
					ELSEIF PALAM:潤滑 >= PALAMLV:4
						PRINT  太ももを愛液が伝っている
					ELSEIF PALAM:潤滑 >= PALAMLV:2
						PRINT  秘裂が愛液で湿っている
					ENDIF
				ENDIF
			ELSEIF PALAM:潤滑 >= PALAMLV:2 && !(CFLAG:TARGET:膣内射精 == 1 || TEQUIP:Ｖ性交 == PLAYER || TEQUIP:振動棒)
				IF PALAM:潤滑 >= PALAMLV:4
					PRINTFORM  秘裂から愛液\@ EX:処女喪失记号? の混じった破瓜血# \@が滴っている
				ELSEIF EX:処女喪失记号
					PRINT  秘裂から破瓜血が流れている
				ELSEIF PALAM:潤滑 >= PALAMLV:3
					PRINTFORM  太ももを愛液が伝っている
				ELSEIF PALAM:潤滑 >= PALAMLV:2
					PRINTFORM  秘裂が愛液で湿っている
				ELSEIF TALENT:処女 == 1
					PRINTFORM 未使用のぴったりと閉じたすじが露わになっている
				ENDIF
			ENDIF
		ENDIF
		CALL LOOK_BELLY(TARGET)
	;下半身着を穿いている、かつ下着も付けている
	ELSE
		着衣状況 = 0
		;上下つなぎになっている服を着ている場合、それを表示する
		FOR 装備部位, 17, 8, -1
			IF GROUPMATCH(装備部位, 15, 14)
				CONTINUE
			ENDIF
			IF EQUIP:TARGET:装備部位
				IF CSTR:(50 + 装備部位) != "" && !CFLAG:睡衣
					表示内容 = %CSTR:(50 + 装備部位)%
				ELSE
					表示内容 = %CLOTHNAME(装備部位,EQUIP:装備部位)%
				ENDIF
				PRINTFORM %表示内容%
				着衣状況 ++
				BREAK
			ENDIF
		NEXT
		;なければ、下半身着衣を表示する
		IF 着衣状況 == 0
			FOR 装備部位, 6, 4, -1
				IF EQUIP:TARGET:装備部位
					IF CSTR:(50 + 装備部位) != "" && !CFLAG:睡衣
						表示内容 = %CSTR:(50 + 装備部位)%
					ELSE
						表示内容 = %CLOTHNAME(装備部位,EQUIP:装備部位)%
					ENDIF
					PRINTFORM %表示内容%
					BREAK
				ENDIF
			NEXT
		ENDIF
		IF !CFLAG:诶嘿嘿;  && PREVCOM != 330
			IF TEQUIP:下半身着衣状況 != 0
				IF PALAM:潤滑 >= PALAMLV:5
					PRINT  腿までビショビショに濡れている
				ELSEIF PALAM:潤滑 >= PALAMLV:3
					PRINT  恥ずかしい部分にぽつりと染みが出来ている
				ENDIF
			ELSEIF (EQUIP:下半身下着１ || EQUIP:下半身下着２) && CFLAG:TARGET:膣内射精 == 1 || CFLAG:TARGET:肛门射精 == 1 ||  PALAM:潤滑 >= PALAMLV:2
				IF EQUIP:下半身下着２
					IF CFLAG:TARGET:膣内射精 == 1 || CFLAG:TARGET:肛门射精 == 1 || PALAM:潤滑 >= PALAMLV:3
						PRINTFORM  %PANTSNAME(EQUIP:TARGET:下半身下着２)%に染みが出来ている
					ELSEIF PALAM:潤滑 >= PALAMLV:2
						PRINTFORM  %PANTSNAME(EQUIP:TARGET:下半身下着２)%が湿っている
					ENDIF
				ELSEIF EQUIP:下半身下着１== 1 || EQUIP:下半身下着１== 4
					IF CFLAG:TARGET:膣内射精 == 1 || CFLAG:TARGET:肛门射精 == 1 || PALAM:潤滑 >= PALAMLV:3
						PRINTFORM  %PANTSNAME(EQUIP:TARGET:下半身下着１)%に染みが出来ている
					ELSEIF PALAM:潤滑 >= PALAMLV:2
						PRINTFORM  %PANTSNAME(EQUIP:TARGET:下半身下着１)%が湿っている
					ENDIF
				ELSEIF EQUIP:下半身下着１== 2 || EQUIP:下半身下着１== 3
					IF CFLAG:TARGET:膣内射精 == 1 || CFLAG:TARGET:肛门射精 == 1 || PALAM:潤滑 >= PALAMLV:5
						PRINTFORM  %PANTSNAME(EQUIP:TARGET:下半身下着１)%が剥がれかかっている
					ELSEIF CFLAG:TARGET:膣内射精 == 1 || CFLAG:TARGET:肛门射精 == 1 || PALAM:潤滑 >= PALAMLV:3
						PRINTFORM  %PANTSNAME(EQUIP:TARGET:下半身下着１)%に染みが出来ている
					ELSEIF PALAM:潤滑 >= PALAMLV:2
						PRINTFORM  %PANTSNAME(EQUIP:TARGET:下半身下着１)%が湿っている
					ENDIF
				ELSEIF EQUIP:下半身下着１== 5 || EQUIP:下半身下着１== 6
					IF CFLAG:TARGET:膣内射精 == 1 || CFLAG:TARGET:肛门射精 == 1 || PALAM:潤滑 >= PALAMLV:3
						PRINTFORM  %PANTSNAME(EQUIP:TARGET:下半身下着１)%に雫が出来ている
					ELSEIF PALAM:潤滑 >= PALAMLV:2
						PRINTFORM  %PANTSNAME(EQUIP:TARGET:下半身下着１)%が湿っている
					ENDIF
				ENDIF
			ENDIF
			;小人
			IF TALENT:TARGET:100 == -5
				LOCAL = 200
			;幼児体型、小柄体型
			ELSEIF TALENT:TARGET:100 < 0
				LOCAL = 300
			;長身以上
			ELSEIF TALENT:TARGET:100 >= 1
