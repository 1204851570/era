@SET_FIRSTKISS(ARG, 接吻相手, シチュエーション)
#DIM 接吻相手
;シチュエーション	0:通常
;					1:デート帰り
;					2:オナバレ
#DIM シチュエーション
IF ARG == 接吻相手
	IF ARG == MASTER
		接吻相手 = TARGET
	ELSE
		接吻相手 = MASTER
	ENDIF
ENDIF
;キス未経験喪失
IF TALENT:ARG:無接吻経験
	SETCOLOR 247,171,166
	PRINTFORMW 無接吻経験喪失(%CALLNAME:ARG%)
	CFLAG:ARG:初吻喪失記念日 = DAY
	TALENT:ARG:無接吻経験 = 0
	CFLAG:ARG:初吻喪失对手 = 接吻相手
	SELECTCASE シチュエーション
		CASE 1
			CSTR:ARG:初吻位置 = 约会からの帰り道
			SIF ARG != MASTER
				CFLAG:ARG:初吻喪失対象 |= 喪失_自己
		CASE 2
			IF ARG != MASTER
				CSTR:ARG:初吻位置 = 自室
				CFLAG:ARG:初吻喪失対象 |= 喪失_なし崩し
			ELSE
				CSTR:ARG:初吻位置 = %CALLNAME:ARG%宅
				CFLAG:ARG:初吻喪失対象 |= 喪失_なし崩し
			ENDIF
		CASEELSE
			IF CHK_DATENOW(CFLAG:ARG:约会中) && CFLAG:ARG:現在位置 != OMANEKIBEYA()
				CSTR:ARG:初吻位置 = %DATENAME_SPOT()%
			ELSE
				CSTR:ARG:初吻位置 = %NAME_FROM_PLACE(CFLAG:ARG:現在位置)%
			ENDIF
			SIF ARG == MASTER
				CFLAG:ARG:初吻喪失対象 |= 喪失_自己
	ENDSELECT
	SIF TALENT:ARG:恋慕
		CFLAG:ARG:初吻喪失対象 |= 喪失_恋慕
	SIF TALENT:ARG:思慕
		CFLAG:ARG:初吻喪失対象 |= 喪失_思慕
	RESETCOLOR
ENDIF
