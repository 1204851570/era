;キス未経験喪失
IF TALENT:PLAYER:6
	SIF GROUPMATCH(SELECTCOM,20,312) || GROUPMATCH(TCVAR:TARGET:counter行動,60)
		CALL SET_FIRSTKISS(MASTER)
ENDIF
IF TALENT:TARGET:6 && !CFLAG:TARGET:睡眠 && !FLAG:70
	IF GROUPMATCH(SELECTCOM,20,312) || GROUPMATCH(TCVAR:TARGET:counter行動,60)
		IF TFLAG:193 != 99 && PREVCOM != 309
			EXP:TARGET:愛情経験 += 5
			CALL SET_FIRSTKISS(TARGET)
		ENDIF
	ENDIF
ENDIF
;アナル処女喪失
IF CFLAG:TARGET:尻穴処女喪失対象 & 喪失_時間停止 || !CFLAG:TARGET:尻穴処女喪失記念日
	SELECTCASE SELECTCOM
		CASE 45,62,63,66,69,70,71,323
			;完全に初めて
			IF !CFLAG:TARGET:尻穴処女喪失記念日
				PRINTFORMW 尻穴処女喪失(%CALLNAME:TARGET%)
				CFLAG:TARGET:尻穴処女喪失記念日 = DAY
				IF SELECTCOM == 45
					CFLAG:TARGET:尻穴処女喪失対象 |= 喪失_振動棒
				ELSEIF TALENT:PLAYER:2 & 2
					CFLAG:TARGET:尻穴処女喪失対象 |= 喪失_ペニス
				ELSE
					CFLAG:TARGET:尻穴処女喪失対象 |= 喪失_假阳具
				ENDIF
				IF !FLAG:70
					TCVAR:Ａ破瓜 = 1
				ELSE
					CFLAG:TARGET:尻穴処女喪失対象 |= 喪失_時間停止
					TCVAR:Ａ破瓜 = -2
				ENDIF
			;時間停止中に経験済み
			ELSEIF !FLAG:70 && CFLAG:TARGET:尻穴処女喪失対象 & 喪失_時間停止
				SIF FIRSTTIME("Ａ破瓜",0)
					PRINTFORMW 尻穴処女喪失？(%CALLNAME:TARGET%)
				TCVAR:Ａ破瓜 = -1
			ENDIF
			IF CHK_DATENOW(CFLAG:TARGET:约会中) && CFLAG:TARGET:現在位置 != OMANEKIBEYA()
				CSTR:TARGET:Ａ処女喪失位置 = %DATENAME_SPOT()%
			ELSE
				CSTR:TARGET:Ａ処女喪失位置 = %NAME_FROM_PLACE(CFLAG:TARGET:現在位置)%
			ENDIF
		CASEELSE
			IF TCVAR:TARGET:counter行動 == 87
				IF !CFLAG:TARGET:尻穴処女喪失記念日
					PRINTFORMW 尻穴処女喪失(%CALLNAME:TARGET%)
					CFLAG:TARGET:尻穴処女喪失記念日 = DAY
					IF TALENT:PLAYER:2 & 2
						CFLAG:TARGET:尻穴処女喪失対象 |= 喪失_ペニス
					ELSE
						CFLAG:TARGET:尻穴処女喪失対象 |= 喪失_假阳具
					ENDIF
				ELSEIF CFLAG:TARGET:尻穴処女喪失対象 & 喪失_時間停止
				ENDIF
			ENDIF
			IF CHK_DATENOW(CFLAG:TARGET:约会中) && CFLAG:TARGET:現在位置 != OMANEKIBEYA()
				CSTR:TARGET:Ａ処女喪失位置 = %DATENAME_SPOT()%
			ELSE
				CSTR:TARGET:Ａ処女喪失位置 = %NAME_FROM_PLACE(CFLAG:TARGET:現在位置)%
			ENDIF
	ENDSELECT
	IF TARGET == MASTER
		CFLAG:MASTER:尻穴処女喪失对手 = PLAYER
	ELSE
		CFLAG:TARGET:尻穴処女喪失对手 = PLAYER
	ENDIF
ENDIF
