;満月になると帰っちゃう（消えちゃう）
;外出中だと移動させても何故か戻っちゃうので凍結
;━━━━━━━━━━━━━━━━━━━━━━━━━━
;@KAGEROU_ESCAPE
;PRINTW 影狼は慌ててどこかへ去って行った…
;CFLAG:63:現在位置 = 99
;CFLAG:63:同室 = 0
;RETURN 1
;----------------------------------------------------
;━━━━━━━━━━━━━━━━━━━━━━━━━━
;==================================================
;部屋で遭遇
;==================================================
@M_KOJO_EVENT_K63_1(ARG,ARG:1)
;口上主はTARGET
;--------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;MASTERが入ってきた
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	;-------------------------------------------------
	;TARGETが入ってきた
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
;1,2はまとめる 
;挨拶は一回だけ
IF LOCAL && !FLAG:70 && LOCAL:1 && (ARG == 1 || ARG == 2) && !TCVAR:350
	;住み込み
	IF FLAG:15 == 63
		;満月の日
		SELECTCASE DAY:3
		CASE 15
			;満月の下
			IF MOON_WATCH(DAY:3) == 9
				;しかも中秋の名月
				SELECTCASE DAY:2
				CASE 3
					PRINTFORMW 「あら、%CALLNAME:MASTER%……」
					PRINTFORMW 「今夜はその……十五夜ね……」
					PRINTFORMW 「その、一緒にお赏月してくれる…？」
					PRINTFORML 「ほ、本当は恥ずかしいけど%CALLNAME:MASTER%になら」
					PRINTFORMW 「ありのままの姿、見て欲しいの……」
					PRINTFORMW 「なんだったらその……」
					PRINTFORMW 「いえ、まずはこのまま一緒にお赏月しましょ！」
					BASE:63:理性 = 0
					TCVAR:発情 = 1
				;普通の満月
				CASEELSE
					PRINTFORMW 「あら、%CALLNAME:MASTER%……」
					PRINTFORMW 「見て、今夜は満月よ」
					PRINTFORML 「恥ずかしいけど、%CALLNAME:MASTER%にだったら…」
					PRINTFORMW 「今の私、見て欲しいの……」
					PRINTFORMW 「ふふ、こんなこと思った事なかったらなんか不思議だわ」
					BASE:63:理性 = 0
					TCVAR:発情 = 1
				ENDSELECT
			;まだ昼で晴れてない
			ELSEIF TIME:2 >=1 && TIME:2 <= 4 && TIME:5 > 1
				PRINTFORMW 「あら、%CALLNAME:MASTER%、おはよう」
				PRINTFORMW 「その……今夜は満月ね……」
				PRINTFORMW 「まだお天気がどうなるか分からないけど」
				PRINTFORMW 「もし晴天たら私とお赏月してくれる？」
				$HALLELUJAH_INPUT_LOOP_4
				PRINTL [0] 晴天たらね
				PRINTL [1] 稍微忙しい
				INPUT
				SELECTCASE RESULT
				;《月霧/Moonmist》
				CASE 0
					PRINTFORMW 「本当！？」
					PRINTFORML 「じゃあ仕方ないわね……」
					PRINTFORMW 「とっておきの呪文（スペル）カードを使う時が来たわ！」
					PRINTL 
					PRINTFORMW 影狼は物陰に隠れると、なにやら見慣れないスペルカードを取り出し呪文を唱えた。
					PRINTFORML その途端、辺りが銀色に光る霧に包まれ、夜になったと思うと
					PRINTFORMW 光が渦巻き、はじけ、やがて霧が晴天ると影狼が姿を現した。
					PRINTFORMW 気づくと天蓋には燦々と輝く太陽が昇っている。
					PRINTL
					PRINTFORMW 「ふふふ…みんなにはナイショよ♪」
					PRINTFORMW 「さあ、十五夜を楽しみましょ」
					TIME:5 = 1
				CASE 1
					PRINTFORMW 「……そう…」
					PRINTFORMW 「まぁこんなお天気じゃね…無理言ってごめんなさい」
					PRINTFORMW 「あ、気にしてないから大丈夫よ」
				CASEELSE
					GOTO HALLELUJAH_INPUT_LOOP_4
				ENDSELECT
			;まだ昼
			ELSEIF TIME:2 >=1 && TIME:2 <= 4
				PRINTFORMW 「あら、%CALLNAME:MASTER%、おはよう」
				PRINTFORMW 「その……今夜は満月ね……」
				PRINTFORMW 「まだお天気がどうなるか分からないけど」
				PRINTFORMW 「とりあえず晴天ることを祈りましょう」
			;夜だけど晴れてない
			ELSEIF TIME:5 > 1
				;中秋の名月
				SELECTCASE DAY:2
				CASE 3
					PRINTFORMW 「あら、%CALLNAME:MASTER%…」
					PRINTFORMW 「せっかくの十五夜なのに、生憎のお天気ね……」
					PRINTFORMW 「ねえ%CALLNAME:MASTER%、もし晴天たら私とお赏月してくれる？」
					$HALLELUJAH_INPUT_LOOP_1
					PRINTL [0] 晴天たらね
					PRINTL [1] 稍微忙しい
					INPUT
					SELECTCASE RESULT
					;《月霧/Moonmist》
					CASE 0
						PRINTFORMW 「本当！？」
						PRINTFORML 「じゃあ仕方ないわね……」
						PRINTFORMW 「とっておきの呪文（スペル）カードを使う時が来たわ！」
						PRINTL 
						PRINTFORMW 影狼は物陰に隠れると、なにやら見慣れないスペルカードを取り出し呪文を唱えた。
						PRINTFORML その途端、辺りが銀色に光る霧に包まれたかと思うと
						PRINTFORMW 光が渦巻き、はじけ、やがて霧が晴天ると影狼が姿を現した。
						PRINTFORML 気づくと天蓋には燦々と輝く満月が昇り
						PRINTFORMW そこからは優しく淡い金色の光が射していた。
						PRINTL
						PRINTFORMW 「ふふふ…みんなにはナイショよ♪」
						PRINTFORMW 「さあ、十五夜を楽しみましょ」
						TIME:5 = 1
					CASE 1
						PRINTFORMW 「……そう…」
						PRINTFORMW 「まぁこんなお天気じゃね…無理言ってごめんなさい」
						PRINTFORMW 「あ、気にしてないから大丈夫よ」
					CASEELSE
						GOTO HALLELUJAH_INPUT_LOOP_1
					ENDSELECT
				;普通の満月
				CASEELSE
					PRINTFORMW 「あら、%CALLNAME:MASTER%、こんばんわ」
					PRINTFORMW 「せっかくの満月なのに、生憎のお天気ね……」
					PRINTFORML 「……まぁ一種の生理周期だから」
					PRINTFORMW 「変身はできるし、今でも中途半端に毛が……」
					PRINTFORMW 「って、どこ見てるのよ！」
				ENDSELECT
			;夜で晴れてるけど室内（秋）
			ELSEIF DAY:2 == 3
				PRINTFORMW 「あら、%CALLNAME:MASTER%……」
				PRINTFORMW 「今夜はその……十五夜ね……」
				PRINTFORMW 「その、一緒にお赏月してくれる…？」
				PRINTFORML 「ほ、本当は恥ずかしいけど%CALLNAME:MASTER%になら」
				PRINTFORMW 「ありのままの姿、見せても構わないわ……」
				PRINTFORMW 「なんだったらその……」
				PRINTFORMW 「いえ、まずは一緒にお赏月しましょ！」
			;夜で晴れてるけど室内
			ELSE
				PRINTFORMW 「あ…%CALLNAME:MASTER%……」
				PRINTFORMW 「ねえ、今夜は満月よ」
				PRINTFORML 「恥ずかしいけど、%CALLNAME:MASTER%にだったら…」
				PRINTFORMW 「今の私、見られてもいいかなって……」
				PRINTFORMW 「でもその……」
				PRINTFORMW 「いえ、なんでもないわ！」
			ENDIF
		;満月以外の日
		CASEELSE
			SELECTCASE TIME:2
			;朝は寝てるんで死に分岐ですが
			CASE 1, 2
				PRINTFORMW 「おはよ～…」
				PRINTFORMW 「朝から元気ねえ……」
				PRINTFORMW 「私朝って苦手……」
			;昼
			CASE 3,4
				PRINTFORMW 「あら%CALLNAME:MASTER%、おはよう！」
				PRINTFORMW 「お昼は一日のはじまりだからね」
			SIF TIME:0 < 901
				PRINTFORMW 「……ふぁ～…眠い……」
			;夜は分岐
			CASE 5 TO 7
				SELECTCASE TIME:0
				;19～22時
				CASE 1140 TO 1320
					SELECTCASE TIME:3
					;18時以降起き
					CASE 1080 TO 1320
						PRINTFORMW 「あら%CALLNAME:MASTER%、おはよう！」
						PRINTFORMW 「ふふ、你は夜型の人类だからね」
						PRINTFORMW 「まさか無理して私に合わせてない？」
						PRINTFORML 「う、嬉しいけど、健康にはよくないんだから」
						PRINTFORMW 「ほどほどにして身体を労ってあげなさいよ」
					;朝～昼起き
					CASEELSE
						PRINTFORMW 「あら%CALLNAME:MASTER%、こんばんわ！」
